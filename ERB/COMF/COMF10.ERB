	PRINTFORM %TEXTS("키스継続")%,
	CALL SETFLAG, "추가키스플래그", TARGET
ENDIF
IF SELECTCOM == PREVCOM && COND("前Ｔ조교자") == PLAYER
	PRINTFORM %CALLNAME:TARGET%의 페니스를 
