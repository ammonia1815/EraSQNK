@COM308
;Ａ부들부들삽입

PRINTL Ａ부들부들삽입

;텍스트の中でSOURCE:쾌Ａの変動アリ
CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = Ａ부들부들삽입

RETURN 1

@FLAG_COM308, ARG
;애널を노출させる
SIF CHECK_CLO("Ａ노출") == 0
	CALL SETFLAG, "바텀을노출：텍스트없음", TARGET
;대상のＡ⇔조교자の손？の汚れが移動
CALL EQ_STAIN, TARGET, PLAYER, 4, 1

@SOURCE_COM308, ARG
VARSET LOCAL

LOCAL:20 += 100
LOCAL:21 += 200

LOCAL:6 = 100
LOCAL:8 = 800
LOCAL:10 = 2000
LOCAL:12 = 2000
LOCAL:14 = 2000

;ABL:Ａ감각をみる
LOCAL:2 = CALCF("Ａ刺激", 2)
SELECTCASE ABL:Ａ감각
CASE 0
	LOCAL:3 = 10
	LOCAL:13 = 100
CASE 1
	LOCAL:3 = 30
	LOCAL:13 = 700
CASE 2
	LOCAL:3 = 100
	LOCAL:13 = 1500
CASE 3
	LOCAL:3 = 200
	LOCAL:13 = 3000
CASE 4
	LOCAL:3 = 450
	LOCAL:13 = 5000
CASEELSE
	LOCAL:3 = 750
	LOCAL:13 = 8000
ENDSELECT

;EXP:Ａ경험をみる
SELECTCASE EXP:Ａ경험
CASE IS >= 300
	TIMES LOCAL:2, 1.60
	TIMES LOCAL:6, 0.50
	TIMES LOCAL:9, 0.20
	TIMES LOCAL:14, 0.20
CASE IS >= 150
	TIMES LOCAL:2, 1.40
	TIMES LOCAL:6, 0.60
	TIMES LOCAL:9, 0.40
	TIMES LOCAL:14, 0.60
CASE IS >= 50
	TIMES LOCAL:2, 1.20
	TIMES LOCAL:6, 0.80
	TIMES LOCAL:9, 0.70
CASE IS >= 20
	TIMES LOCAL:14, 1.20
CASE IS >= 5
	TIMES LOCAL:2, 0.50
	TIMES LOCAL:6, 1.50
	TIMES LOCAL:9, 1.50
	TIMES LOCAL:14, 1.50
CASEELSE
	TIMES LOCAL:2, 0.10
	TIMES LOCAL:6, 2.00
	TIMES LOCAL:9, 2.00
	TIMES LOCAL:14, 2.00
ENDSELECT

;Ａ민감, 둔감をみる
;쾌Ａ自体の체크は後でまとめてやる
IF TALENT:Ａ둔감
	TIMES LOCAL:6, 0.50
	TIMES LOCAL:11, 0.50
	TIMES LOCAL:13, 0.50
	TIMES LOCAL:14, 0.50
ELSEIF TALENT:Ａ민감
	TIMES LOCAL:6, 1.50
	TIMES LOCAL:11, 1.50
	TIMES LOCAL:13, 1.50
	TIMES LOCAL:14, 1.50
ENDIF

;처녀で정조관념
SIF TALENT:처녀 == 1 && TALENT:정조관념
	LOCAL:14 /= 3

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM308
SOURCE:쾌Ａ = 0
;PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의 장내를 부들부들한 것으로 채웠다, 천천히 자극했다…
PRINTFORML %CALLNAME:PLAYER%의 Ａ부들부들 삽입이다! 
PRINTFORMW 「부륫부륫부륫～♪」
;試しに遊び요정素
IF RAND:10 == 0
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 진지하게 할 마음이 생겼다! 
	SOURCE:쾌Ａ += 2000

ELSEIF RAND:5 == 0
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 조금 할 마음을 보이고 있다…
	SOURCE:쾌Ａ += ABL:PLAYER:기교*200
ENDIF
