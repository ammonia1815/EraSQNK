	RETURN 0

STR:0 = 애널피스트

RETURN 1

@FLAG_COM142, ARG
CALL FLAG_INSERTION, "Ａ피스트", ARG, 100
;애널を노출させる
SIF CHECK_CLO("Ａ노출") == 0
	CALL SETFLAG, "바텀을노출：텍스트없음", TARGET

@SOURCE_COM142, ARG
CALL SOURCE_INSERTION, "Ａ피스트", ARG, 100
