	TIMES LOCAL:2 , 1.40
CASE IS < 50
	TIMES LOCAL:2 , 1.60
CASE IS < 150
	TIMES LOCAL:2 , 2.00
CASE IS < 300
	TIMES LOCAL:2 , 2.50
CASEELSE
	TIMES LOCAL:2 , 3.00
ENDSELECT

