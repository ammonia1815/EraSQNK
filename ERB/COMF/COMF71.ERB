CASE 1
	LOCAL:3 = 30
CASE 2
	LOCAL:3 = 100
CASE 3
	LOCAL:3 = 200
CASE 4
	LOCAL:3 = 450
CASEELSE
	LOCAL:3 = 750
ENDSELECT

;PALAM:욕정をみる
SELECTCASE PALAM:욕정
CASE IS < PALAMLV:1
	TIMES LOCAL:3 , 0.30
CASE IS < PALAMLV:2
	TIMES LOCAL:3 , 0.60
CASE IS < PALAMLV:3
	TIMES LOCAL:3 , 1.00
CASE IS < PALAMLV:4
	TIMES LOCAL:3 , 1.50
CASE IS >= PALAMLV:4
	TIMES LOCAL:3 , 1.80
ENDSELECT

