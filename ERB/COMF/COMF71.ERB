	LOCAL:3 = 30
ELSEIF ABL:Ａ감각 == 2
	LOCAL:3 = 100
ELSEIF ABL:Ａ감각 == 3
	LOCAL:3 = 200
ELSEIF ABL:Ａ감각 == 4
	LOCAL:3 = 450
ELSE
	LOCAL:3 = 750
ENDIF

;ABL:Ａ감각をみる
SELECTCASE ABL:Ａ감각
