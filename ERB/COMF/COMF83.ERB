@SOURCE_COM83, ARG
VARSET LOCAL

LOCAL:20 += 200
LOCAL:21 += 240

LOCAL:3 = 3000
LOCAL:8 = 5000 + COM_ORDER_STAIN("Ｐ", "요도")*200
LOCAL:11 = 2000 + SIZE("페니스", PLAYER)*1000
LOCAL:12 = 300
LOCAL:13 = 10000
LOCAL:14 = 20000 + SIZE("페니스", PLAYER)*5000

;どっちの요도か
IF PENIS(TARGET)
	LOCALS = %CSTR:4%
	LOCAL:6 = 9000
	LOCAL:9 = 9000
ELSE
	LOCALS = %CSTR:3%
	LOCAL:6 = 2000
	LOCAL:9 = 2000
ENDIF

;初めて
IF EXP:요도성교경험 == 0 && TALENT:도착적 == 0 && TALENT:마조 == 0
	LOCAL:9 += 5000
	LOCAL:14 += 20000
ENDIF

;ABL:Ｃ감각をみる
LOCAL:0 = CALCF("요도刺激", 2)
;조교자の허리놀림をみる
LOCAL:0 = SOURCE_EXABL(LOCAL:0, 33, PLAYER)
CALL SETFLAG, "요도성감플래그", TARGET

;요도の方が페니스の太さよりも太いなら아픔が軽減. 逆だと痛い. しかしブジーに比べれば부드러운 ので大分マシ
SELECTCASE (SIZE("페니스の直径", PLAYER) - MIN_STR(LOCALS) - GET_EXP_LV(EXP:요도성교경험) )/3
CASE IS <= -4
	TIMES LOCAL:6, 0.05
CASE -3
	TIMES LOCAL:6, 0.10
CASE -2
	TIMES LOCAL:6, 0.25
CASE -1
	TIMES LOCAL:6, 0.50
CASE 1
	TIMES LOCAL:0, 0.50
	TIMES LOCAL:6, 1.50
	TIMES LOCAL:9, 2.00
	TIMES LOCAL:20, 1.50
	TIMES LOCAL:21, 1.50
CASE 2
	LOCAL:6 += 3000
	LOCAL:9 += 3000
	TIMES LOCAL:0, 0.10
	TIMES LOCAL:6, 2.50
	TIMES LOCAL:9, 3.00
	TIMES LOCAL:20, 2.00
	TIMES LOCAL:21, 2.00
CASE IS >= 3
	LOCAL:6 += 10000
	LOCAL:9 += 6000
	TIMES LOCAL:0, 0.01
	TIMES LOCAL:6, 5.00
	TIMES LOCAL:9, 5.00
	TIMES LOCAL:20, 3.00
	TIMES LOCAL:21, 3.00
ENDSELECT

;요도확장경험が5, 20, 50, 150…と区切りを越えて大きいなら, 공포を軽減する
SELECTCASE GET_EXP_LV(EXP:요도확장경험)
CASE 1
	TIMES LOCAL:9, 0.50
CASE 2
	TIMES LOCAL:9, 0.30
CASE IS >= 3
	TIMES LOCAL:9, 0.15
ENDSELECT

SELECTCASE EXP:요도성교경험 + MIN(EXP:PLAYER:요도삽입경험, EXP:PLAYER:요도고문경험/10, 20)
CASE IS < 5
CASE IS < 20
	TIMES LOCAL:6, 0.80
	TIMES LOCAL:9, 0.80
CASE IS < 50
	TIMES LOCAL:0, 1.20
	TIMES LOCAL:6, 0.60
	TIMES LOCAL:9, 0.60
CASE IS < 150
	TIMES LOCAL:0, 1.35
	TIMES LOCAL:6, 0.40
	TIMES LOCAL:9, 0.40
CASEELSE
	TIMES LOCAL:0, 1.50
	TIMES LOCAL:6, 0.20
	TIMES LOCAL:9, 0.20
ENDSELECT

;PALAM:욕정をみる
SELECTCASE PALAM:욕정
CASE IS < PALAMLV:1
	TIMES LOCAL:14, 1.50
CASE IS < PALAMLV:2
	TIMES LOCAL:14, 1.00
CASE IS < PALAMLV:3
	TIMES LOCAL:14, 0.90
CASE IS < PALAMLV:4
	TIMES LOCAL:14, 0.70
CASE IS >= PALAMLV:4
	TIMES LOCAL:14, 0.50
ENDSELECT

;ABL:마조끼をみる
SELECTCASE ABL:마조끼
CASE 0
	TIMES LOCAL:11, 0.05
CASE 1
	TIMES LOCAL:11, 0.50
CASE 2

CASE 3
	TIMES LOCAL:11, 1.50
CASE 4
	TIMES LOCAL:11, 2.00
CASEELSE
	TIMES LOCAL:11, 3.00
ENDSELECT


;쾌Ｃについては既に参照済み
IF TALENT:요도광
	TIMES LOCAL:6, 0.10
	TIMES LOCAL:9, 0.10
	LOCAL:7 += 10000 + SIZE("페니스の直径", PLAYER)*200
	LOCAL:11 += LOCAL:14
	TIMES LOCAL:14, 0.05
ELSEIF TALENT:요도성감
	TIMES LOCAL:6, 0.20
	TIMES LOCAL:9, 0.20
	LOCAL:7 += 5000 + SIZE("페니스の直径", PLAYER)*100
	LOCAL:11 += LOCAL:14
	TIMES LOCAL:14, 0.20
ENDIF

;場所をみる
IF TEQUIP:관내노출플레이 || TEQUIP:야외플레이
	TIMES LOCAL:14, 2.50
	TIMES LOCAL:12, 1.50
ELSEIF TEQUIP:수치플레이
	TIMES LOCAL:14, 1.50
	TIMES LOCAL:12, 1.20
ELSEIF TEQUIP:목욕탕플레이
	TIMES LOCAL:14, 0.50
ENDIF

SELECTCASE EXP:방뇨경험 + EXP:실금경험
CASE IS < EXPLV:1
	TIMES LOCAL:14, 3.00
CASE IS < EXPLV:2
	TIMES LOCAL:14, 2.50
CASE IS < EXPLV:3
	TIMES LOCAL:14, 2.00
CASE IS < EXPLV:4
	TIMES LOCAL:14, 1.00
CASE IS < EXPLV:5
	TIMES LOCAL:14, 0.80
CASEELSE
	TIMES LOCAL:14, 0.60
ENDSELECT

;근육이완제
SIF GETBIT(TEQUIP:근육이완제, 1)
	TIMES LOCAL:6, 0.20

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG



