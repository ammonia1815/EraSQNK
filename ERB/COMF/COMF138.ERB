	LOCAL:14 += 5000
	IF CHECK("방광内방뇨")
		LOCAL:13 += 3000
	ELSE
		LOCAL:15 += 2000
	ENDIF
ENDIF

;ABL8,마조끼で일탈減少＆굴복ＵＰ
SELECTCASE ABL:마조끼
CASE 0
CASE 1
	TIMES LOCAL:13, 1.20
	TIMES LOCAL:14, 0.80
	TIMES LOCAL:15, 0.80
CASE 2
	TIMES LOCAL:13, 1.50
	TIMES LOCAL:14, 0.60
	TIMES LOCAL:15, 0.60
CASE 3
	TIMES LOCAL:13, 2.00
	TIMES LOCAL:14, 0.40
	TIMES LOCAL:15, 0.40
CASE 4
	TIMES LOCAL:13, 2.50
	TIMES LOCAL:14, 0.20
	TIMES LOCAL:15, 0.20
CASEELSE
	TIMES LOCAL:13, 3.00
	TIMES LOCAL:14, 0.05
	TIMES LOCAL:15, 0.05
ENDSELECT

