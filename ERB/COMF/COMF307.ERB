@COM307
;부들부들삽입
;처녀を奪わずにＶ경험を得られます

PRINTL 부들부들삽입

;텍스트の中でSOURCE:쾌Ｖの変動アリ
CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = 부들부들삽입

RETURN 1

@FLAG_COM307, ARG
;보지를노출させる
SIF CHECK_CLO("Ｖ노출") == 0
	CALL SETFLAG, "바텀을노출：텍스트없음", TARGET
;대상のＶ⇔조교자の손？の汚れが移動
CALL EQ_STAIN, TARGET, PLAYER, 3, 1

@SOURCE_COM307, ARG
VARSET LOCAL

LOCAL:20 += 40
LOCAL:21 += 150

;それぞれ, Ｖ윤, 노출, 일탈
LOCAL:10 = 1000
LOCAL:12 = 1000
LOCAL:14 = 1000

;촉수경험者なら, 일탈が少なめ
SIF EXP:촉수경험
	TIMES LOCAL:14 , 0.50

;ABL:Ｖ감각をみる
LOCAL:1 = CALCF("Ｖ刺激", 2)
SELECTCASE ABL:Ｖ감각
CASE 0
	LOCAL:3 = 50
CASE 1
	LOCAL:3 = 80
CASE 2
	LOCAL:3 = 150
CASE 3
	LOCAL:3 = 200
CASE 4
	LOCAL:3 = 300
CASEELSE
	LOCAL:3 = 500
ENDSELECT

;정조관념
IF TALENT:정조관념
	IF TALENT:처녀 == 1
		TIMES LOCAL:3 , 0.60
		LOCAL:15 = 5000
	ELSE
		TIMES LOCAL:3 , 0.60
		LOCAL:15 = 500
	ENDIF
ENDIF

;ABL:순종をみる
SELECTCASE ABL:순종
CASE 0
	TIMES LOCAL:3 , 0.60
	TIMES LOCAL:15 , 2.00
CASE 1
	TIMES LOCAL:3 , 0.80
	TIMES LOCAL:15 , 1.50
CASE 2
	TIMES LOCAL:3 , 1.00
	TIMES LOCAL:15 , 1.00
CASE 3
	TIMES LOCAL:3 , 1.20
	TIMES LOCAL:15 , 0.80
CASE 4
	TIMES LOCAL:3 , 1.40
	TIMES LOCAL:15 , 0.60
CASEELSE
	TIMES LOCAL:3 , 1.60
	TIMES LOCAL:15 , 0.30
ENDSELECT

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM307
SOURCE:쾌Ｖ = 0
;PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의 질을 부들부들한 것으로 채웠다, 천천히 자극했다…
PRINTFORML %CALLNAME:PLAYER%의 부들부들 삽입이다! 
PRINTFORMW 「부륫부륫부륫～♪」
;試しに遊び요정素
IF RAND:10 == 0
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 진지하게 할 마음이 생겼다! 
	SOURCE:쾌Ｖ += 2000

ELSEIF RAND:5 == 0
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 조금 할 마음을 보이고있다…
	SOURCE:쾌Ｖ += ABL:PLAYER:기교*200
ENDIF

