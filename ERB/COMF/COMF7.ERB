@COM7
;보지애무。指を入れないで、クリや陰唇、膣입までを弄る（インサート中でも実行가능）
;TEQUIP:보지애무で行為を記録する
;0bitアソコ表層 1bit恥丘 2bitクリを弄る 3bitピアスをいじめる
;基本的にはアソコor恥丘애무→음핵애무 となる

CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM7, ARG
IF PREVCOM == 5
	CALL FLAG_COM5, 30
ELSEIF PREVCOM == 6
	CALL FLAG_COM6, 30
ENDIF
IF CHECK_COM("쓱쓱닦기")
	CALL SETFLAG, "쓱쓱닦기플래그", TARGET
	STAIN:Ｖ = SET_STAIN(0, "Ｖ")
	STAIN:Ａ = SET_STAIN(0, "Ａ")
	STAIN:PLAYER:손 = 0
ELSEIF TEQUIP:촉수
	;대상のＶに촉수の汚れがつく
	CALL ADD_STAIN, TARGET, "Ｖ", "촉수"
ELSE
	;대상のＶ⇔조교자の손가락の汚れが移動
	CALL EQ_STAIN, TARGET, PLAYER, 3, 1
ENDIF

@SOURCE_COM7, ARG
IF PREVCOM == 5
	CALL SOURCE_COM5, 30
ELSEIF PREVCOM == 6
	CALL SOURCE_COM6, 30
ENDIF

VARSET LOCAL

LOCAL:20 += 20
LOCAL:21 += 50
LOCAL:12 = 300
LOCAL:14 = 100

;ABL:Ｃ감각をみる
LOCAL:0 = CALCF("Ｃ刺激", 1)
;조교자の손가락놀림をみる
LOCAL:0 = SOURCE_EXABL(LOCAL:0, 31, PLAYER)

LOCAL:3 = 200 + (ABL:Ｃ감각 + ABL:Ｖ감각)*100

;보지が노출していないと쾌Ｃ減
IF !CHECK_CLO("Ｖ노출") && COND("조교中")
	;바지をはいていると쾌Ｃ激減
	IF CHECK_OUTER("바지") || CHECK_PANTIES("가죽")
		TIMES LOCAL:0, 0.20
		TIMES LOCAL:12, 0.20
	ELSE
		TIMES LOCAL:0, 0.60
		TIMES LOCAL:12, 0.50
	ENDIF
ENDIF

;정조관념
IF TALENT:정조관념
	IF TALENT:처녀 == 1
		TIMES LOCAL:3, 0.60
		TIMES LOCAL:13, 2.00
		LOCAL:14 += 500
		LOCAL:15 += 300
	ENDIF
;정조관결여
ELSEIF TALENT:정조관결여
	IF TALENT:처녀 == 1
		TIMES LOCAL:3, 0.60
		LOCAL:14 += 100
		LOCAL:15 += 100
	ENDIF
ELSE
	IF TALENT:처녀 == 1
		LOCAL:14 += 500
		LOCAL:15 += 200
	ENDIF
ENDIF

IF CHECK_COM("쓱쓱닦기")
	CALL SOURCE_COM13, 30
	TIMES LOCAL:0, 0.10
ELSEIF CHECK_COM("음핵애무")
	TIMES LOCAL:0, 1.50
ENDIF

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


