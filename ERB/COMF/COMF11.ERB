@COM11
;파이즈리한다
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM11, ARG
;대상のＰ⇔조교자の가슴の汚れが移動
CALL EQ_STAIN, TARGET, PLAYER, 2, 5

@SOURCE_COM11, ARG
VARSET LOCAL

LOCAL:20 += 50
LOCAL:21 += 50

LOCAL:3 = 1000
LOCAL:5 = 1000
LOCAL:12 = 220
LOCAL:14 = 50

;남자か후타나리
IF PENIS(TARGET)
	;ABL:Ｃ감각をみる
	LOCAL:0 = CALCF("Ｃ刺激", 1)
	LOCAL:0 = SOURCE_EXABL(LOCAL:0, 34, PLAYER)
ENDIF

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM11
IF PREVCOM == SELECTCOM
	IF TEQUIP:넘어뜨리기
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 %조사처리(TEXTS("おっぱい", PLAYER),"로")% 마음껏 즐기고있다…
	ELSE
		PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%에게 계속 파이즈리 봉사를 하고있다…
	ENDIF
ELSE
	IF TEQUIP:넘어뜨리기
		PRINTFORM %조사처리(CALLNAME:TARGET,"는")% 말타기 자세로 
		IF RAND:2
			PRINTFORM 자신의 페니스
		ELSE
			PRINTFORM %TEXTS("페니스：視覚", TARGET)% 페니스
		ENDIF
		PRINTFORM 를 %CALLNAME:PLAYER%의 %TEXTS("おっぱい", PLAYER)%에 끼우고, 
		IF RAND:2
			PRINTFORMW 가슴 계곡을 범하기 시작했다…
		ELSE
			PRINTFORMW 허리를 움직여서 파이즈리를 즐기기 시작했다…
		ENDIF
	;それ以外
	ELSE
		PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% %조사처리(TEXTS("おっぱい", PLAYER),"로")% %CALLNAME:TARGET%의 %TEXTS("페니스：視覚", TARGET)% 페니스를 
		IF TALENT:PLAYER:거유 && RAND:2
			PRINT 감싸서 
		ELSEIF TALENT:PLAYER:빈유 == 0 && RAND:2
			PRINT 끼워서 
		ENDIF
		PRINTFORMW 문질러주었다…
	ENDIF
ENDIF
