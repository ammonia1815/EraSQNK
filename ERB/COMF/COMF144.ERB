		LOCAL:12 = 5000
		LOCAL:14 = 1500
		LOCAL:15 = 500
	ELSE
		LOCAL:20 += 10
		LOCAL:21 += 50
		LOCAL:5 = 3000
		LOCAL:8 = 1000
		LOCAL:12 = 3000
		LOCAL:14 = 1000
	ENDIF
	;노출벽をみる
	SELECTCASE ABL:노출벽
	CASE 0
		LOCAL:13 = 6000
	CASE 1
		LOCAL:13 = 5000
	CASE 2
		LOCAL:13 = 4000
	CASE 3
		LOCAL:13 = 3000
	CASE 4
		LOCAL:13 = 2000
	CASEELSE
		LOCAL:13 = 1000
	ENDSELECT

	;ABL:마조끼をみる
	SELECTCASE ABL:마조끼
