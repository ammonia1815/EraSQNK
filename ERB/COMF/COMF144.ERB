	CASE 1
		TIMES LOCAL:3, 1.00
		TIMES LOCAL:12, 1.00
		LOCAL:11 = 300
	CASE 2
		TIMES LOCAL:3, 1.30
		TIMES LOCAL:12, 1.20
		LOCAL:11 = 700
	CASE 3
		TIMES LOCAL:3, 1.40
		TIMES LOCAL:12, 1.40
		LOCAL:11 = 1200
	CASE 4
		TIMES LOCAL:3, 1.70
		TIMES LOCAL:12, 1.50
		LOCAL:11 = 1800
	CASEELSE
		TIMES LOCAL:3, 2.00
		TIMES LOCAL:12, 1.70
		LOCAL:11 = 2500
	ENDSELECT
	SIF TALENT:저항
		TIMES LOCAL:14, 3.00
	SIF TALENT:교만
		TIMES LOCAL:14, 2.00
ENDIF

