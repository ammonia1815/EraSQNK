@COM36
;착유기
IF TEQUIP:촉수
	PRINTL 촉수착유
ELSE
	PRINTL 착유기
ENDIF

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

IF TEQUIP:촉수
	STR:0 = 촉수착유
ELSE
	STR:0 = 착유기
ENDIF

RETURN 1

@FLAG_COM36, ARG

@SOURCE_COM36, ARG
VARSET LOCAL

LOCAL:20 += 50
LOCAL:21 += 100

LOCAL:4 = 100
LOCAL:5 = 100
LOCAL:6 = 100
LOCAL:7 = 100
LOCAL:12 = 100
LOCAL:13 = 100
LOCAL:16 = 100

;ABL:Ｂ감각をみる
LOCAL:17 = CALCF("Ｂ刺激", 60)

SIF TALENT:거유
	TIMES LOCAL:17 , 1.50
;빈유
SIF TALENT:빈유
	TIMES LOCAL:6 , 1.50

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM36
IF TEQUIP:착유기
	TEQUIP:착유기 = 0
	PRINTFORMW %CALLNAME:TARGET%에게서 착유기를 떼어냈다…
ELSE
	TEQUIP:착유기 = 1
	PRINTFORMW %CALLNAME:TARGET%에게 착유기를 달았다…
ENDIF

@TRAIN_MESSAGE_COM205
IF TEQUIP:착유기
	TEQUIP:착유기 = 0
	PRINTFORMW 촉수는 %CALLNAME:TARGET%의 유두를 풀어주었다…
ELSE
	TEQUIP:착유기 = 1
	IF RAND:2 == 0
		PRINTFORMW 흡반모양의 촉수가 %CALLNAME:TARGET%의 유두를 빨아들이기 시작했다…
	ELSE
		;빈유
		IF TALENT:빈유 || TALENT:남자
			PRINTFORMW 그릇모양의 촉수가 %CALLNAME:TARGET%의 가슴팍에 달라붙어, 리드미컬하게 빨아들이기 시작했다…
		ELSE
			PRINTFORMW 그릇 모양의 촉수가  %CALLNAME:TARGET%의 유방에 달라붙어, 리드미컬하게 빨아들이기 시작했다…
		ENDIF
	ENDIF
	CALL ADD_STAIN, TARGET, "Ｂ", "촉수"
ENDIF


@EQUIP_COM16
;착유기装備中
CALL SOURCE_COM36, 30
CALL FLAG_COM36, 30
RETURN 1

