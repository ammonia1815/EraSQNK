@COM2
;애널애무
IF TEQUIP:애널바이브 && TEQUIP:촉수 == 0
	LOCALS = Ａ바이브애무
ELSE
	LOCALS = 애널애무
ENDIF
PRINTFORML %LOCALS%

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %LOCALS%

RETURN 1

@FLAG_COM2, ARG
IF PREVCOM == 5
	CALL FLAG_COM5, 30
ELSEIF PREVCOM == 6
	CALL FLAG_COM6, 30
ENDIF

IF TEQUIP:애널바이브
	CALL FLAG_COM33, 130
ELSEIF CHECK("Ａ손가락넣기")
	CALL FLAG_INSERTION, "Ａ손가락넣기", ARG, 100
ENDIF

@SOURCE_COM2, ARG
IF PREVCOM == 5
	CALL SOURCE_COM5, 30
ELSEIF PREVCOM == 6
	CALL SOURCE_COM6, 30
ENDIF
IF TEQUIP:애널바이브
	CALL SOURCE_INSERTION, "Ａ바이브", ARG, 130
ELSEIF CHECK("Ａ손가락넣기")
	CALL SOURCE_INSERTION, "Ａ손가락넣기", ARG, 100
ELSE
	VARSET LOCAL

	LOCAL:20 += 20
	LOCAL:21 += 50

	LOCAL:3 = ABL:Ａ감각 * 100
	LOCAL:12 = 400
	LOCAL:14 = 1000

	;ABL:Ａ감각をみる
	LOCAL:2 = CALCF("Ａ刺激", 1)
	;조교자の손가락놀림をみる
	LOCAL:2 = SOURCE_EXABL(LOCAL:2, 31, PLAYER)

	;EXP:Ａ경험に応じたＡ快感, 고통, 공포, 일탈(반감)보정
	SELECTCASE EXP:Ａ경험
	CASE IS >= 300
		TIMES LOCAL:2, 1.60
		TIMES LOCAL:6, 0.50
		TIMES LOCAL:9, 0.20
		TIMES LOCAL:14, 0.20
	CASE IS >= 150
		TIMES LOCAL:2, 1.40
		TIMES LOCAL:6, 0.60
		TIMES LOCAL:9, 0.40
		TIMES LOCAL:14, 0.60
	CASE IS >= 50
		TIMES LOCAL:2, 1.20
		TIMES LOCAL:6, 0.80
		TIMES LOCAL:9, 0.70
	CASE IS >= 20
		TIMES LOCAL:14, 1.20
	CASE IS >= 5
		TIMES LOCAL:2, 0.50
		TIMES LOCAL:6, 1.50
		TIMES LOCAL:9, 1.50
		TIMES LOCAL:14, 1.50
	CASEELSE
		TIMES LOCAL:2, 0.10
		TIMES LOCAL:6, 2.00
		TIMES LOCAL:9, 2.00
		TIMES LOCAL:14, 2.00
	ENDSELECT

	;PALAM:욕정をみる
	SELECTCASE PALAM:욕정
	CASE IS < PALAMLV:1
		TIMES LOCAL:13, 0.30
	CASE IS < PALAMLV:2
		TIMES LOCAL:13, 0.50
	CASE IS < PALAMLV:3
		TIMES LOCAL:13, 0.80
	CASE IS < PALAMLV:4
	CASE IS >= PALAMLV:4
		TIMES LOCAL:13, 1.20
	ENDSELECT

	;Ａ민감, 둔감をみる
	;쾌Ａ自体の체크は後でまとめてやる
	IF TALENT:Ａ둔감
		TIMES LOCAL:6, 0.50
		TIMES LOCAL:11, 0.50
		TIMES LOCAL:13, 0.50
		TIMES LOCAL:14, 0.50
	ELSEIF TALENT:Ａ민감
		TIMES LOCAL:6, 1.50
		TIMES LOCAL:11, 1.50
		TIMES LOCAL:13, 1.50
		TIMES LOCAL:14, 1.50
	ENDIF

	;처녀で정조관념or엉덩이許可플래그
	IF (TALENT:처녀 == 1 && TALENT:정조관념) || CFLAG:100
		LOCAL:14 /= 3
		LOCAL:15 /= 3
	ENDIF

	CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG
ENDIF

@TRAIN_MESSAGE_COM2
;特に[연모], [Ａ윤Lv2], [A감각Lv3], [라미아], [直前の조교], [애널바이브orパールor플러그orバルーンor電極], [縄]等で텍스트が変わります. 
;애널촉수中なら
IF TEQUIP:촉수
	IF RAND:3 == 0
		PRINTFORMW 점액투성이인 촉수로 %CALLNAME:TARGET%의 애널을 휘저었다…
	ELSEIF RAND:2 == 0
		PRINTFORMW 브러쉬 형태의 촉수가 %CALLNAME:TARGET%의 장벽을 문질렀다…
	ELSE
		PRINTFORMW 돌기 모양의 촉수가 장벽을 자극하면서 안 깊숙한 곳까지 침입해왔다…
	ENDIF
ELSEIF PREVCOM == SELECTCOM && COND("前Ｔ조교자") == PLAYER
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의 애널을 정성스럽게 풀어주었다…
;인서트中
ELSEIF INSERT("Ｖ인서트") || INSERT("요도인서트")
	IF INSERT("기승위")
		PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%에게 허리를 흔들게 하면서, 애널에 장난을 쳤다…
	ELSE
		PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 허리를 찔러넣으면서, %CALLNAME:TARGET%의 애널에 장난을 쳤다…
	ENDIF
;Ａ인서트中
ELSEIF INSERT("Ａ인서트")
	PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% , 
	IF TALENT:PLAYER:후타나리 || TALENT:PLAYER:남자
		PRINT 육봉
	ELSE
		PRINT 딜도
	ENDIF
	PRINTFORMW 의 움직임에 맞춰 이리저리 움직이는 %CALLNAME:TARGET%의 애널에 장난을 쳤다…
;それ以外
ELSE
	SIF PLAYER != MASTER
		PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 
	IF TEQUIP:키스
		PRINTFORM %CALLNAME:TARGET%%TEXTS("키스継続助詞付き")%,
		CALL SETFLAG, "추가키스플래그", TARGET
	ELSEIF PREVCOM == 5
		PRINTFORM %CALLNAME:TARGET%의 가슴을 애무하면서, 
	ELSE
		;식스나인
		IF TEQUIP:식스나인
			PRINTFORM %TEXTS("６９")%
		;플레이を受け入れ가능かつ호색
		ELSEIF CHECK("Ａ손가락넣기") && TALENT:호색 && RAND:2 == 0
			PRINT 요염하게 미소짓고있는 
		;縄
		ELSEIF TEQUIP:구속
			PRINT 꼼짝도 못하고있는 
		ELSEIF CHECK("Ａ손가락넣기") && COND("다리 사용")
			;부끄럼쟁이で쾌Ａ or욕정が低いなら追加したり
			SIF TALENT:부끄럼쟁이 && (PALAM:쾌Ａ <= PALAMLV:3 || PALAM:욕정 <= PALAMLV:4)
				PRINT 부끄러워하면서도 
			PRINT 솔선해서 다리를 벌리고 있는 
		;Ａ감각1이하or반항적or부끄럼쟁이
		ELSEIF (ABL:Ａ감각 <= 1 || TALENT:반항적 || TALENT:부끄럼쟁이) && RAND:3 >= 1
			;부끄럼쟁이で쾌Ａ or욕정が低いなら追加したり
			SIF TALENT:부끄럼쟁이 && (PALAM:쾌Ａ <= PALAMLV:3 || PALAM:욕정 <= PALAMLV:4)
				PRINT 부끄러움에 떨리는 
		ENDIF
		PRINTFORM %CALLNAME:TARGET%의 
		;라미아に엉덩이は無い方바늘で
		IF COND("다리が無い")
			SIF CHECK_CLO("Ａ노출") == 0
				CALL SETFLAG, "바텀을노출：텍스트없음", TARGET
		;애널が노출していないで, 팬티をずらしていないなら自動的にずらす
		ELSE
			SIF !CHECK_CLO("Ａ노출") && CHECK_CLO("Ａ노출可")
				CALL SETFLAG, "바텀을노출：텍스트有り", TARGET

			;작은몸집か요정なら
			IF TALENT:작은몸집 || TALENT:요정
				PRINT 작은 
			;Ａ민감なら
			ELSEIF TALENT:Ａ민감 && RAND:3 >= 1
				PRINT 부드러운 
			;음란なら
			ELSEIF TALENT:음란 && RAND:3 >= 1
				PRINT 탄력있는 
			;それ以外
			ELSEIF RAND:2 == 0
				PRINT 모양 좋은 
			ENDIF
			;乱数分岐
			IF RAND:2 == 0
				PRINT 엉덩이를 
			ELSE
				PRINT 볼기를 
			ENDIF
			;Ａ감각1이하or반항적or부끄럼쟁이
			SIF (ABL:Ａ감각 <= 1 || TALENT:반항적 || TALENT:부끄럼쟁이)&& TALENT:연모 == 0 && RAND:3 >= 1
				PRINT 억지로 
			;연모orＡ손가락넣기を楽しめる状態かつ순종3이상で縛ってないと몽마に엉덩이를広げさせます
			IF CHECK("Ａ손가락넣기") + TALENT:연모 && ABL:순종 >= 3 && TEQUIP:구속 == 0 && RAND:2 == 0
				PRINT 벌리고, 
			ELSE
				PRINT 열고, 
			ENDIF
		ENDIF
	ENDIF
	;애널바이브orパールor애널플러그orバルーンか, 直前に애널系조교かで分岐. 
	IF COND("애널栓")
		PRINT 애널
		;乱数分岐
		IF COND("애널플러그")
			PRINT 을 막고잇는 
		ELSEIF TEQUIP:확장벌룬 == 2
			PRINT 을 단단히 넓히고 있는 
		ELSEIF RAND:3 == 0
			PRINT 을 단단히 넓히고 있는 
		ELSEIF RAND:2 == 0
			PRINT 에서 튀어나온 
		ELSE
			PRINT 로 꽉 물고있는 
		ENDIF
		;Ａ윤3이상で追加
		SIF PALAM:Ａ윤 >= PALAMLV:3 && (TEQUIP:애널바이브 || TEQUIP:확장벌룬 == 2)
			PRINT 점액 투성이가 된 
		IF COND("애널비즈")
			PRINT 애널비즈를 
		ELSEIF TEQUIP:애널바이브
			PRINT 애널바이브를 
		ELSEIF COND("애널플러그")
			PRINT 애널플러그를 
		ELSEIF TEQUIP:확장벌룬 == 2
			PRINT 애널벌룬을 
		ENDIF
		;乱数分岐
		IF COND("애널플러그") || TEQUIP:확장벌룬 == 2
			PRINT 빙글빙글 
		ELSEIF RAND:3 == 0
			PRINT 찌걱찌걱 
		ELSEIF RAND:2 == 0
			PRINT 부드럽게 
		ELSE
			PRINT 천천히 
		ENDIF
		IF COND("애널비즈") ||COND("애널플러그") || TEQUIP:확장벌룬 == 2
			PRINTFORMW 가지고놀았다…
		ELSE
			PRINTFORMW 넣다빼었다…
		ENDIF
	ELSE
		PRINTFORM %TEXTS("애널")% 애널
		;애널系조교やＡ민감やＡ경험150이상にあわせて乱数分岐. 하피のときは舐めます. 
		IF (PREVCOM == 33 || (PREVCOM >= 60 && PREVCOM <= 69) || (PREVCOM >= 136 && PREVCOM <= 138) || PREVCOM == 142 || PREVCOM == 143 || PREVCOM == 202 || EXP:Ａ경험 >= 150) && TALENT:요정 == 0 && RAND:3
			PRINT 에 손가락을 몇개씩 넣고 
		ELSEIF (PREVCOM == 33 || (PREVCOM >= 60 && PREVCOM <= 69) || (PREVCOM >= 136 && PREVCOM <= 138) || PREVCOM == 142 || PREVCOM == 143 || PREVCOM == 202 || EXP:Ａ경험 >= 150 || TALENT:Ａ민감) && TALENT:요정 == 0 && RAND:2
			PRINT 에 손가락을 뿌리끝까지 넣고 
		ELSEIF TALENT:요정 && EXP:Ａ경험 < 150
			PRINT 을 손가락끝으로 
		ELSEIF CHECK("Ａ손가락넣기")
			PRINTFORM 에 \@ TALENT:요정 ? %CALLNAME:TARGET%에게는 팔정도 두께인 # \@ 손가락을 넣어 
		ELSE
			PRINTFORM 에 손가락을 문질러
		ENDIF
		IF TALENT:요정 && EXP:Ａ경험 < 150
			PRINT 빙글빙글 
		ELSEIF CUP:굴복 < 500 && (PREVCOM == 33 || (PREVCOM >= 60 && PREVCOM <= 69) || (PREVCOM >= 136 && PREVCOM <= 138) || PREVCOM == 142 || PREVCOM == 143 || PREVCOM == 202 || EXP:Ａ경험 >= 150) && RAND:3
			PRINT 찌걱찌걱 
		ELSEIF RAND:3 == 0 && CHECK("Ａ손가락넣기")
			PRINT 격렬하게 
		ELSEIF RAND:2 == 0
			PRINT 부드럽게 
		ELSE
			PRINT 빙글빙글 구멍의 가장자리를 따라서 
		ENDIF
		;乱数分岐
		IF (TALENT:요정 && EXP:Ａ경험 < 150) || RAND:3 == 0
			PRINTFORMW 가지고 놀았다…
		ELSEIF RAND:2 && CHECK("Ａ손가락넣기")
			PRINTFORMW 넣다빼었다…
		ELSE
			PRINTFORMW 풀어주었다…
		ENDIF
	ENDIF
ENDIF

