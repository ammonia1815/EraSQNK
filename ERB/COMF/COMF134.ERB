@FLAG_COM134, ARG

@SOURCE_COM134, ARG
VARSET LOCAL

;体勢などで소비変動
IF TEQUIP:구속
	LOCAL:20 += 30
	LOCAL:21 += 20
	;20bit 손が使えない, 21bit 발が使えない, 22bit 体勢が変えられない, 23bit 무리な姿勢, 24bit 가슴, 25bit 가랑이
	IF GETBIT(TEQUIP:구속, 25)
		LOCAL:20 += 10
		LOCAL:21 += 10
	ENDIF
	IF GETBIT(TEQUIP:구속, 24)
		LOCAL:20 += 5
		LOCAL:21 += 5
	ENDIF
	IF GETBIT(TEQUIP:구속, 23)
		LOCAL:20 += 80
		LOCAL:21 += 60
	ENDIF
	IF GETBIT(TEQUIP:구속, 22)
		LOCAL:20 += 60
		LOCAL:21 += 40
	ENDIF
	IF GETBIT(TEQUIP:구속, 21)
		LOCAL:20 += 40
		LOCAL:21 += 20
	ENDIF
	IF GETBIT(TEQUIP:구속, 20)
		LOCAL:20 += 20
		LOCAL:21 += 10
	ENDIF
ELSE
	LOCAL:20 = 20
	LOCAL:21 = 20
ENDIF

;긴박경험が高いと소비減少
IF EXP:긴박경험 >= 300
	TIMES LOCAL:20 , 0.30
	TIMES LOCAL:21 , 0.30
ELSEIF EXP:긴박경험 >= 50
	TIMES LOCAL:20 , 0.70
	TIMES LOCAL:21 , 0.70
ENDIF

SIF GETBIT(TEQUIP:구속, 24)
	LOCAL:17 = CALCF("Ｂ刺激", 0, 10)
SIF GETBIT(TEQUIP:구속, 25)
	LOCAL:0 = CALCF("Ｃ刺激", 0, 10)

LOCAL:6 = 100
LOCAL:9 = 100
LOCAL:12 = 500
LOCAL:13 = 1000
LOCAL:14 = 500

;0bit 고통を伴う, 1bit 強い고통を伴う, 2bit 非常に強い고통を伴う 
IF GETBIT(TEQUIP:구속, 0)
