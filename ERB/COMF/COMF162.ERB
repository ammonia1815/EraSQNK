CALL SAMEN_CHECK

RETURN 1

@FLAG_COM162, ARG
CALL EQ_STAIN, ASSI, MASTER, 0, 2
;汚れをなめ取る
CALL RESET_STAIN_EX, "Ｐ", PLAYER

@SOURCE_COM162, ARG
VARSET LOCAL

LOCAL:20 += 0
LOCAL:21 += 30

;とにかく욕정추가
LOCAL:11 = 500

;ABL:욕정をみる
SELECTCASE ABL:욕망
CASE 0
	TIMES LOCAL:11 , 0.25
CASE 1
	TIMES LOCAL:11 , 0.50
CASE 2
	TIMES LOCAL:11 , 0.75
	LOCAL:21 += 10
CASE 3
	TIMES LOCAL:11 , 1.00
	LOCAL:21 += 20
CASE 4
	TIMES LOCAL:11 , 1.25
	LOCAL:21 += 30
CASEELSE
	TIMES LOCAL:11 , 1.50
	LOCAL:21 += 40
ENDSELECT

;ABL:정액중독を見る
SELECTCASE ABL:정액중독
CASE 0
	TIMES LOCAL:11 , 0.10
CASE 1
	TIMES LOCAL:11 , 0.40
CASE 2
	TIMES LOCAL:11 , 0.70
CASE 3
	TIMES LOCAL:11 , 1.00
	LOCAL:21 += 10
CASE 4
	TIMES LOCAL:11 , 1.30
	LOCAL:21 += 20
CASEELSE
	TIMES LOCAL:11 , 1.60
	LOCAL:21 += 30
ENDSELECT

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM162
;인서트中断
IF INSERT("Ｖ인서트") || (PREVCOM >= 60 && PREVCOM <= 69)
	PRINTFORMW %CALLNAME:TARGET%의 질에서 페니스를 뽑자마자, %CALLNAME:ASSI%에게 물게 했다. 
	PRINTFORMW %조사처리(CALLNAME:ASSI,"는")% 즐거운듯이 %CALLNAME:TARGET%의 더러움을 핥아서 닦고 있다…
;Ａ인서트中断
ELSEIF INSERT("Ａ인서트") || (PREVCOM >= 70 && PREVCOM <= 79)
	PRINTFORMW %CALLNAME:TARGET%의 애널에서 페니스를 뽑자마자, %CALLNAME:ASSI%에게 물게 했다. 
	PRINTFORMW %조사처리(CALLNAME:ASSI,"는")% 묵묵히 페니스의 더러움을 핥아서 씻어내고 있다…
;インサート中断
ELSEIF INSERT("요도인서트") || PREVCOM == 83
	PRINTFORMW %CALLNAME:TARGET%의 것인지등페니스를 뽑자 마자,%CALLNAME:ASSI%에 물게 했다.
	PRINTFORMW %조사처리(CALLNAME:ASSI,"는")% 묵묵히 페니스의 더러움을 핥아서 씻어내고 있다…
ELSE
	PRINTFORMW %CALLNAME:TARGET%에게 과시하듯이, %CALLNAME:ASSI%에게 펠라치오를 시켰다. 
	PRINTFORMW %조사처리(CALLNAME:ASSI,"는")% 정성스럽게 빨면서, 페니스를 깨끗하게 했다…
ENDIF
