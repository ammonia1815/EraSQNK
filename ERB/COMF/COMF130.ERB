@COM130
;스팽킹
;実際には고통があるためもっと체력기력をもっていかれる

PRINTL 스팽킹

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = 스팽킹

RETURN 1

@FLAG_COM130, ARG
IF INSERT("인서트") && PENIS(PLAYER)
	CALL SETFLAG, "스팽킹섹스플래그", TARGET
	CALL SETFLAG, "스팽킹섹스플래그", PLAYER
ENDIF

@SOURCE_COM130, ARG
VARSET LOCAL

LOCAL:20 += 80
LOCAL:21 += 40

;노출
LOCAL:12 = 200
;일탈
LOCAL:14 = 500

;PALAM:고통をみる
SELECTCASE PALAM:고통
CASE IS < PALAMLV:1
	LOCAL:6 = 300
CASE IS < PALAMLV:2
	LOCAL:6 = 500
CASE IS < PALAMLV:3
	LOCAL:6 = 800
CASE IS < PALAMLV:4
	LOCAL:6 = 1200
CASE IS >= PALAMLV:4
	LOCAL:6 = 1800
ENDSELECT

CALL SOURCE_SMPLAY, "스팽킹", ARG

LOCAL:13 += MULTIPLY(LOCAL:6 / 2, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)

SIF AGE(TARGET) == 1
	TIMES LOCAL:13, 1.50
;삽입しながら
IF INSERT("인서트")
	TIMES LOCAL:6, 0.50
	TIMES LOCAL:12, 1.50
	TIMES LOCAL:13, 1.50
ENDIF

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@SOURCE_SMPLAY, ARGS, ARG
SIF ABL:마조끼 == 0
	RETURN 0
VARSET LOCAL

SELECTCASE ARGS
CASE "아이마스크"
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*100, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*100, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASE "스팽킹"
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*200, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*200, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASE "구속", "재갈", "방뇨", "제모", "정음"
	SIF ABL:마조끼 <= 1
		RETURN 0
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*200, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*200, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASE "이라마치오"
	SIF ABL:마조끼 <= 1
		RETURN 0
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*300, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*300, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;딥스로트中？
	IF TEQUIP:딥스로트
		TIMES LOCAL:7, 2.00
		TIMES LOCAL:11, 2.00
	ENDIF
CASE "채찍"
	SIF ABL:마조끼 <= 1
		RETURN 0
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*300, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*300, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASE "관장", "확장벌룬", "성수플레이시킨다", "피스트", "요도카테터"
	SIF ABL:마조끼 <= 2
		RETURN 0
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*400, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*400, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASE "Ｗ피스트"
	SIF ABL:마조끼 <= 3
		RETURN 0
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*400, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*400, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASE "바늘", "피어싱"
	SIF ABL:마조끼 <= 4
		RETURN 0
	;중독충족
	LOCAL:7 = MULTIPLY(ABL:마조끼*400, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
	;욕정추가
	LOCAL:11 = MULTIPLY(ABL:마조끼*400, 100 + (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*20)
CASEELSE
	THROW %GAMEBASE_YEAR% Ver.{VER_ERASQN()} BUG : "%ARGS%" 를 찾을 수 없습니다.
ENDSELECT

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM130
;前回と同じ
IF PREVCOM == SELECTCOM && COND("前Ｔ조교자") == PLAYER
	PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 
	IF INSERT("인서트")
		IF RAND:3
			PRINTFORM %TEXTS("합체中", PLAYER)%
		ELSE
			PRINTFORM 격렬하게 허리를 흔들면서, 
		ENDIF
	ELSEIF TEQUIP:키스
		PRINTFORM %TEXTS("키스継続")%,
		CALL SETFLAG, "추가키스플래그", TARGET
	ENDIF
	PRINTFORM %CALLNAME:TARGET%의 엉덩이를 
	IF ABL:PLAYER:새드끼 >= 3 && RAND:2
		PRINTFORM 손바닥으로 
	ELSEIF ABL:PLAYER:새드끼 && (ABL:PLAYER:새드끼 >= 3 || RAND:2)
		PRINTFORM 찰싹찰싹 
	ELSE
		PRINTFORM 짝짝 
	ENDIF
	PRINTFORMW 두들겼다…
ELSE
	PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 
	IF INSERT("인서트")
		IF RAND:3
			PRINTFORM %TEXTS("합체中", PLAYER)%
		ELSE
			PRINTFORM 꾹꾹 허리를 찔러넣으면서, 
		ENDIF
	ELSEIF TEQUIP:키스
		PRINTFORM %TEXTS("키스継続")%,
		CALL SETFLAG, "추가키스플래그", TARGET
	ENDIF
	PRINTFORM %CALLNAME:TARGET%의 %조사처리(TEXTS("엉덩이：감촉"),"를")% 손바닥으로 
	IF ABL:PLAYER:새드끼 >= 3 && RAND:2
		PRINTFORM 강하게 
	ELSEIF ABL:PLAYER:새드끼 && (ABL:PLAYER:새드끼 >= 3 || RAND:2)
		PRINTFORM 찰싹찰싹 
	ELSE
		PRINTFORM 탁탁 
	ENDIF
	PRINTFORMW 때렸다…

	IF INSERT("인서트") && PENIS(PLAYER)
		IF INSERT("Ｖ인서트")
			LOCALS = 질
		ELSEIF INSERT("Ａ인서트")
			LOCALS = 애널
		ELSEIF INSERT("요도인서트")
			LOCALS = 
		ENDIF
		IF ABL:마조끼 >= 3
			PRINTFORMW %타겟은()% 얻어맞을 때 기쁜듯이 %LOCALS%의 조임을 강하게 해%조사처리(CALLNAME:PLAYER,"를")% 즐겁게 하고 있다…
		ELSE
			PRINTFORMW %타겟은()% 고통이 덮칠 때마다 몸을 경직시켜,%조사처리(LOCALS,"가")% %CALLNAME:PLAYER%의 페니스를 강하게 단단히 조이고 있다…
		ENDIF
	ELSEIF CHOSECOM(SELECTCOM) == 0
		IF ABL:PLAYER:새드끼 >= 3
			PRINTFORM 몇번이나 두들겨맞는 사이에 %CALLNAME:TARGET%의 엉덩이는 빨갛게 부풀고, 
			IF ABL:마조끼 >= 3 && TALENT:남자 == 0
				PRINTFORM 고통때문에 발정해버렸는지 
				IF CUP:Ｖ윤 >= 500
					PRINTFORMW 보지는 애액으로 질척질척해져버렸다…
				ELSE
					PRINTFORMW 가랑이는 애액으로 젖기시작했다…
				ENDIF
			ELSE
				PRINTFORMW 상당히 애처로운 모습이다…
			ENDIF
		ELSEIF ABL:마조끼 >= 3
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 가해지는 자극이 부족한 듯한 모습을 보이고 있지만, 엉덩이를 두들겨 맞는 것에 명백하게 흥분하고있는 것같다…
		ENDIF
	ENDIF
ENDIF
