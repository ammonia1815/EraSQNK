@FLAG_COM100, ARG
;대상のＰ⇔조교자のＰの汚れが移動
SIF PENIS(TARGET)
	CALL EQ_STAIN, TARGET, PLAYER, 2, 2

@SOURCE_COM100, ARG
VARSET LOCAL

LOCAL:20 += 40
LOCAL:21 += 130

LOCAL:8 = COM_ORDER_STAIN("Ｐ", "Ｐ")*10 + 60
LOCAL:11 = CALCF("실행치による욕정추가")
LOCAL:13 = 1200
LOCAL:14 = 400

;ABL:봉사정신をみる
SELECTCASE ABL:봉사정신
CASE 0
	LOCAL:4 = 200
	LOCAL:5 = 100
CASE 1
	LOCAL:4 = 250
	LOCAL:5 = 180
CASE 2
	LOCAL:4 = 300
	LOCAL:5 = 250
CASE 3
	LOCAL:4 = 350
	LOCAL:5 = 350
CASE 4
	LOCAL:4 = 400
	LOCAL:5 = 500
	TIMES LOCAL:8 , 0.50
CASEELSE
	LOCAL:4 = 450
	LOCAL:5 = 800
	TIMES LOCAL:8 , 0.10
ENDSELECT

;ABL:기교をみる
SELECTCASE ABL:기교
CASE 0
	TIMES LOCAL:4 , 0.50
	TIMES LOCAL:5 , 0.50
CASE 1
	TIMES LOCAL:4 , 0.80
	TIMES LOCAL:5 , 0.80
CASE 2
CASE 3
	TIMES LOCAL:4 , 1.20
	TIMES LOCAL:5 , 1.20
CASE 4
	TIMES LOCAL:4 , 1.50
	TIMES LOCAL:5 , 1.50
CASEELSE
	TIMES LOCAL:4 , 2.00
	TIMES LOCAL:5 , 2.00
ENDSELECT

;ABL:Ｃ감각をみる
LOCAL:0 = CALCF("Ｃ刺激", 2, 70)

;PALAM:Ｖ윤
SELECTCASE PALAM:Ｖ윤
CASE IS < PALAMLV:1
	TIMES LOCAL:0 , 0.30
	TIMES LOCAL:4 , 0.60
CASE IS < PALAMLV:2
	TIMES LOCAL:0 , 0.60
	TIMES LOCAL:4 , 0.80
CASE IS < PALAMLV:3
	TIMES LOCAL:0 , 1.00
	TIMES LOCAL:4 , 1.00
CASE IS < PALAMLV:4
	TIMES LOCAL:0 , 1.50
	TIMES LOCAL:4 , 1.20
CASE IS < PALAMLV:5
	TIMES LOCAL:0 , 2.00
	TIMES LOCAL:4 , 1.40
CASEELSE
	TIMES LOCAL:0 , 2.50
	TIMES LOCAL:4 , 1.60
ENDSELECT

;페니스があると, 쾌Ｃ増加
SIF PENIS(TARGET)
	TIMES LOCAL:0 , 1.50

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM100
;縄
IF TEQUIP:구속
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 꼼짝할 수 없는 %CALLNAME:TARGET%의 배수구멍을, 마음껏 유린했다…
;후타나리？
ELSEIF (TALENT:후타나리 || TALENT:남자) && RAND:3 > 0
	PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 배수구멍에 삽입하고, 
	IF TALENT:작은몸집 && BASE:절정 < 5000
		PRINT 사랑스러운 
	ELSEIF BASE:절정 >= 5000 && RAND:2 == 0
		PRINT 배꼽을 향해서 발기해있는 
	ELSEIF BASE:절정 >= 3000
		PRINT 형태가 뚜렷이 보일정도로 천을 밀어올리고 있는  
	ENDIF
	PRINTFORM %CALLNAME:TARGET%의 페니스
	IF RAND:2 == 0
		PRINTW 를, 학교수영복 천으로 감싸 격렬하게 문질러 올렸다…
	ELSE
		PRINTW 에 벅벅 문질렀다…
	ENDIF
;説明的
ELSEIF RAND:3 == 0
	PRINTFORMW %조사처리(CALLNAME:TARGET,"를")% 위를 향해 엎드리게 하고, 학교수영복의 하복부에 열려있는 배수구멍에 페니스를 삽입해, 매끈매끈한 배와 학교수영복 원단의 감촉을 즐겼다…
ELSEIF RAND:2 == 0
	;「부끄럼쟁이」で「겁쟁이」でない
	IF TALENT:부끄럼쟁이 && TALENT:겁쟁이 == 0
		PRINT 부끄러운 듯이 
	;「겁쟁이」で「연모」でない
	ELSEIF TALENT:겁쟁이 && TALENT:연모 == 0
		PRINT 흠칫흠칫 
	;「소악마」で랜덤1/2
	ELSEIF TALENT:소악마 && RAND:2 == 0
		PRINT 히죽히죽거리면서 
	;음란or욕정3000이상
	ELSEIF (TALENT:음란 || PALAM:욕정 >= 3000) && RAND:4 > 0
		PRINT 요염하게 미소지으면서 
	ELSEIF RAND:2 == 0 && TEQUIP:아이마스크 == 0
		SIF RAND:2 == 0
			PRINT 즐거운듯이 
		PRINT 미소지으면서 
	ELSEIF ABL:봉사정신 >= 3
		PRINT 자진해서 
	ENDIF
	PRINTFORMW 배수구멍을 벌리고 있는 %조사처리(CALLNAME:TARGET,"가")% 권하는 대로, 학교수영복으로 훑으며 즐겼다…
ELSE
	PRINTFORM %조사처리(CALLNAME:PLAYER,"가")% 배수구멍에 페니스를 넣자, %조사처리(CALLNAME:TARGET,"는")% 
	;「부끄럼쟁이」で「겁쟁이」でない
	IF TALENT:부끄럼쟁이 && TALENT:겁쟁이 == 0
		PRINT 눈을 내리깔면서 
	;「겁쟁이」で「연모」でない
	ELSEIF TALENT:겁쟁이 && TALENT:연모 == 0
		PRINT 흠칫흠칫 
	;「소악마」で랜덤1/2
	ELSEIF TALENT:소악마 && RAND:2 == 0
		PRINT 히죽히죽거리면서 
	;음란or욕정3000이상
	ELSEIF (TALENT:음란 || PALAM:욕정 >= 3000) && RAND:4 > 0
		PRINT 요염하게 미소지으면서 
	ELSEIF RAND:2 == 0 && TEQUIP:아이마스크 == 0
		SIF RAND:2 == 0
			PRINT 즐거운 듯이 
		PRINT 미소지으면서 
	ELSEIF ABL:봉사정신 >= 3
		PRINT 배 위에 손을 얹고, 
	ENDIF
	PRINTFORMW 학교수영복너머로 페니스를 자극했다…
ENDIF


@COM_ORDER_COM100, ARG
VARSET LOCAL
LOCALS = 

;すべての命令に共通の요정素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER, ARG

LOCAL = RESULT

;ABL:봉사정신
IF ABL:봉사정신
	LOCAL += ABL:봉사정신 * 4
	LOCALS = %LOCALS% + %ABLNAME:6%LV{ABL:봉사정신}({ABL:봉사정신 * 4})
ENDIF
;ABL:정액중독
IF ABL:정액중독
	LOCAL += ABL:정액중독 * 3
	LOCALS = %LOCALS% + %ABLNAME:11%LV{ABL:정액중독}({ABL:정액중독 * 3})
ENDIF
;부끄럼쟁이
IF TALENT:부끄럼쟁이
	LOCAL -= 5
	LOCALS = %LOCALS% - %TALENTNAME:35%(5)
ENDIF
;쾌감을부정
IF TALENT:쾌감을부정
	LOCAL -= 1
	LOCALS = %LOCALS% - %TALENTNAME:71%(1)
ENDIF

;미약
IF TEQUIP:미약
	LOCAL += 6
	LOCALS = %LOCALS% + %ITEMNAME:31%(6)
ENDIF

;불결있음
IF COM_ORDER_STAIN("Ｐ", "Ｐ")/3
	LOCAL -= COM_ORDER_STAIN("Ｐ", "Ｐ")/3
	LOCALS = %LOCALS% - 불결있음
	;악취둔감
	SIF TALENT:악취둔감 || TALENT:악취민감
		LOCALS = %LOCALS%, \@TALENT:악취둔감 ? %TALENTNAME:61% # %TALENTNAME:62%\@
	LOCALS = %LOCALS%({COM_ORDER_STAIN("Ｐ", "Ｐ")/3})
ENDIF

;合計を表示(30이상で実行)
CALL PRINT_COM_ORDER_COM, LOCALS, ARG, LOCAL, 30

RETURN RESULT
