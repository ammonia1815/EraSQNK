@COM12
;풋잡
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM12, ARG

@SOURCE_COM12, ARG
VARSET LOCAL

LOCAL:20 += 30
LOCAL:21 += 100

LOCAL:14 = 500

;ABL:마조끼をみる
SELECTCASE ABL:마조끼
CASE 0
	LOCAL:5 = 150
CASE 1
	LOCAL:5 = 300
CASE 2
	LOCAL:5 = 600
CASE 3
	LOCAL:5 = 900
CASE 4
	LOCAL:5 = 1500
CASEELSE
	LOCAL:5 = 2200
ENDSELECT
;도착적
SIF TALENT:도착적
	TIMES LOCAL:5 , 1.80

;ABL:Ｃ감각をみる
LOCAL:0 = CALCF("Ｃ刺激", 1)
LOCAL:0 = SOURCE_EXABL(LOCAL:0, 32, PLAYER)

SIF PENIS(TARGET) == 0 && TEQUIP:바이브
	CALL SOURCE_COM31, 30

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM12
PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의 
IF PENIS(TARGET)
	PRINTFORMW %TEXTS("페니스：視覚", TARGET)% 페니스를 발로 훑었다…
ELSE
	IF TEQUIP:바이브
		PRINTFORMW 그곳에 있는 바이브를 발로 만지작거렸다.
		;바이브を弄られるのに慣れている
		IF CHECK("바이브")
			PRINTFORMW %타겟은()% 발을 만지작거리는데 맞춰서 질을 오물거려 바이브를 밀어내, 한층 더 고문을 기다리며 허리를 내밀었다…
		ELSE
			PRINTFORMW 체중이 실린 바이브로 예상밖의 깊은 장소를 후벼파진 %타겟은()% ,공포가 배인 얼굴로 %조사처리(CALLNAME:PLAYER,"를")% 노려봐 왔다…
		ENDIF
	ELSE
		PRINTFORMW 가랑이를 발로 희롱했다…
	ENDIF
ENDIF



