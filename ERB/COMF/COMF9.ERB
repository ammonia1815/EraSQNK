@COM9
;애널핥기※소비を少し高めにする
IF TEQUIP:넘어뜨리기
	PRINTL Ａ안면기승
ELSE
	PRINTL 애널핥기
ENDIF

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

IF TEQUIP:넘어뜨리기
	STR:0 = Ａ안면기승
ELSE
	STR:0 = 애널핥기
ENDIF

;-------------------------------------------------
;사정체크
;-------------------------------------------------
SIF TEQUIP:넘어뜨리기
	CALL SAMEN_CHECK

RETURN 1

@FLAG_COM9, ARG
;대상のＡ⇔조교자の입の汚れが移動
CALL EQ_STAIN, TARGET, PLAYER, 4, 0


@SOURCE_COM9, ARG
VARSET LOCAL

LOCAL:20 += 20
LOCAL:21 += 70

;※노출のソースを元より高めにしてみる
LOCAL:12 = 800
LOCAL:14 = 500
;Ａ액체추가
LOCAL:19 = 100

;ABL:Ａ감각をみる
LOCAL:2 = CALCF("Ａ刺激", 1, 70)
;조교자の혀놀림をみる
LOCAL:2 = SOURCE_EXABL(LOCAL:2, 30, PLAYER)

;안면기승の時には快感を得るよりはむしろ攻撃重視
SIF TEQUIP:넘어뜨리기
	TIMES LOCAL:2, 0.30

;EXP:Ａ경험に応じたＡ快感, 고통, 공포, 일탈(반감)보정
SELECTCASE EXP:Ａ경험
CASE IS >= 300
	TIMES LOCAL:2, 1.60
	TIMES LOCAL:6, 0.50
	TIMES LOCAL:9, 0.20
	TIMES LOCAL:14, 0.20
CASE IS >= 150
	TIMES LOCAL:2, 1.40
	TIMES LOCAL:6, 0.60
	TIMES LOCAL:9, 0.40
	TIMES LOCAL:14, 0.60
CASE IS >= 50
	TIMES LOCAL:2, 1.20
	TIMES LOCAL:6, 0.80
	TIMES LOCAL:9, 0.70
CASE IS >= 20
	TIMES LOCAL:14, 1.20
CASE IS >= 5
	TIMES LOCAL:2, 0.50
	TIMES LOCAL:6, 1.50
	TIMES LOCAL:9, 1.50
	TIMES LOCAL:14, 1.50
CASEELSE
	TIMES LOCAL:2, 0.10
	TIMES LOCAL:6, 2.00
	TIMES LOCAL:9, 2.00
	TIMES LOCAL:14, 2.00
ENDSELECT

;PALAM:욕정をみる
SELECTCASE PALAM:욕정
CASE IS < PALAMLV:1
	TIMES LOCAL:13, 0.30
CASE IS < PALAMLV:2
	TIMES LOCAL:13, 0.50
CASE IS < PALAMLV:3
	TIMES LOCAL:13, 0.80
CASE IS < PALAMLV:4
CASE IS >= PALAMLV:4
	TIMES LOCAL:13, 1.20
ENDSELECT

;Ａ민감, 둔감をみる
;쾌Ａ自体の체크は後でまとめてやる
IF TALENT:Ａ둔감
	TIMES LOCAL:6, 0.50
	TIMES LOCAL:11, 0.50
	TIMES LOCAL:13, 0.50
	TIMES LOCAL:14, 0.50
ELSEIF TALENT:Ａ민감
	TIMES LOCAL:6, 1.50
	TIMES LOCAL:11, 1.50
	TIMES LOCAL:13, 1.50
	TIMES LOCAL:14, 1.50
ENDIF

;처녀で정조관념or엉덩이許可플래그
IF (TALENT:처녀 == 1 && TALENT:정조관념) || CFLAG:100
	LOCAL:14 /= 3
	LOCAL:15 /= 3
ENDIF

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM9
;同時넘어뜨리기
IF TEQUIP:Ｗ넘어뜨리기
	PRINTFORM %조사처리(CALLNAME:TARGET,"는")% 
	;조수が前回と同じ行動(違う行動の時は, ここでなく, EQUIP_COM32で記述)
	SIF SELECTCOM:1 == PREVCOM:1 && SELECTCOM:1 >= 0
		PRINTFORM %TEXTS("Ｗ넘어뜨리기 중")%
	IF SELECTCOM == PREVCOM
		PRINTFORM %CALLNAME:MASTER%의 얼굴
		IF TALENT:부끄럼없음 || TALENT:음란
			PRINT 위에서 엉덩이를 주욱 벌려, 
		ELSE
			PRINT 에 허리를 내려서, 
		ENDIF
		PRINTFORMW 엉덩이구멍을 계속 핥게했다…
	ELSE
		IF COND("다리 사용") == 0
			PRINTFORMW %CALLNAME:MASTER%의 얼굴을 억누르고, 입에 애널을 밀어붙였다……
		ELSE
			PRINTFORMW %CALLNAME:MASTER%의 얼굴에 올라타고, 입에 애널을 밀어붙였다……
		ENDIF
		;음란or욕정4LV이상なら追加
		IF TALENT:음란 || PALAM:욕정 >= PALAMLV:4
			SIF (SELECTCOM:1 == 65 || SELECTCOM:1 == 75) && ABL:레즈끼 >= 3 && RAND:2 == 0
				PRINTFORMW %타겟은()% %조사처리(CALLNAME:(TARGET:1),"과")% 혀를 걸면서, 쾌감을 탐내고 있다…
		ENDIF
	ENDIF
;押し倒されているときは안면기승
ELSEIF TEQUIP:넘어뜨리기 && (TEQUIP:안면기승 == SELECTCOM || PREVCOM == SELECTCOM) && COND("前Ｔ조교자") == PLAYER
	PRINTFORM %조사처리(CALLNAME:TARGET,"는")% 
	IF RAND:2 == 0
		PRINTFORM %TEXTS("애널")%
	ELSEIF ABL:욕망 + ABL:Ａ감각 >= 6
		PRINTFORM %조사처리(NAMES("손가락"),"로")% 넓힌 
	ENDIF
	PRINTFORMW 애널을 %CALLNAME:PLAYER%에게 계속 봉사시키고있다…
	;애널が노출していないで, 팬티をずらしていないなら自動的にずらす
	SIF !CHECK_CLO("Ａ노출")
		CALL SETFLAG, "바텀을노출：텍스트없음", TARGET
ELSEIF TEQUIP:넘어뜨리기
	PRINTFORM %조사처리(CALLNAME:TARGET,"는")% 
	;前回が안면기승
	IF (TEQUIP:안면기승 == 1 || PREVCOM == 1) && COND("前Ｔ조교자") == PLAYER
		PRINTFORM 가볍게 허리를 띄우고, 이번에는 
		;애널が노출していないで, 팬티をずらしていないなら自動的にずらす
		SIF !CHECK_CLO("Ａ노출")
			CALL SETFLAG, "바텀을노출：텍스트없음", TARGET
	;발がある
	ELSEIF COND("다리が無い") == 0
		;「부끄럼쟁이」で「겁쟁이」でない
		IF TALENT:부끄럼쟁이 && TALENT:겁쟁이 == 0
			PRINT 부끄러운 듯이 
		;「겁쟁이」で「연모」でない
		ELSEIF TALENT:겁쟁이 && TALENT:연모 == 0
			PRINT 흠칫흠칫 
		;「소악마」で랜덤1/2
		ELSEIF TALENT:소악마 && RAND:2 == 0
			PRINT 히죽히죽거리면서 
		ENDIF
		PRINTFORM %CALLNAME:PLAYER%의 안면 위에 
		;「부끄럼쟁이」or「겁쟁이」で랜덤
		IF (TALENT:부끄럼쟁이 || TALENT:겁쟁이) && RAND:3 == 0
			PRINTFORM 그곳을 %조사처리(NAMES("손"),"로")%  숨기면서 
		;「건방짐」or「음란」or「소악마」で랜덤
		ELSEIF (TALENT:건방짐 || TALENT:음란 || TALENT:소악마) && RAND:2 == 0
			PRINT 다리를 크게 벌리고
		ENDIF
		PRINT 주저앉으며, 
		;애널が노출していないで, 팬티をずらしていないなら自動的にずらす
		SIF !CHECK_CLO("Ａ노출") && CHECK_CLO("Ａ노출可")
			CALL SETFLAG, "바텀을노출：텍스트有り", TARGET
	;라미아, で後は一応他の条件もつける…라미아って팬티はいてないな
	ELSEIF TALENT:라미아
		;「부끄럼쟁이」で「겁쟁이」でない
		IF TALENT:부끄럼쟁이 && TALENT:겁쟁이 == 0
			PRINT 부끄러운 듯이 
		;「겁쟁이」で「연모」でない
		ELSEIF TALENT:겁쟁이 && TALENT:연모 == 0
			PRINT 흠칫흠칫 
		;「소악마」で랜덤1/4
		ELSEIF TALENT:소악마 && RAND:4 == 0
			PRINT 히죽히죽거리면서 
		;「건방짐」or「음란」or「소악마」で랜덤1/3
		ELSEIF (TALENT:건방짐 || TALENT:음란 || TALENT:소악마) && RAND:3 == 0
			PRINT 과시하는 듯이 천천히 
		;욕정4이상＋A감각3이상で랜덤1/2
		ELSEIF PALAM:욕정 >= PALAMLV:4 && ABL:Ａ감각 >= 3 && RAND:2 == 0
			PRINT 기다릴 수 없다는 듯이 허겁지겁 
		;その他
		ELSE
			PRINT 슥슥
		ENDIF
		PRINTFORM 위를 향한 %CALLNAME:PLAYER%의 얼굴에 허리를 접근하고, 
	;머메이드, で後は一応他の条件もつける…머메이드のアレは옷には見えないのだが一体なんだろう…
	ELSEIF TALENT:머메이드
		PRINT 뒤에 손을 짚고 
		;「부끄럼쟁이」で「겁쟁이」でない
		IF TALENT:부끄럼쟁이 && TALENT:겁쟁이 == 0
			PRINT 부끄러운 듯이 
		;「겁쟁이」で「연모」でない
		ELSEIF TALENT:겁쟁이 && TALENT:연모 == 0
			PRINT 흠칫흠칫 
		;「소악마」で랜덤1/2
		ELSEIF TALENT:소악마 && RAND:2 == 0
			PRINT 히죽히죽거리면서 
		;「건방짐」or「음란」or「소악마」で랜덤1/3
		ELSEIF (TALENT:건방짐 || TALENT:음란 || TALENT:소악마) && RAND:3 == 0
			PRINT 천천히 
		;욕정4이상＋A감각3이상で랜덤1/2
		ELSEIF PALAM:욕정 >= PALAMLV:4 && ABL:Ａ감각 >= 3 && RAND:2 == 0
			PRINT 기다리지 못하고 허겁지겁 
		ELSE
			PRINT 깨나른하게 
		ENDIF
		PRINTFORM 드러누운 %CALLNAME:PLAYER%의 얼굴에 주저앉듯이 허리를 가져가, 
	ENDIF
	;욕정6이상orA감각3이상で랜덤1/2
	IF PALAM:욕정 >= PALAMLV:6 && ABL:Ａ감각 >= 3 && RAND:2 == 0
		PRINTFORMW %CALLNAME:PLAYER%의 입에 %TEXTS("애널")% 애널을 밀어붙였다……
	;それ以外
	ELSE
		PRINTFORMW %CALLNAME:PLAYER%의 입가에 %TEXTS("애널")%애널을 내밀었다……
	ENDIF
ELSEIF PREVCOM == SELECTCOM && COND("前Ｔ조교자") == PLAYER
	PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의
	IF RAND:2 || TEQUIP:식스나인 || COND("성벽：엉덩이", PLAYER)
		PRINTFORM %TEXTS("엉덩이：감촉")%%TEXTR("에 얼굴을 파묻고", "를 자신의 얼굴로 즐기면서", "를 만끽하면서")%,
	ELSEIF COND("손가락 사용", PLAYER)
		PRINTFORM 엉덩이를 펴넓혀,%TEXTR("완전히 보이는", "노출된", "확대된")%
	ENDIF
	PRINTFORMW %TEXTR("아누스를 집요하게 핥는것을 ", "엉덩이 구멍의 점막을 혀로 맛보길", "항문의 주름을 혀로 핥길", "엉덩이구멍의 맛을 즐기길")%계속했다…
;押し倒されていないときは後ろから…いや前からってのもあるか…
ELSE
	IF TEQUIP:식스나인
		PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의 
		;애널が노출していないで, 팬티をずらしていないなら自動的にずらす
		IF !CHECK_CLO("Ａ노출") && CHECK_CLO("Ａ노출可")
			CALL SETFLAG, "바텀을노출：텍스트有り", TARGET
		ELSE
			PRINTFORM 엉덩이살을 주욱 벌리고 
		ENDIF
		PRINTFORM , %TEXTS("６９")%
	ELSE
		SIF PLAYER != MASTER
			PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 
		;前回Ａくぱぁ
		IF PREVCOM == 14
			PRINT 애널을 과시하고있는 
		;縄着用時かつ라미아でないかつ머메이드でない
		ELSEIF TEQUIP:구속 && COND("다리が無い") == 0
			PRINT 다리가 크게 벌려진 
		;부끄럼쟁이で쾌Ａor욕정が高い
		ELSEIF TALENT:부끄럼쟁이 && (PALAM:쾌Ａ >= PALAMLV:3 || PALAM:욕정 >= PALAMLV:4)
			PRINT 양손으로 얼굴을 숨기고 부끄러워하면서도, 자진해서 다리를 열고있는 
		;부끄럼쟁이で쾌Ａor욕정が低い
		ELSEIF TALENT:부끄럼쟁이 && (PALAM:쾌Ａ <= PALAMLV:3 || PALAM:욕정 <= PALAMLV:4)
			PRINT 단단히 다리를 닫고있는 
		;음란or욕정4LV이상
		ELSEIF (TALENT:음란 || PALAM:욕정 >= PALAMLV:4) && ABL:Ａ감각 >= 3
			PRINT 다리를 크게 벌리고있는 
		ENDIF
		PRINTFORM %CALLNAME:TARGET%의 
		;애널が노출していないで, 팬티をずらしていないなら自動的にずらす
		IF !CHECK_CLO("Ａ노출") && CHECK_CLO("Ａ노출可")
			CALL SETFLAG, "바텀을노출：텍스트有り", TARGET
		;前から
		ELSEIF RAND:2
			PRINT 허리를 꾹 들어올리고
		;後ろから
		ELSE
			PRINT 가랑이에 얼굴을 대고
		ENDIF
		PRINT , 
	ENDIF
	PRINTFORM %TEXTS("애널")% 엉덩이구멍을 
	IF RAND:2 == 0
		PRINT 혀로 
	ELSE
		PRINT 입술로 
	ENDIF
	IF RAND:4 == 0
		PRINT 부드럽게 
	ELSEIF RAND:3 == 0
		PRINT 정중하게 
	ELSEIF RAND:2 == 0
		PRINT 격렬하게 
	ELSE
		PRINT 거칠게 
	ENDIF
	IF RAND:4 == 0
		PRINTFORMW 빨아들였다…
	ELSEIF RAND:3 == 0
		PRINTFORMW 핥아주었다…
	ELSEIF RAND:2 == 0
		PRINTFORMW 애무했다…
	ELSE
		PRINTFORMW 맛보았다…
	ENDIF
ENDIF
