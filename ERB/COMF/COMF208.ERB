@COM208
;촉수구욕
PRINTL 촉수구욕

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = 촉수구욕

RETURN 1

@FLAG_COM208, ARG

@SOURCE_COM208, ARG
VARSET LOCAL

LOCAL:20 += 80
LOCAL:21 += 100

;ABL:Ｍ감각をみる
LOCAL:18 = CALCF("Ｍ刺激", 2, 150)

;ABL:봉사정신をみる
SELECTCASE ABL:봉사정신
CASE 0
	LOCAL:4 += 200
	LOCAL:13 = 100
	TIMES LOCAL:18 , 0.25
CASE 1
	LOCAL:4 += 300
	LOCAL:13 = 200
	TIMES LOCAL:18 , 0.33
CASE 2
	LOCAL:4 += 400
	LOCAL:13 = 400
	TIMES LOCAL:18 , 0.75
CASE 3
	LOCAL:4 += 500
	LOCAL:13 = 600
	TIMES LOCAL:18 , 1.50
CASE 4
	LOCAL:4 += 600
	LOCAL:13 = 800
	TIMES LOCAL:18 , 3.00
CASEELSE
	LOCAL:4 += 800
	LOCAL:13 = 1100
	TIMES LOCAL:18 , 6.00
ENDSELECT

LOCAL:16 += LOCAL:13

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM208
;촉수の抜き挿し
IF TEQUIP:촉수구욕
	TEQUIP:촉수구욕 = 0
	PRINTFORMW 촉수는 %CALLNAME:TARGET%의 입에서 뽑혀나왔다…
ELSEIF TEQUIP:구속
	IF (ABL:목확장 >= 3 || TALENT:PLAYER:새드) && TEQUIP:바이브 + TEQUIP:애널바이브 && RAND:2 == 0
		PRINTFORML 격렬하게 찔러 올려지고 있는 %조사처리(CALLNAME:TARGET,"가")% 숨이 차서 크게 입을 열자, 
		PRINTFORMW 단단히 묶고있던 촉수 중 한개가 스르르륵 밀고 들어와, 목구멍의 안까지 범하기 시작했다…
		TEQUIP:촉수구욕 = 2
	ELSE
		PRINTFORML 단단히 몸을 묶으면서 체내를 기어 돌아다니던 촉수 중 한개가 조심스럽게 입술을 노크하자, 
		PRINTFORM %조사처리(CALLNAME:TARGET,"는")% 
		;どれくらいか多いのかで思案中 暫定で150 300くらいを想定
		IF EXP:촉수경험 >= 150
			PRINTW 탐내듯이 혀를 내밀어 쿠퍼액을 핥고, 입에 넣었다…
		ELSE
			PRINTW 조심조심 입을 열어 쿠퍼액을 질질 흘리는 촉수를 받아들였다…
		ENDIF
		TEQUIP:촉수구욕 = 1
	ENDIF
ELSE
	PRINTFORM 축축하게 빛나는 촉수가 %CALLNAME:TARGET%의 
	IF (ABL:목확장 >= 3 || TALENT:PLAYER:새드) && RAND:2 == 0
		PRINTFORMW 목구멍 안까지 침입했다…
		TEQUIP:촉수구욕 = 2
	ELSE
		PRINTFORMW 입에 침입했다…
		TEQUIP:촉수구욕 = 1
	ENDIF
ENDIF
CALL ADD_STAIN, TARGET, "Ｍ", "촉수"

@EQUIP_COM98
SIF SELECTCOM == 208
	RETURN 0
;촉수구욕中
CALL SOURCE_COM208, 30

RETURN 1

@EQUIP_COM98_2
SIF SELECTCOM == 208
	RETURN 0
;촉수구욕中
PRINTL ＜촉수구욕중＞
EXP:펠라경험 += 1
PRINTL 펠라경험＋１
RETURN 1
