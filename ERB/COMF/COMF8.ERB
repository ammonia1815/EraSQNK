@COM8
;손가락넣기
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = %STR:2%

RETURN 1

@FLAG_COM8, ARG
IF CHECK_COM("Ｇ스팟자극")
	CALL FLAG_INSERTION, "손가락넣기", ARG, 150
ELSEIF TEQUIP:바이브
	CALL FLAG_COM31, 130
ELSE
	CALL FLAG_INSERTION, "손가락넣기", ARG, 100
ENDIF

@SOURCE_COM8, ARG
IF CHECK_COM("Ｇ스팟자극")
	CALL SOURCE_INSERTION, "손가락넣기", ARG, 150
ELSEIF TEQUIP:바이브
	CALL SOURCE_INSERTION, "바이브", ARG, 130
ELSE
	CALL SOURCE_INSERTION, "손가락넣기", ARG, 100
ENDIF


@TRAIN_MESSAGE_COM8
PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 
IF TEQUIP:키스
	PRINTFORM %TEXTS("키스継続")%,
	CALL SETFLAG, "추가키스플래그", TARGET
ELSEIF TEQUIP:식스나인
	PRINTFORM %TEXTS("６９")%
ELSEIF INSERT("Ａ인서트") && PREVCOM != SELECTCOM
	PRINTFORM 애널을 후벼내면서, 
ELSEIF PREVCOM == 1 && PENIS(TARGET) == 0 && CHECK_CLO("Ｖ노출")
	SIF PALAM:욕정 >= 3000 && ABL:Ｃ감각 >= 3
		PRINT 발기한 
	PRINTFORM 클리토리스를 핥으면서, 
ELSEIF PREVCOM == 13 && TEQUIP:바이브 == 0
	PRINT 눈 앞에서 전개되고 있는 
ENDIF
PRINTFORM %CALLNAME:TARGET%의 

;바이브
IF TEQUIP:바이브
	;穴開きでない팬티をはいていて, ずらされていないなら, 必ずずらす
	SIF CHECK_CLO("Ｖ노출") == 0
		CALL SETFLAG, "바텀을노출：텍스트有り", TARGET
	IF RAND:2 == 0
		PRINTFORM 질에 묻혀있는 
	ELSE
		PRINTFORM 질을 진동시키고있는 
	ENDIF

	SELECTCASE SIZE("바이브")
	CASE IS >= 3
		IF RAND:2 == 0
			PRINT 흉악한 
		ELSE
			PRINT 극태 
		ENDIF
	CASE 2
		IF RAND:2
			PRINT 굵은 
		ELSE
			PRINT 커다란 
		ENDIF
	ENDSELECT

	;ピストン
	IF RAND:2 == 0
		PRINTFORM 바이브를 
		IF (ABL:Ｖ감각 >= 3 || ABL:Ｖ확장 >= SIZE("바이브") || TALENT:PLAYER:새드) && RAND:2 == 0
			PRINT 벅벅 격렬한 
		ELSEIF RAND:2 == 0
			PRINT 완급을 조절하는 
		ELSE
			PRINT 느긋하고 큰 
		ENDIF
		PRINTFORM 스트로크로 움직
		IF ABL:Ｖ확장 < SIZE("바이브") && RAND:3 == 0
			PRINTFORMW 여, 커다란 이물로 확대된 구멍을 유린했다…
		ELSEIF (ABL:Ｖ감각 >= 3 || TALENT:PLAYER:새드) && RAND:4 == 0
			PRINTFORMW 여, 단단한 첨단으로 자궁구를 밀어올렸다…
		ELSEIF RAND:3 == 0
			PRINTFORMW 여, 질내의 점액에서 거품이 날 정도로 휘저었다…
		ELSEIF RAND:2 == 0
			PRINTFORMW 여, 질의 점막을 문질렀다…
		ELSE
			PRINTFORMW 였다…
		ENDIF
	;スイッチON
	ELSE
		PRINTFORM 바이브의 스위치를 최강으로 설정하고, 
		IF (ABL:Ｖ감각 >= 4 || ABL:Ｖ확장 >= SIZE("바이브") || TALENT:PLAYER:새드) && RAND:3 == 0
			IF RAND:2 == 0
				PRINTFORM 도려내듯이 
			ELSE
				PRINTFORM 가차없이
			ENDIF
		ELSEIF ABL:Ｖ확장 <= SIZE("바이브") && (ABL:Ｖ감각 >= 3 || TALENT:PLAYER:새드) && RAND:2 == 0
			PRINTFORM 질이 펼쳐질 듯이 힘을 집중해 
		ELSEIF RAND:3 == 0
			PRINTFORM 빙글빙글 
		ELSEIF RAND:2 == 0
			PRINTFORM 상태를 살피면서 
		ELSE
			PRINTFORM 좌우로 비틀면서 
		ENDIF
		PRINTFORMW 꿀단지를 휘저었다…
	ENDIF
ELSEIF CHECK_COM("Ｇ스팟자극")
	IF RAND:2
		PRINTFORM 질의 민감한 
		IF RAND:2
			PRINTFORM 부분을 손가락으로 집요하게 문질러주었다
		ELSE
			PRINTFORM 점막을 문지르듯이 손가락을 움직였다
		ENDIF
	ELSE
		PRINTFORM 질 안의 손가락을 구부려, 
		IF RAND:2
			PRINTFORM 까끌까끌한 천장을 자극했다
		ELSE
			PRINTFORM 살짝 만지는 느낌이 다른 델리케이트한 점막을 집중적으로 애무했다
		ENDIF
	ENDIF
	;前回こらえた＆そろそろ限界
	IF ABL:Ｖ감각 >= 3 && PREVEX("가는것을참았다") && UP_PALAM(1) > MAX(MULTIPLY(MAXBASE:성감허용치여자, 70), 10000)
		PRINTFORMW 
		IF RAND:3 == 0
			PRINTFORM 주어진 쾌감에 반응하듯이, 
		ELSEIF RAND:2
			PRINTFORM 격한 자극에 견딜 수 없는지, 
		ELSE
			PRINTFORM 상당히 기분이 좋은지, 
		ENDIF
		IF ABL:Ｖ명기도 >= 30 && RANDIF(2, CUP:Ｖ윤 >= 1000)
			PRINTFORMW %CALLNAME:TARGET%의 질은 손가락 움직임에 맞춰 경련하는 듯한 움직임을 보여주거나 갑자기 강한 힘으로 입구를 조이거나 하고있다…
		ELSEIF CUP:Ｖ윤 >= 1000 && RAND:2
			PRINTFORMW %CALLNAME:TARGET%의 질은 굉장한 양의 애액을 흘리며 %CALLNAME:PLAYER%의 손을 더럽히고있다…
		ELSE
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% \@ TALENT:호색 && RAND:2 ? 적극적 # 무의식적 \@으로 %CALLNAME:PLAYER%의 움직임에 맞춰서 허리를 움직이고있다…
		ENDIF
	ELSE
		PRINTFORMW …
	ENDIF
;손가락
ELSE
	LOCALS = 
	;穴開きでない팬티をはいていて, ずらされていないならずらすことも
	IF CHECK_CLO("Ｖ노출") == 0
		IF RAND:3 == 0
			CALL SETFLAG, "바텀을노출：텍스트有り", TARGET
		ELSE
			PRINTFORM %CLO("가랑이")%에 손을 넣고
		ENDIF
		LOCALS = 바텀묘사
	ENDIF
	IF PREVEX("Ｖ내사정") && CHECK_STAIN(STAIN:Ｖ, "정액")
		PRINTFORMW 질내사정 직후의 질에 손가락을 삽입하고, 주름에 달라붙은 정액을 천천히 긁어내기시작했다…
	ELSE
		IF CHECK_CLO("Ｖ노출") == 0 || RAND:3 == 0
			SIF PALAM:Ｖ윤 >= PALAMLV:4
				PRINT 함빡 젖은 
			IF TALENT:작은몸집
				PRINT 어린 균열
			ELSE
				PRINT 비렬
			ENDIF
		ELSE
			PRINTFORM %TEXTS("보지：감촉")%
		ENDIF

		IF PREVCOM == SELECTCOM
			PRINTFORMW 을 손가락으로 휘저었다…
		ELSE
			PRINTFORM 에 
			IF ABL:Ｖ확장 >= 4 && EXP:Ｖ피스트경험 >= 5 && CHECK("Ｖ피스트") && RAND:2
				PRINTFORM 가지런히 모은 손가락을 손등까지 삽입
			ELSE
				IF ABL:Ｖ확장 >= 4 || (ABL:Ｖ확장 >= 2 && RAND:2)
					PRINTFORM 중지와 약지를 
				ELSE
					PRINTFORM \@ RAND:2 ? 중지를 # 손가락을 \@ 
				ENDIF
				PRINTFORM \@ RAND:2 ? 뿌리까지 # 천천히 \@삽입
			ENDIF

			IF LOCALS == "바텀묘사"
				PRINTFORMW 했다…
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 해서, 질벽을 손가락의 끝부분으로 간질였다…
				ELSEIF RAND:2
					PRINTFORMW 해서, 주름을 문지르듯이 휘저었다…
				ELSE
					PRINTFORMW 해서, 안의 상태를 즐겼다…
				ENDIF
			ENDIF
		ENDIF
	ENDIF

	IF PREVEX("Ｖ내사정") && CHECK_STAIN(STAIN:Ｖ, "정액")
		PRINTFORM 질에서 뽑아낸 정액 투성이 손가락을 보여주자, 
		IF ABL:정액중독 > 3 && PALAM:욕정 >= 10000
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 황홀한 듯이 혀를 내밀었다…
		ELSEIF TALENT:연모
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 기쁜듯이 미소를 보였다…
		ELSE
			IF RAND:2
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 부끄러운 듯한 표정을 지었다…
			ELSE
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 부끄러운 듯이 시선을 피했다…
			ENDIF
		ENDIF
	ELSEIF EXP:Ｖ경험
		SELECTCASE EXP:Ｖ경험
		CASE IS <= 30
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손가락의 감촉에 한숨을 토해 참고 있다…
		CASE IS <= 50
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손가락의 감촉을 즐기고 있는 것 같다…
		CASE IS <= 80
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손가락이 꿈틀거리는 감촉에 넋을 잃고 있다…
		CASE IS <= 120
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 뱃속을 휘저어지는 감각에 뜨거운 한숨을 토해 몸부림치고 있다…
		CASEELSE
			IF ABL:Ｖ감각 >= 3 && RAND:2
				PRINTFORMW %CALLNAME:TARGET%의 쾌락을 철저히 가르쳐진 암컷 구멍은 괴롭혀도 괴로운 듯한 모습은 없고, 오히려 기뻐해 손가락을 물고 있다…
			ELSE
				PRINTFORMW %CALLNAME:TARGET%는 군침을 흘려, 스스로 허리를 흔들어 쾌락을 탐내고 있다…
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
