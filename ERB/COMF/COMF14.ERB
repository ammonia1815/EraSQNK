@COM_ORDER_COM14, ARG
VARSET LOCAL
LOCALS = 

;すべての命令に共通の요정素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER, ARG

LOCAL = RESULT

;ABL:Ａ감각
IF ABL:Ａ감각
	LOCAL += ABL:Ａ감각 * 5
	LOCALS = %LOCALS% + %ABLNAME:5%LV{ABL:Ａ감각}({ABL:Ａ감각 * 5})
ENDIF
;ABL:노출벽
IF ABL:노출벽
	LOCAL += ABL:노출벽 * 5
	LOCALS = %LOCALS% + %ABLNAME:7%LV{ABL:노출벽}({ABL:노출벽 * 4})
ENDIF
;ABL:마조끼
IF ABL:마조끼
	LOCAL += ABL:마조끼 * 3
	LOCALS = %LOCALS% + %ABLNAME:8%LV{ABL:마조끼}({ABL:마조끼 * 3})
ENDIF
;自制心
IF TALENT:냉정
	LOCAL -= 5
	LOCALS = %LOCALS% - %TALENTNAME:20%(5)
ENDIF
;부끄럼쟁이
IF TALENT:부끄럼쟁이
	LOCAL -= 25
	LOCALS = %LOCALS% - %TALENTNAME:35%(25)
ENDIF
;부끄럼없음
IF TALENT:부끄럼없음
	LOCAL += 5
	LOCALS = %LOCALS% + %TALENTNAME:36%(5)
ENDIF
;청초
IF TALENT:청초
	LOCAL -= 20
	LOCALS = %LOCALS% - %TALENTNAME:39%(20)
ENDIF
;쾌감을부정
IF TALENT:쾌감을부정
	LOCAL -= 5
	LOCALS = %LOCALS% - %TALENTNAME:71%(5)
ENDIF
;前回내사정
IF PREVEX("Ａ내사정")
	LOCAL -= 10
	LOCALS = %LOCALS% - 내사정直後(10)
ENDIF
;미약
IF TEQUIP:미약
	LOCAL += 2
	LOCALS = %LOCALS% + %ITEMNAME:31%(2)
ENDIF
;ビデオ촬영
IF TEQUIP:비디오카메라
	LOCAL -= 30
	LOCALS = %LOCALS% - 촬영중(30)
ENDIF
;야외or관내노출
IF TEQUIP:야외플레이
	LOCAL -= 10
	LOCALS = %LOCALS% - 야외플레이(10)
ELSEIF TEQUIP:관내노출플레이
	LOCAL -= 10
	LOCALS = %LOCALS% - 관내노출(10)
ENDIF

;合計を表示(40이상で実行)
CALL PRINT_COM_ORDER_COM, LOCALS, ARG, LOCAL, 40

