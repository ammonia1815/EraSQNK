@COM99
;식스나인
CALL PRINT_TRAIN_NAME(SELECTCOM)

IF TEQUIP:식스나인
	PRINTL 
	PRINTFORMW \@ TEQUIP:넘어뜨리기 ? %CALLNAME:TARGET% # %CALLNAME:PLAYER% \@は식스나인の体勢をやめた…
	STR:0 = %STR:2%
	TEQUIP:식스나인 = 0
	TEQUIP:PLAYER:식스나인 = 0
	PREVCOM = 99
	RETURN 0
ENDIF
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL SHOW_COM_ORDER_COM, 99

;実行できない
SIF TCVAR:28 == 0
	RETURN 0
;-------------------------------------------------
;実行決定
;-------------------------------------------------
;서로청소플레이
IF CHECK_COM("청소펠라", 91) + CHECK_COM("청소컨닐링구스", 98) && CHECK_COM("청소펠라", 4) + CHECK_COM("청소컨닐링구스", 1)
	IF CHECK_COM("청소펠라", 91)
		TEQUIP:식스나인 = 91
	ELSE
		TEQUIP:식스나인 = 98
	ENDIF
	IF CHECK_COM("청소펠라", 4)
		TEQUIP:PLAYER:식스나인 = 4
	ELSE
		TEQUIP:PLAYER:식스나인 = 1
	ENDIF
ELSE
	;페니스あるならそっち優先
	IF PENIS(PLAYER)
		TEQUIP:식스나인 = 91
	ELSE
		TEQUIP:식스나인 = 98
	ENDIF
	IF PENIS(TARGET)
		TEQUIP:PLAYER:식스나인 = 4
	ELSE
		TEQUIP:PLAYER:식스나인 = 1
	ENDIF
ENDIF

CALL TRAIN_MESSAGE_B

IF REFUSE_CHECK()
	TEQUIP:식스나인 = 0
	TEQUIP:PLAYER:식스나인 = 0
	RETURN 0
ENDIF

STR:0 = %STR:2%

;안면기승ＯＮ
SIF TEQUIP:안면기승 + TEQUIP:넘어뜨리기 && TEQUIP:PLAYER:식스나인 == 1
	TEQUIP:안면기승 = 1
;-------------------------------------------------
;사정체크
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

