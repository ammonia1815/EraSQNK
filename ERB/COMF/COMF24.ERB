@COM24
;칭찬하기
PRINTL 칭찬하기
;-------------------------------------------------
;前回の行動をみる
;-------------------------------------------------
;TFLAG:16 = 1	봉사系を褒めた
;TFLAG:16 = 2	ＳＥＸ系を褒めた
;TFLAG:16 = 3	Ａ・ＳＥＸ系を褒めた
;TFLAG:16 = 4	ＳＭ系を褒めた
;TFLAG:16 = 5	자위を褒めた
;TFLAG:16 = 6	자위以外の羞恥系を褒めた
;TFLAG:16 = 7	ＳＥＸ系を褒めた
;TFLAG:16 = 20	その他を褒めた

;ＳＥＸ系（정상위・후배위・굴곡위・대면좌위・배면좌위・기승위）
IF INSERT("Ｖ인서트") && PENIS(PLAYER)
	TFLAG:16 = 2
;Ａ・ＳＥＸ系（애널섹스・정상위애널・후배위애널・대면좌위애널・배면좌위애널・기승위애널）
ELSEIF INSERT("Ａ인서트") && PENIS(PLAYER)
	TFLAG:16 = 3
;ＳＥＸ系
ELSEIF INSERT("요도인서트") && PENIS(PLAYER)
	TFLAG:16 = 7
ELSEIF PREVUP(9) >= 500
	TFLAG:16 = 4
ENDIF

SELECTCASE PREVCOM
;봉사系
CASE 90 TO 100, 104 TO 119, 140, 150
	TFLAG:16 = 1
;ＳＭ系
CASE 130 TO 132, 137, 141 TO 143, 304, 305
	TFLAG:16 = 4
;羞恥系：자위やくぱぁ
CASE 3, 13, 14, 145
	TFLAG:16 = 5
;羞恥系：관장
CASE 136
	IF TEQUIP:관장 == 0
		TFLAG:16 = 6
	ELSE
		TFLAG:16 = 4
	ENDIF
;浴尿
CASE 138
	IF PREVPLAY("飲尿")
		TFLAG:16 = 1
	;何か違う気がする…
	ELSEIF PREVPLAY("질내욕뇨")
		TFLAG:16 = 4
	ELSEIF PREVPLAY("온천관장")
		TFLAG:16 = 4
	ELSEIF PREVPLAY("유内욕뇨")
		TFLAG:16 = 4
	ENDIF
;羞恥系：방뇨
CASE 144, 149, 505
	TFLAG:16 = 6
ENDSELECT

;前回のTARGETと今回のTARGETが異なる
SIF COND("前Ｔ대상") != TARGET
	TFLAG:16 = 20

;---------------------------------------------------------------------------------------------------
;成功率・失敗率を判定(「달콤한말」からお借りしました)
;---------------------------------------------------------------------------------------------------
;소질でのプラス分は, キャラ購入直後からの基本成功率になります
;새드系のキャラ以外は購入直後から50%前後と高めに설정
;成功率は能力や호감도に依存して上昇
;能力オール3辺りになれば大体どのキャラでも失敗率は0%です
;成功するとTEQUIP:칭찬하기 > 0となり, 失敗すると0. 
;===================================================================================================
LOCAL = CALCF("칭찬하기")

;成否判定
IF LOCAL >= RAND:100
	TEQUIP:칭찬하기 = 2 + LOCAL/50
ELSE
	TEQUIP:칭찬하기 = 0
ENDIF

CALL TRAIN_MESSAGE_B

IF REFUSE_CHECK()
	TEQUIP:칭찬하기 = 0
	RETURN 0
ENDIF

STR:0 = 칭찬하기

RETURN 1

@FLAG_COM24, ARG
;成功
IF TEQUIP:칭찬하기
	;호감도보너스
	SIF ASSIPLAY == 0
		TFLAG:30 += LOCAL:30/30 + 2
	;무드 업
	TFLAG:33 += LOCAL:30/10 + 5
	;羞恥系だと무드は上がりにくい
	SIF TFLAG:16 == 5 || TFLAG:16 == 6
		TFLAG:33 -= 5
ELSE
	;わずかに무드ダウン
	TFLAG:33 -= 5
ENDIF

@SOURCE_COM24, ARG
VARSET LOCAL

LOCAL:20 += 5
LOCAL:21 += 40

;成功
IF TEQUIP:칭찬하기
	LOCAL:30 = CALCF("칭찬하기")
	LOCAL:31 = MIN(LOCAL:30 + RAND:26 , 90) * 8

	LOCAL:3 += LOCAL:31
	LOCAL:16 += LOCAL:31
	LOCAL:11 += LOCAL:31 / 10
	CUP:굴복 += LOCAL:31 / 5
	CUP:치정 += LOCAL:31 / 5
	;羞恥系なら치정が大きく上がる
	SIF TFLAG:16 == 5 || TFLAG:16 == 6
		CUP:치정 += LOCAL:31 * 3
ELSE
	LOCAL:21 += 80
ENDIF

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG



@EQUIP_COM24_2
SIF SELECTCOM == 24
	RETURN 1

PRINTFORML ＜칭찬받아 기분 좋음＞
SIF TEQUIP:칭찬하기 >= 2
	TEQUIP:칭찬하기 -= 1
TEQUIP:칭찬하기커맨드からの派生플래그 = 0

RETURN 1

