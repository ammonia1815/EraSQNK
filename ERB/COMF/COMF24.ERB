SIF ABL:욕망 < 3 || PENIS(PLAYER) == 0 || INSERT("인서트")
	RETURN 0

HOPE_SEX = 0
HOPE_A_SEX = 0
HOPE_N_SEX = 0
HOPE_SEMEN = 0

CALL COM_ABLE60
SIF RESULT && CHECK("Ｖ인서트")
	HOPE_SEX = 1 + ABL:Ｖ감각 + TALENT:음호*10

CALL COM_ABLE71
SIF RESULT && FIRSTCOMS("Ａ삽입") && CHECK("Ａ인서트")
	HOPE_A_SEX = 1 + ABL:Ａ감각 + TALENT:애널광*10

CALL COM_ABLE83
SIF RESULT && FIRSTCOMS("요도삽입") && TALENT:요도광 && CHECK("요도인서트")
	HOPE_N_SEX = 1 + ABL:Ｃ감각/2 + TALENT:요도성감*5 + TALENT:요도광*10

CALL COM_ABLE91
SIF RESULT && FIRSTCOM(91) && ABL:정액중독 >= 2 && !(TALENT:교만 && TALENT:새드)
	HOPE_SEMEN = 1


IF HOPE_SEX && COND("발정기")
	HOPE_A_SEX = 0
	HOPE_N_SEX = 0
