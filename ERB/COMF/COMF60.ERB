		ELSEIF INSERT("Ａ인서트")
			LOCALS = %LOCALS% Ａ인서트중
			EXP:Ａ경험 += 1
			LOCALS:2 = Ａ경험＋１
		ELSEIF INSERT("요도인서트")
			LOCALS = %LOCALS%インサート中
			EXP:요도경험 += 1
			LOCALS:2 = 요도경험＋１

