	LOCAL = 18
CASE 94
	;랄스소년
	LOCAL = 1
CASE 95
	;아들
	LOCAL = 1
CASE 97
	;딸
	LOCAL = 1
CASEELSE
	LOCAL = 1 + CFLAG:ARG:8*4
ENDSELECT

ABL:ARG:ＬＶ = MIN(LOCAL, NUM("최대레벨", 90) )

;주인の場合には경험치も得る
SIF ARG == MASTER
	EXP:ARG:경험치 = EXP_NEXTLV(ARG, ABL:ARG:ＬＶ - 1)


@GET_MAGIC, ARG
SIF ARG <= 0
	RETURN 0

SIF TALENT:ARG:마법사 == 2
	RETURN 0

SIF ABL:ARG:기교 >= RAND:5 + 1
	TALENT:ARG:미니멈 = 1
SIF ABL:ARG:기교 >= RAND:5
	TALENT:ARG:센시브 = 1
SIF ABL:ARG:기교 >= RAND:5
	TALENT:ARG:우르즈군 = 1
SIF ABL:ARG:기교 >= RAND:5 + 1
	TALENT:ARG:시길군 = 1
SIF ABL:ARG:기교 >= RAND:5 + 1
	TALENT:ARG:에올군 = 1

SIF ABL:ARG:기교 >= RAND:5
	TALENT:ARG:로긴 = 1
SIF ABL:ARG:기교 >= RAND:5 + 3
	TALENT:ARG:투와이스 = 1
SIF ABL:ARG:기교 >= RAND:5 + 1
	TALENT:ARG:이사군 = 1
SIF ABL:ARG:기교 >= RAND:5 + 2
	TALENT:ARG:아르스이사 = 1
SIF ABL:ARG:기교 >= RAND:5 + 2
	TALENT:ARG:바인드 = 1
SIF ABL:ARG:기교 >= RAND:5 + 1
	TALENT:ARG:인라지 = 1
SIF ABL:ARG:기교 >= RAND:5 + 3
	TALENT:ARG:비젼 = 1

;最低でもこのどれかは覚えている
IF TALENT:ARG:미니멈 + TALENT:ARG:센시브 + TALENT:ARG:우르즈군 == 0
	IF RAND:3 == 0
		TALENT:ARG:미니멈 = 1
	ELSEIF RAND:2
		TALENT:ARG:센시브 = 1
	ELSE
		TALENT:ARG:우르즈군 = 1
	ENDIF
ENDIF

@SET_3SIZE, ARG
VARSET LOCAL

;体型の変更
IF BASE:ARG:신장
	;신장の変化による소질の変化
	IF TALENT:ARG:요정
		SIF BASE:ARG:신장 > NUM("요정の상한")
			LOCAL = 1
	ELSE
		SELECTCASE BASE:ARG:신장
		;요정では無くなった時
		CASE IS <= NUM("요정の상한")
			LOCAL = 1
		CASE IS <= NUM("小柄の상한")
			SIF TALENT:ARG:장신 || TALENT:ARG:작은몸집 == 0
				LOCAL = 1
		CASE IS <= NUM("보통の상한")
			SIF TALENT:ARG:장신 || TALENT:ARG:작은몸집
				LOCAL = 1
		CASEELSE
			SIF TALENT:ARG:장신 == 0 || TALENT:ARG:작은몸집
				LOCAL = 1
		ENDSELECT
	ENDIF
	IF LOCAL
		SIF CONFIG("３사이즈：표시")
			PRINTFORML （%CALLNAME:ARG%의 신장과 허리둘레 등을 재측정합니다）

		IF TALENT:ARG:요정
			SIF BASE:ARG:신장 > NUM("요정の상한")
				CALL CALC, "요정化", ARG
		ELSE
			SELECTCASE BASE:ARG:신장
			;요정では無くなった時
			CASE IS <= NUM("요정の상한")
				IF TALENT:ARG:장신
					CALL CALC, "장신化", ARG
				ELSEIF TALENT:ARG:작은몸집
					CALL CALC, "小柄化", ARG
				ELSE
					CALL CALC, "보통体型化", ARG
				ENDIF
			CASE IS <= NUM("小柄の상한")
				TALENT:ARG:장신 = 0
				TALENT:ARG:작은몸집 = 1
				CALL CALC, "ＢＷＨ計測", ARG
			CASE IS <= NUM("보통の상한")
				TALENT:ARG:장신 = 0
				TALENT:ARG:작은몸집 = 0
				CALL CALC, "ＢＷＨ計測", ARG
			CASEELSE
				TALENT:ARG:장신 = 1
				TALENT:ARG:작은몸집 = 0
				CALL CALC, "ＢＷＨ計測", ARG
			ENDSELECT
		ENDIF
	ENDIF

	LOCAL = 0
	;라미아の꼬리
	IF TALENT:ARG:라미아 && BASE:ARG:신장의추가요소 == 0
		SIF CONFIG("３사이즈：표시")
			PRINTFORML （%CALLNAME:ARG%의 꼬리 길이를 재겠습니다）
		BASE:ARG:신장의추가요소 = 350 + RAND:100
	ENDIF

	LOCAL = 0

	;바스트컵の変化による소질の変化
	BASE:ARG:컵 = CALCF("바스트컵", CALCF("바스트컵計算値", ARG) )

	IF TALENT:ARG:남자
		SIF BASE:ARG:컵
			CALL CALC, "男乳化", ARG
	ELSEIF TALENT:ARG:거유 >= 3
		SIF BASE:ARG:컵 <= NUM("폭유の상한")
			LOCAL = 1
	ELSEIF TALENT:ARG:거유 == 2
		SIF BASE:ARG:컵 <= NUM("거유の상한")
			LOCAL = 1
	ELSEIF TALENT:ARG:거유 == 1
		SIF BASE:ARG:컵 <= NUM("평유の상한") || BASE:ARG:컵 > NUM("거유の상한")
			LOCAL = 1
	ELSEIF TALENT:ARG:빈유
		SIF BASE:ARG:컵 > NUM("빈유の상한")
			LOCAL = 1
	ELSEIF TALENT:ARG:거유 + TALENT:ARG:빈유 == 0
		SIF BASE:ARG:컵 <= NUM("빈유の상한") || BASE:ARG:컵 > NUM("평유の상한")
			LOCAL = 1
	ENDIF
	IF LOCAL
		SIF CONFIG("３사이즈：표시")
			PRINTFORML （%CALLNAME:ARG%의 바스트 사이즈를 재측정합니다）

		CALL SETFLAG, "바스트설정", ARG

		SIF TALENT:ARG:임신 && CONFIG("３사이즈：표시")
			PRINTFORML （웨이스트 사이즈를 재측정하는 것은, 역시 그만두겠습니다…）
	ENDIF

	;엉덩이のサイズ計測（웨스트と힙から判定するだけなので問題ない）
	CALL SETFLAG, "힙설정", ARG
	RETURN 0
ENDIF

;ゴールデンカノン (바스트 = 신장*0.54, 웨스트 = 신장*0.38, 힙 = 신장*0.54)を参考. 誤差は上下10%程が妥当か. 
;허리놀림LVが高いと, 웨스트が低く, 힙が大きくなりやすい
;BASE:신장=신장, BASE:바스트=바스트, BASE:컵=바스트컵, BASE:웨스트=웨스트, BASE:힙=힙
;요정
IF TALENT:ARG:요정
	CALL CALC, "요정化", ARG
;장신 164～180cm
ELSEIF TALENT:ARG:장신
	CALL CALC, "장신化", ARG
;작은몸집 115～144cm
ELSEIF TALENT:ARG:작은몸집
	CALL CALC, "小柄化", ARG
;보통 145～163cm
ELSE
	CALL CALC, "보통体型化", ARG
ENDIF

;바스트컵計測. 바스트サイズ(BASE:바스트)と컵(BASE:컵)が決められる
CALL SETFLAG, "바스트설정", ARG

;라미아の꼬리
SIF TALENT:ARG:라미아 && BASE:ARG:신장의추가요소 == 0
	BASE:ARG:신장의추가요소 = 350 + RAND:100

