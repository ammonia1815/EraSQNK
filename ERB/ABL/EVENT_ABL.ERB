;-------------------------------------------------
;～놀림など, 特技は調整の簡便さ等の観点から, csvには記述せずに, ここで全て処理することにする. 
;ARGSに소질名, ARGにキャラを代入する(ARG==0でTARGETが居るならARG = TARGET)
;-------------------------------------------------
@SET_EXTALENT(ARGS, ARG)
#FUNCTION

SIF ARG == 0 && TARGET
	ARG = TARGET

VARSET LOCAL

SELECTCASE ARGS
CASE "혀놀림"
	SELECTCASE BASE:ARG:종족
	CASE 6, 17, 35, 46 TO 48, 57
		LOCAL = 3
	CASE 10, 21, 29, 34, 41, 42
		LOCAL = 2
	CASE 1, 13, 22, 37, 43
		LOCAL = 1
	ENDSELECT
CASE "손가락놀림"
	SELECTCASE BASE:ARG:종족
	CASE 18, 48
		LOCAL = 3
	CASE 3, 17, 21
		LOCAL = 2
	CASE 1, 22, 46, 54, 41, 61
		LOCAL = 1
	ENDSELECT
CASE "발놀림"
	SELECTCASE BASE:ARG:종족
	CASE 38, 48
		LOCAL = 3
	CASE 11, 12, 39, 41, 42
		LOCAL = 2
	CASE 1, 14, 17, 21, 31, 40
		LOCAL = 1
	ENDSELECT
CASE "허리놀림"
	SELECTCASE BASE:ARG:종족
	CASE 48
		LOCAL = 3
	CASE 7, 14, 18, 21, 22, 23, 26, 33, 34, 61
		LOCAL = 2
	CASE 1, 6, 29, 30, 31, 35, 42, 43, 46, 47, 50
		LOCAL = 1
	ENDSELECT
CASE "마유"
	SIF TALENT:ARG:남자
		RETURNF 0

	SELECTCASE BASE:ARG:종족
	CASE 29, 34, 48
		LOCAL = 3
	CASE 10, 14
		LOCAL = 2
	CASE 1, 18, 21, 35, 43, 61
		LOCAL = 1
	ENDSELECT
CASE "외견연령"
	;설정済みなら戻る
	SIF TALENT:ARG:외견연령
		RETURNF TALENT:ARG:외견연령

	;유치とか小柄とかそういうのを抜きにした외견연령
	SELECTCASE BASE:ARG:종족
	;例外のある종족…ただしここでのCFLAG:ARG:209による分岐は특수なキャラメイクの時のみ有効
	;通常の소환をするときには無意味なので、初回조교前の구상선택画面での설정も必須となる
	;스킬라
	CASE 15
		IF CFLAG:ARG:209 == 2
			LOCAL = 2
		ELSE
			LOCAL = 3
		ENDIF
	;뱀파이어
	CASE 25
		IF CFLAG:ARG:209 == 3
			LOCAL = 2
		ELSE
			LOCAL = 3
		ENDIF
	;おとな
	CASE 10, 14, 18, 27, 47, 49
		LOCAL = 4
	;아이
	CASE 2, 4, 31, 40
		LOCAL = 1
	;소녀
	CASE 8, 9, 20, 21, 32, 33, 39, 44, 50, 52, 55, 57, 59, 60
		LOCAL = 2
	CASE 98
		LOCAL = TALENT:MASTER:외견연령
	;언니
	CASEELSE
		LOCAL = 3
	ENDSELECT

CASE "흡정체질"
	SIF (TALENT:ARG:몽마 && ABL:ARG:정액중독 >= 5) || (TALENT:ARG:상위몽마 && ABL:ARG:정액중독 >= 3)
		LOCAL += 1
	;악마
	SIF TALENT:ARG:꼬리 == 6 || TALENT:ARG:뿔 == 6 || TALENT:ARG:날개 == 6
		LOCAL += 1
	SIF TALENT:ARG:흡혈귀
		LOCAL += 1
	SIF TALENT:ARG:상위몽마 >= 2
		LOCAL += 1
	SIF TALENT:ARG:마법사 >= 2 && TALENT:ARG:조합지식 && TALENT:ARG:금단의지식
		LOCAL += 1

	;소질関係なく, もって居てしかるべきキャラを判定. 重複しないように注意! 
	SELECTCASE NO:ARG
	CASE 3
		;알라우네
		SIF CFLAG:ARG:8 == 2
			LOCAL += 1
	CASE 8, 9
		;밴시, 알바스타
		SIF CFLAG:ARG:8 == 2 || CFLAG:ARG:8 == 4
			LOCAL += 1
	;아라크네, 루시펠
	CASE 18, 21
		LOCAL += 1
	ENDSELECT

	;정액각인3LV이상
	SIF MARK:ARG:정액각인 >= 3
		LOCAL += 1

CASE "구토반사내성"
	SIF TALENT:ARG:슬라임 || TALENT:ARG:마도인형 || TALENT:ARG:라미아
		RETURNF 1
CASE "확장적정"
	SIF TALENT:ARG:슬라임
		RETURNF 1
ENDSELECT

RETURNF LOCAL


