ELSEIF SHOOT("역레이프") || SHOOT("역애널") || SHOOT("애널봉사")
	PRINTFORMW 
;-------------------------------------------------
;MASTERが조수に사정
;-------------------------------------------------
ELSEIF SHOOT("조수")
	PRINTFORMW 
;-------------------------------------------------
;PLAYERが女で, イってしまった…
;-------------------------------------------------
ELSEIF SHOOT("조교자가여자")
	;強절정
	IF COND("절정", PLAYER) >= 2
		PRINTFORMW 
	;절정
	ELSEIF COND("절정", PLAYER) == 1
		PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
;드래곤に사정された
;-------------------------------------------------
IF TFLAG:9 > 0 && TEQUIP:넘어뜨리기 == 0
	;마조끼Lv3이상+정액중독Lv3이상
	IF ABL:마조끼 >= 3 && ABL:정액중독 >= 3
		PRINTFORMW 
	;마조끼Lv3이상
	ELSEIF ABL:마조끼 >= 3
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
