;※@COUNTER_KOJO関係（97を캐릭터번호に置換）
;カウンター行動をトリガーにした구상
;カウンター行動時に구상を発動します
;なお, ここに書かれているのは범용文章なので, TFLAG:90 == 0 ～ TFLAG:90 == 9においてはCALL COUNTER_TEXT, ARGS でも同じ働きをする. 
;-------------------------------------------------
@COUNTER_KOJO_97_22, ARGS

SELECTCASE ARGS
CASE "카운터없음"
	IF NOWEX:절정수합계
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 황홀로 하고 있다…
	ELSEIF TEQUIP:구속
		IF GETBIT(TEQUIP:구속, 40)
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 밧줄로 구속되어 움직일 수 없다……
		ELSE
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 구속되어 움직일 수 없다……
		ENDIF
	ELSEIF TEQUIP:촉수
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 꿈틀거린다촉수에 희롱해지고 있다……
	ELSEIF TEQUIP:넘어뜨리기
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 위에서 미소를 띄우고 있다……
	ELSE
		CALL COUNTER_TEXT, ARGS
	ENDIF
CASE "Ｖ인서트"
	SELECTCASE CONDS("카운터：強度")
	CASE "必殺"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 질벽을 교묘하게 조종해 %CALLNAME:PLAYER%의 페니스를 조여왔다! 
	;反撃・強
	CASE "強"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 질벽을 조종해 %CALLNAME:PLAYER%의 페니스를 훑어왔다……
	;反撃・中
	CASE "中"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 질벽을 조종해 %CALLNAME:PLAYER%의 페니스를 훑어왔다……
	;反撃・弱
	CASE "弱"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 질벽을 조종해 %CALLNAME:PLAYER%의 페니스를 훑어왔다……
	;反撃・失敗
	CASEELSE
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 격렬한 피스톤 운동에 희롱당하고있다……
	ENDSELECT
CASE "Ａ인서트"
	SELECTCASE CONDS("카운터：強度")
	CASE "必殺"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 민감한 장소를 단번에 괴롭혀왔다! 
	;反撃・強
	CASE "強"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 틈을 봐서 반격해왔다……
	;反撃・中
	CASE "中"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 틈을 봐서 반격해왔다……
	;反撃・弱
	CASE "弱"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 틈을 봐서 반격해왔다……
	;反撃・失敗
	CASEELSE
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 자극에 희롱당하고있다……
	ENDSELECT
CASE "애무"
	SELECTCASE CONDS("카운터：強度")
	CASE "必殺"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 민감한 장소를 단번에 괴롭혀왔다! 
	;反撃・強
	CASE "強"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 틈을 봐서 반격해왔다……
	;反撃・中
	CASE "中"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 틈을 봐서 반격해왔다……
	;反撃・弱
	CASE "弱"
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 틈을 봐서 반격해왔다……
	;反撃・失敗
	CASEELSE
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 자극에 희롱당하고있다……
	ENDSELECT
CASE "봉사"
	IF PENIS(PLAYER)
		SELECTCASE CONDS("카운터：強度")
		CASE "必殺"
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 페니스를 격렬하게 훑어 내고 왔다! 
		;反撃・強
		CASE "強"
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 쉴틈을 주지않고 %CALLNAME:PLAYER%의 페니스를 계속 훑고있다……
		;反撃・中
		CASE "中"
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 쉴틈을 주지않고 %CALLNAME:PLAYER%의 페니스를 계속 훑고있다……
		;反撃・弱
		CASE "弱"
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 쉴틈을 주지않고 %CALLNAME:PLAYER%의 페니스를 계속 훑고있다……
		;反撃・失敗
		CASEELSE
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 페니스에 열중하고 있다……
		ENDSELECT
	ELSE
		SELECTCASE CONDS("카운터：強度")
		CASE "必殺"
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 민감한 장소를 단번에 괴롭혀왔다! 
		;反撃・強
		CASE "強"
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 쉴 틈을 주지않고 %CALLNAME:PLAYER%에게 계속 쾌감을 주고있다……
		;反撃・中
		CASE "中"
