					PRINTFORMW 「%CNAME("一人称")%, %CNAME("二人称")%의가 들어가지 않게 되기 때문에 그것 싫다…」
					PRINTFORMW %타겟을()% 불만인 것 같다…
				ELSE
					PRINTFORMW 「%CNAME("一人称")%, 그것 그다지 좋아하지 않는다…」
					PRINTFORMW %타겟을()% 불만인 것 같다…
				ENDIF
			;それ以外
			ELSE
				PRINTFORMW 「또 그것 해…？」
			ENDIF
		ENDIF
	;애널비즈
	ELSEIF COND("애널비즈")
		;구상を書かれる場合にはRETURN 0 を消してください
		RETURN 0
		;初回の反応
		IF GETBIT(CFLAG:379, 6) == 0
			PRINTFORMW 
			SETBIT CFLAG:379, 6
		ELSE
			PRINTFORMW 
		ENDIF
	;後は大きさで分岐
	ELSE
		SELECTCASE SIZE("Ａ바이브")
		;애널바이브
		CASE 0
			;初回の反応
			IF GETBIT(CFLAG:379, 2) == 0
				PRINTFORMW 
				SETBIT CFLAG:379, 2
			ELSE
				PRINTFORMW 
			ENDIF
		;보통の바이브
		CASE 1
			;初回の反応
			IF GETBIT(CFLAG:379, 3) == 0
				PRINTFORMW 
				SETBIT CFLAG:379, 3
			ELSEIF CHECK("Ａ바이브")
				PRINTFORMW 
			ELSE
				PRINTFORMW 
			ENDIF
		;太い바이브
		CASE 2
			;初回の反応
			IF GETBIT(CFLAG:379, 4) == 0
				PRINTFORMW 
				SETBIT CFLAG:379, 4
			ELSEIF CHECK("Ａ바이브")
				PRINTFORMW 
			ELSE
				PRINTFORMW 
			ENDIF
		;極太이상の바이브
		CASE IS >= 3
			;初回の反応
			IF GETBIT(CFLAG:379, 5) == 0
				IF CHECK("Ａ바이브")
					PRINTFORMW 
				ELSE
					PRINTFORMW 
				ENDIF
				SETBIT CFLAG:379, 5
			ELSEIF CHECK("Ａ바이브")
				PRINTFORMW 
			ELSE
				PRINTFORMW 
			ENDIF
		ENDSELECT
	ENDIF
	RETURN 1
