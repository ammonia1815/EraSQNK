CASE "카운터없음"
	S = SELECTCOM
	D = MAXBASE:절정 - BASE:절정
	;ペニバンでＶ인서트された時には一方的
	IF S >= 60 && S <= 79 && TALENT:PLAYER:후타나리 == 0 && TALENT:PLAYER:남자 == 0
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 격렬한 피스톤 운동에 희롱당하고있다……
	ELSEIF TFLAG:29
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 황홀로 하고 있다…
	ELSEIF TEQUIP:구속
		IF GETBIT(TEQUIP:구속, 40)
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 밧줄로 구속되어 움직일 수 없다……
		ELSE
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 구속되어 움직일 수 없다……
		ENDIF
	ELSEIF TEQUIP:촉수
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 꿈틀거린다촉수에 희롱해지고 있다……
	ELSEIF TEQUIP:넘어뜨리기
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 위에서 미소를 띄우고 있다……
	ELSE
		SIF NOWEX:사정 > 0 || BASE:절정 >= MAXBASE:절정
			RETURN 1
		IF CFLAG:492 >= 8000
		
		ELSEIF CFLAG:492 >= 5000
		
		ELSE
			IF CHECK_CLO("알몸")
				IF SELECTCOM >= 120 && SELECTCOM <= 124
					IF TALENT:동정
						PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 처음의감각에 희롱해지고 있다…
					ELSE
						PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%로부터 주어지는 쾌감에 몸부림 하고 있다…
					ENDIF
				ELSE
					IF D <= 100
						PRINTFORML %CALLNAME:TARGET%의 숨은 난폭하고페니스로부터는 쿠퍼액이 멈추지 않는다…
						PRINTFORMW 입김을 내뿜은 것 뿐이라도 가 버릴 것 같다…
					ELSEIF D <= 1000
						PRINTFORML %CALLNAME:TARGET%의 페니스는 뒤로 젖혀 쿠퍼액으로 젖고 있다…
						PRINTFORMW 갈 것 같은 것이 한눈에 안다…
					ELSEIF D <= 2000
						PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 갈 것 같게 되어 있다…
					ELSEIF D <= 5000 && TALENT:동정 == 0
						PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 얼굴를빨강놈들이라고 있다…
					ELSE
						PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 흠칫흠칫 하고 있다…
					ENDIF
				ENDIF
			ELSEIF EQUIP:웃옷 + EQUIP:보디슈츠 == 0
				IF D <= 100
					PRINTFORML 팬티에 얼룩이 되어있는 것이 분명히 안다…
					PRINTFORMW 팬티를 내린 것 뿐이라도%CALLNAME:TARGET%가 버릴 것 같다…
				ELSEIF D <= 1000
					PRINTFORML 팬티에 얼룩이 되어있는 것 같다…
					PRINTFORMW 조금 더 하면%CALLNAME:TARGET%가 버릴 것이다…
				ELSEIF D <= 2000
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 갈 것 같게 되어 있다…
				ELSEIF D <= 5000 && TALENT:동정 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 얼굴를빨강놈들이라고 있다…
				ELSE
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 흠칫흠칫 하고 있다…
				ENDIF
			ELSE
				IF D <= 100
					PRINTFORML 많이%CALLNAME:TARGET%의 호흡이 거칠다…
					PRINTFORMW 조금 더 하면 가 버릴 것 같다…
				ELSEIF D <= 1000
					PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 안타깝게 몸을 비튼다…
					PRINTFORMW 인내를 할 수 없게 되어 온 것 같다…
				ELSEIF D <= 2000
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 갈 것 같게 되어 있다…
				ELSEIF D <= 5000 && TALENT:동정 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 얼굴를빨강놈들이라고 있다…
				ELSE
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 흠칫흠칫 하고 있다…
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	CALL CRISIS_LARS_2
CASE "Ｖ인서트"
	CALL COUNTER_TEXT, ARGS
CASE "Ａ인서트"
	CALL COUNTER_TEXT, ARGS
CASE "애무"
	CALL COUNTER_TEXT, ARGS
CASE "봉사"
	CALL COUNTER_TEXT, ARGS
CASE "ＳＭ"
	CALL COUNTER_TEXT, ARGS
CASE "황홀"
	PRINTFORMW 
CASE "넘어뜨리기"
	PRINTFORMW 
CASE "스톡사용"
	PRINTFORMW 
CASE "強制종료"
	PRINTFORMW 
CASE "황홀から復帰"
	PRINTFORMW 
CASE "조수が限界"
	PRINTFORMW 
ENDSELECT

RETURN 1

