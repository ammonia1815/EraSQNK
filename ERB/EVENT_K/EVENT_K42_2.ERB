@GUEST_KOJO_K42_2, ARGS, ARG
SELECTCASE ARGS
CASE "조교상대に選ばれた"
	IF RAND:3 == 0
		PRINTFORMW 「응…빨리, 와」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「응……가득, 하자」
	ELSE
		PRINTFORMW 「…%CNAME("一人称")%, 즐거움이다……끓는, 끓는다」
	ENDIF
CASE "조수に抜擢"
	;初めて
	IF EXP:ARG:조수경험 == 0
		PRINTFORMW 「응……좋아…도움, 해 준다」
	ELSE
		PRINTFORM 「또,도움무엇이다…후후,
