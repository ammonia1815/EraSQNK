;-------------------------------------------------
;EVENTTRAIN関係（X1を캐릭터번호に置換）
;조교개시時のセリフ CFLAG 201～210を使用
;-------------------------------------------------
@EVENTTRAIN
SIF NO:TARGET != 59
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

;-------------------------------------------------
;※カウンター行動の플래그ON／OFF　CFLAG:9
;CFLAG:9 = 1 に変更するとOFF
;-------------------------------------------------
;CFLAG:9 = 1

DRAWLINE
;-------------------------------------------------
;初조교時 CFLAG201
;-------------------------------------------------
IF GETBIT(CFLAG:201, 0) == 0
	DRAWLINE
	PRINTFORMW 방에 들어가든지, %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 소매를 이끌어 왔다
	PRINTFORMW 「있지있지! %CNAME("二人称", "かな")%하지만%타겟과()% 『』해!？해!？」
	PRINTFORMW …『몽마한다』란 어떤 의미일까
	PRINTFORMW 「저기요, 『』하면 말이죠, 와~는 해! 아라고 해! 굉장해」
	PRINTFORMW 어딘지 모르게 납득하면서, 팔에 매달린다%조사처리(CALLNAME:TARGET,"를")% 그대로 방의 침대에 옮겼다
	SETBIT CFLAG:201, 0
	RETURN 1

;-------------------------------------------------
;조교개시時（一回のみ） CFLAG201
;-------------------------------------------------
;반발각인Lv3
ELSEIF MARK:반발각인 == 3
	CALL K59_KOJO2_1
;굴복각인Lv1
ELSEIF GETBIT(CFLAG:201, 1) == 0 && MARK:굴복각인 == 1
	PRINTFORMW 「%CNAME("二人称", "かな")%! 『』는 『』가 아니었던 것이구나!」
	PRINTFORMW …？
	PRINTFORMW 「%CNAME("二人称", "かな")%로 하고 있는 것은 『』가 아니고 『나비 오늘』이네요？」
	PRINTFORMW 「그러니까, 좀 더%타겟과()% 『나비 오늘』해♪빠르게 빠르게～읏」
	PRINTFORMW 미묘하게 어긋난 회화에 쓴웃음 지으면서, %조사처리(CALLNAME:PLAYER,"는")% %조사처리(CALLNAME:TARGET,"를")% 안아 올렸다
SETBIT CFLAG:201, 1
	RETURN 1
;굴복각인Lv2
ELSEIF GETBIT(CFLAG:201, 2) == 0 && MARK:굴복각인 == 2
	PRINTFORMW 문을 열면, 거기에 있어야 할%조사처리(CALLNAME:TARGET,"가")% 눈에 띄지 않는다
	PRINTFORMW …도망칠 수 있었는가!？
	PRINTFORMW 순조롭게조교하지만 진행되고 있다고 생각해부주의하고 있었던 것이 화가 되었는지, (와)과 입술을 깨물고 있으면
	PRINTFORMW 「라고 아니」
	PRINTFORMW 배후로 들린 사랑스러운 구령과 동시에, 급격하게 시점이 낙하했다
	PRINTFORMW 「에헤헷, 무릎이나 훈제개─♪있지있지, 깜짝 놀랐다―？」
	PRINTFORMW 「있지있지…에, 엣또, …화냈다？」
	PRINTFORMW …좀 더조교를 어렵고하지 않으면 될 리 없다
	SETBIT CFLAG:201, 2
	RETURN 1
;굴복각인Lv3
ELSEIF GETBIT(CFLAG:201, 3) == 0 && MARK:굴복각인 == 3 && TALENT:연모 == 0
	PRINTFORMW 「아, %CNAME("二人称", "かな")%읏! 오늘도 암돼지에게 두려워하고 히를 부탁 합니다아!」
	PRINTFORMW 「에헤헤─, 『어떤 것―』의 인사야! 책으로 공부했어, 대단하지요」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 근심이 없는 소얼굴로 말하지만, 의미는 모르고 있을 것이다
	PRINTFORMW 「오늘도, 내일도, 가득 『나비 오늘』해!」
	SETBIT CFLAG:201, 3
	RETURN 1
;굴복각인Lv3+연모
ELSEIF GETBIT(CFLAG:201, 4) == 0 && TALENT:연모
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 베개를 안아, 기운이 없게 침대에 앉아 있었다
	PRINTFORMW 「%CNAME("二人称", "かな", "甘え")%…%CALLNAME:TARGET%, 신사 귀댁도 모르는거야」
	PRINTFORMW 「%CNAME("二人称", "かな")%를 생각하면 말이죠, 몸이 이상하게 되어」
	PRINTFORMW 「머리가 와~는 되어, 오가슴먼인가의 아래의 편이 나무라고 되어, 눈물이 나와」
	PRINTFORMW 눈물고인 눈이 되었다%조사처리(CALLNAME:TARGET,"는")% 베개를 꽉 껴안았다
	PRINTFORMW 「기분 좋은데 기분 나쁜거야…저기, 어떻게 하면──무긋」
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %조사처리(CALLNAME:TARGET,"를")% 껴안으면, 입술을 빼앗아혀를 얽어
	PRINTFORMW 「무웃, 푸하아…조금 좋아졌는지도」
	PRINTFORMW 「좀 더 치료해, %CNAME("二人称", "かな")%그렇지 않으면 안돼」
	PRINTFORMW 두 명은 서로의 입술을 탐내면서 침대에 쓰러졌다
	PRINTFORMW 「쭉, 이렇게 하고 있고 싶구나♪」
	SETBIT CFLAG:201, 4
	RETURN 1
;조수の有無を체크（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI <= 0
	CALL K59_KOJO2_1

;구상のある조수が居ない場合は, 通常の二回目以降の구상へ飛ぶ
ELSE
	DRAWLINE
	CALL K59_KOJO2_1
ENDIF

;-------------------------------------------------
;조교개시時（2回目以降, X1を캐릭터번호に変更する） CFLAGなし 
;-------------------------------------------------
@K59_KOJO2_1
;반발각인Lv3
IF MARK:반발각인 == 3
	PRINTFORMW 방에 들어가면%CALLNAME:TARGET%의 모습이 안보인다
	PRINTFORMW …하지만, 침대의 시트가 울창한으로 고조되고 있다
	PRINTFORMW 「%조사처리(CALLNAME:TARGET,"는")% 지금 없어요─」
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 입다물어 시트를 떼어냈다
	RETURN 1
;굴복각인Lv0
ELSEIF MARK:굴복각인 == 0
	PRINTFORMW 「아, %CNAME("二人称", "かな")%(이)다! 오늘은 뭐 해？」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 호기심를 가득 채운 눈동자로%조사처리(CALLNAME:PLAYER,"를")% 응시하고 있다
	RETURN 1
;굴복각인Lv1
ELSEIF MARK:굴복각인 == 1
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 침대에 엎드려 드러누운 채로허니비스킷를 먹고 있다
	PRINTFORMW 발을 탁탁 시켜기분모습이다
	PRINTFORMW 이쪽을 눈치채면, 당황해 정리하기 시작했다
	PRINTFORMW 「아와와, 침대에서 범해 먹어 미안해요…화내지 않았다？」
	RETURN 1
;굴복각인Lv2
ELSEIF MARK:굴복각인 == 2
	PRINTFORMW 「%CNAME("二人称", "かな")%(와)과 나비 오늘♪～～주거지～♪」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 상태 벗어남의 노래를 부르면서%CALLNAME:PLAYER%의 주위를 빙글빙글 돌고 있다
	RETURN 1
;굴복각인Lv3＋연모無し
ELSEIF MARK:굴복각인 == 3 && TALENT:연모 == 0
	PRINTFORMW 「-응♪」
	PRINTFORMW 방에 들어가든지, %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 가슴…다시 말해 배에 뛰어들어 왔다
	PRINTFORMW 「오늘도 가득 간─군요♪」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 기대에 홍조 한 미소를 띄웠다 
	RETURN 1
;굴복각인Lv3+연모
ELSEIF TALENT:연모
	;랜덤で구상が変化する（使わない場合はすべて同じにすればよい）
	IF RAND:3 == 0
		PRINTFORMW 「묻는다♪」
		PRINTFORMW 갑자기 눈앞이 부드럽고 따뜻한 것에 휩싸여졌다
		PRINTFORMW %조사처리(CALLNAME:TARGET,"가")% 침대를 트란포린대신에 해%CALLNAME:PLAYER%의 얼굴면에 달려들어 온 것 같다
		PRINTFORMW 「응～, %CNAME("二人称", "かな")%의 냄새～」
		PRINTFORMW %CALLNAME:PLAYER%의 두정[頭頂]부에얼굴을 갖다댄다%조사처리(CALLNAME:TARGET,"를")% 목말 상태에 고치면, 침대에 향했다
	ELSEIF RAND:2 == 0
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 펫드에 앉는다%CALLNAME:PLAYER%의 위에, 툭 뛰어 올라탔다
		PRINTFORMW 「에헤헤─, %CNAME("二人称", "かな")%♪%CNAME("二人称", "かな")%♪…에헤헤─」
		PRINTFORMW 함께 있을 수 있는 것이 상당히 기쁜 것인지, 때때로발을 탁탁 시키고 있다
		PRINTFORMW 손그래서머리카락를 빗어 주면, 기분 좋은 것 같이 눈을가늘음
		PRINTFORMW 「하후우, %CALLNAME:TARGET%, 굉장한 행복해～♪」
	ELSE
		PRINTFORMW 방에 들어가면%조사처리(CALLNAME:TARGET,"는")%, 기다리고 있었습니다라는 듯이 큰 접시를 탔다검정있고 나니카를 내며 왔다
		PRINTFORMW 「%CALLNAME:TARGET%, 시프리샌드만들었어! 라고 먹어～♪」
		PRINTFORMW 보면 전부 한 근(600g)의 루이스 빵의 갈라진 곳에, 각각2～3센치 있으리라 말하는 치즈와 양파의 츠노키리가 충분히 실어 있었다
		PRINTFORMW 곳곳에 보이는 갈색빛 나는 것은 양파의 껍질일까
		PRINTFORMW 
		PRINTFORMW ──그것은, 시프리샌드이렇게 말하려면 너무 컸다
		PRINTFORMW 크고 두껍고 무겁고 그리고, 너무 대략적이었다
		PRINTFORMW 그것은 확실히 빵 덩어리였다──
		PRINTFORMW 
		PRINTFORMW 「…%CNAME("二人称", "かな")%, 혹시 배 가득？」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")%, 불안한 듯한 눈으로%조사처리(CALLNAME:PLAYER,"를")% 응시하고 있다
		PRINTFORMW 
		PRINTFORMW 맛있게 먹었습니다
	ENDIF
	RETURN 1
ENDIF
RETURN 1

;-------------------------------------------------
;EVENTEND関係（X1を캐릭터번호に置換）
;조교종료時のセリフ CFLAG 211～220を使用
;-------------------------------------------------
@EVENTEND
SIF NO:TARGET != 59
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

DRAWLINE
;-------------------------------------------------
;※정력限界値を超えた場合の終了구상　CFLAG451
;정력0でこちらがダウンしたときはCFLAG451で分岐
;-------------------------------------------------
;조교強制終了時の구상
IF STATE("撃沈", MASTER)
	;반발각인LV3
	IF MARK:반발각인 == 3
		PRINTFORMW 「영차, 영차…에잇」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 움직일 수 없게 되었다%조사처리(CALLNAME:PLAYER,"를")% 방의 밖에 쳐 버렸다
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「옷, %CNAME("二人称", "かな")%괜찮아？오얼굴새하얘？!」
		PRINTFORMW 허풍을 토하려고 했다%CALLNAME:PLAYER%의 시야가 승손에 비스듬하게 기울어 간다
		PRINTFORMW 「%조사처리(CALLNAME:TARGET,"는")% 이제 되니까, 다음은 또 이번으로 하자？」
		PRINTFORMW 그렇게 말해%조사처리(CALLNAME:TARGET,"는")%, 넘어졌다%CALLNAME:PLAYER%의 머리를 자신의 무릎에 실었다
		PRINTFORMW %CALLNAME:TARGET%의 작은손그리고 머리를 어루만져지면서, %조사처리(CALLNAME:PLAYER,"는")% 잠에 떨어져 갔다
		PRINTFORMW 
		PRINTFORMW 「우～…발이 저려 와 버린 케드…%CNAME("二人称", "かな")%를 위해서(때문에) 자제 자제…」
	;その他
	ELSE
		PRINTFORMW 「저것, %CNAME("二人称", "かな")%의 자지 쓸쓸히 해 버렸다」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:PLAYER%의 시든 일물[逸物]을 털거나 빨거나 해 희롱하고 있다
		PRINTFORMW 「그렇게 말하면 책으로, 엉덩이를손가락그리고 동글동글 하면 부활한다고…라는 조원, %CNAME("二人称", "かな")%!？」
		PRINTFORMW %조사처리(CALLNAME:PLAYER,"가")% 하양눈을 부릅뜨고 있는데 눈치, 당황하기 시작했다
		PRINTFORMW 「, 힘들다아! 네─키─에―! 에～키～헤～!!」
	ENDIF
	RETURN 1
;반발각인Lv3
ELSEIF MARK:반발각인 == 3
	PRINTFORMW 「이제 놀아 주지 않는 걸, 이다!」
	RETURN 1
;굴복각인Lv1이하
ELSEIF MARK:굴복각인 <= 1
	PRINTFORMW 「또 다시 보자―♪」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손를 털고 전송하고 있다
	RETURN 1
;굴복각인Lv2
ELSEIF MARK:굴복각인 == 2
	PRINTFORMW 「%CALLNAME:TARGET%, 오빠와 좀 더 함께 있고 싶구나」
	RETURN 1
;굴복각인Lv3+연모無し
ELSEIF MARK:굴복각인 == 3 && TALENT:연모 == 0
	PRINTFORMW 「무─…가서는, 싫다아」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 눈물고인 눈으로 %CALLNAME:PLAYER%의 소매를 집고 있다
	RETURN 1
;연모＋만족
ELSEIF TALENT:연모 && CFLAG:99
	PRINTFORMW 「핫…하앗…스, 굉장한 기분 좋았어요」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 침대에서 일어서려고 했지만, 풀썩 엉덩방아를 붙었다
	PRINTFORMW 「아하핫, 재미있다♪끊으면 무릎이 승손에 여차저차 한다아」 
	RETURN 1
;연모
ELSEIF TALENT:연모
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"가")% 침대에서 나오려고 하면, %조사처리(CALLNAME:TARGET,"가")% 무언으로 등에 매달려 왔다
	PRINTFORMW 떨어질 것 같지도 않기 때문에, %조사처리(CALLNAME:TARGET,"가")% 잘 때까지 곁잠 하기로 했다
	RETURN 1
ENDIF
RETURN 1

;-------------------------------------------------
;@KOJO_MESSAGE_COM関係（X1を캐릭터번호に置換）
;커맨드実行時に出力されます
;-------------------------------------------------
@KOJO_MESSAGE_COM_59_1
;조수が조교した時に구상をスキップする（好みに応じて使う, 行頭の;を消すとスキップするようになる）
;SIF ASSIPLAY
;	RETURN 0
;-------------------------------------------------
;커맨드実行時のセリフ CFLAG 221～270を使用
;-------------------------------------------------
;-------------------------------------------------
;애무 CFLAG221
;-------------------------------------------------
IF SELECTCOM == 0
	;初めて
	IF CFLAG:221 == 0
		;반발각인Lv3
		IF MARK:반발각인 == 3
			PRINTFORMW 「야아…무서운거야…」
			PRINTFORMW 몸을 비틀게 해 피하려고 하는 %조사처리(CALLNAME:TARGET,"를")% 애무했다
		;굴복각인Lv2이상
		ELSEIF MARK:굴복각인 >= 2
			PRINTFORMW 「에헤헤─, %CNAME("二人称", "かな")%의손, 따듯하고 좋아해～」 
		;それ以外
		ELSE
			PRINTFORMW 「꺄하하, %CNAME("二人称", "かな")%낯간지럽다♪」
		ENDIF
		CFLAG:221 = 1
		RETURN 1
	;二回目以降
	ELSE
		;반발각인Lv3
		IF MARK:반발각인 == 3
			PRINTFORMW 「우우…싫어…」
		;연모+굴복각인Lv3
		ELSEIF TALENT:연모
			PRINTFORMW 「햐우우, 젖가슴 기분 있어라」
			PRINTFORMW 「부지런함응도 찌릿찌릿은 해…%CALLNAME:TARGET%, 벌써 모르게 되어 버린다」
		;굴복각인Lv3
		ELSEIF MARK:굴복각인 == 3
			PRINTFORMW 「에헤헤, 굉장해─…%CALLNAME:TARGET%의 젖가슴도 콩(물집)도 딱딱이 되어 있다～」
		;굴복각인Lv2
		ELSEIF MARK:굴복각인 == 2
			PRINTFORMW 「%CNAME("二人称", "かな")%, 좀 더 손대어…%조사처리(CALLNAME:TARGET,"를")% 좀 더 『개발』해♪」
		;それ以外
		ELSEIF MARK:굴복각인 <= 1
			PRINTFORMW 「%CALLNAME:TARGET%, 몸이 따뜻하게 되어 왔어」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;-------------------------------------------------
;컨닐링구스 CFLAG222	CFLAG351(押し倒されているとき)
;석간수 CFLAG328
;-------------------------------------------------
