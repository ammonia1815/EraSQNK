@USE_MAGIC_EST
;입か目が制限されてるとダメ
SIF COND("입의 사용", TARGET) == 0 || COND("視力", TARGET) == 0
	RETURN 0
;정력10이상じゃないとダメ
SIF BASE:정력 < 10
	RETURN 0

;아르카나선택（と영창가능체크）
LOCAL = 0
;센시브
CALL COM_ABLE231_2
SIF RESULT == 1 && RAND:2 == 0
	LOCAL = 1
;우르즈군
CALL COM_ABLE232
SIF LOCAL == 0 && RESULT == 1 && RAND:2 == 0
	LOCAL = 2
;피블
SIF LOCAL == 0 && STATE("피블", PLAYER) == 0 && RAND:4 == 0
	LOCAL = 3
;自分に센시브(연모・음란限定）
SIF LOCAL == 0 && STATE("센시브", TARGET) == 0 && (TALENT:연모 || TALENT:음란) && PLAYER == MASTER && RAND:3 == 0
	LOCAL = 4
;바인드
SIF LOCAL == 0 && TALENT:바인드 && STATE("바인드", PLAYER) == 0 && RAND:4 == 0
	LOCAL = 5
;ラプチャ
SIF LOCAL == 0 && STATE("황홀", PLAYER) == 0 && RAND:6 == 0
	LOCAL = 6
;이사군(低확률）
SIF LOCAL == 0 && TALENT:이사군 && STATE("군", PLAYER) && RAND:5 == 0
	LOCAL = 7

;どの아르카나も선택されなかった場合は1/2でユビキタス・ラブを使用
IF LOCAL == 0 && RAND:2 == 0
	CALL U_LOVE
	RETURN 1
ENDIF

