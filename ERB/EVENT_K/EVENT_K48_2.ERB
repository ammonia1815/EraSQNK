LOCAL = (90 + RAND:11) * (ABL:욕망 * ABL:PLAYER:욕망) + (TALENT:음란 * RAND:2000) + RAND:100 + RAND:10

BASE:MASTER:절정 += LOCAL
;절정게이지増加の一時保存
TCVAR:MASTER:101 += LOCAL
IF BASE:MASTER:절정 >= MAXBASE:MASTER:절정 && PLAYER != MASTER
	LOCAL = PLAYER

	PLAYER = MASTER
	CALL BOUHATSU

	PLAYER = LOCAL
ENDIF

IF ASSI
	;조수の方がMASTERより影響を受けやすい
	LOCAL = (100 + RAND:11) * (ABL:욕망 * ABL:ASSI:욕망) + (TALENT:음란 * RAND:2000) + (TALENT:ASSI:음란 * RAND:1500) + (ABL:ASSI:욕망 * RAND:100) + RAND:100 + RAND:10
	BASE:ASSI:절정 += LOCAL
	TCVAR:ASSI:101 += LOCAL
	IF BASE:ASSI:절정 >= MAXBASE:ASSI:절정 && PLAYER != ASSI
		LOCAL = PLAYER

		PLAYER = ASSI
		CALL BOUHATSU

		PLAYER = LOCAL
	ENDIF
ENDIF
PRINTL 
RETURN 1

