ELSE
	CALL K25_KOJO2_2
ENDIF

RETURN 1

;-------------------------------------------------
;조교開始時이벤트などに날짜限定이벤트がある際に、発生条件を満たしていたらRETURNF 1するようにしてください
;そうするとSHOP画面で～からお話がありますと表示されるようになります
;また、その日の이벤트終了条件があるならRETURNF 0とする（범용セリフがあるなら常にRETURNF 1 で良い）
;ここの条件をもとにCDAYEV(ARGS, ARG)を用いて조교中の날짜이벤트の発生を判定することができる
;-------------------------------------------------
@IS_DAYEV_K25_2(ARGS, ARG)
#FUNCTION

SIF ARG == 0 && TARGET
	ARG = TARGET
SIF ARGS == ""
	ARGS = %TEXTS("날짜対応이벤트")%

;病んでいたらダメ
SIF GETBIT(CFLAG:ARG:201, 30)
	RETURNF 0
;연모でないとダメ
SIF TALENT:ARG:연모 == 0
	RETURNF 0
;상사상애でない場合にはダメな時もある
IF TALENT:ARG:상사상애 == 0
	;寂しがらせていたら一度기분を取ってあげないとダメ
	SIF COND("ご無沙汰", ARG) || COND("조교공백기간", ARG) >= 30
		RETURNF 0
ENDIF

SELECTCASE ARGS
CASE "정월"
	RETURNF 1
CASE "발렌타인"
	RETURNF 1
CASE "발렌타인다음날"

CASE "화이트데이"
	RETURNF 1
CASE "바다의날"
	RETURNF 1
CASE "할로윈"
	RETURNF 1
CASE "크리스마스일주일전"
	SIF CEVENT("날짜：크리스마스일주일전") == 0
		RETURNF 1
CASE "크리스마스"
	RETURNF 1
CASE "크리스마스다음날"
	;訪れた？すっぽかした？
	SIF DAYEV("크리스마스다음날", "現実のみ")
		RETURNF 1
	;크리스마스に訪れている
	SIF CEVENT("날짜：크리스마스", ARG)
		RETURNF 1
CASE "크리스마스주간"

ENDSELECT

RETURNF 0

