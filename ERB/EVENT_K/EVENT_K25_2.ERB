	IF COND("가성포경")
		PRINTFORMW 
	;ずる剥け
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1
CASE "신피약"
	;薬を使用した後の状態で分岐
	IF COND("진성포경")
		PRINTFORMW 
	ELSEIF COND("가성포경")
		PRINTFORMW 
	ENDIF
	RETURN 1
CASE "풍유환"
	;薬を使用した後の状態で分岐
	SELECTCASE TALENT:거유
	;並乳
	CASE 0
		PRINTFORMW 
	;거유
	CASE 1
		PRINTFORMW 
	;폭유
	CASE 2
		PRINTFORMW 
	;초유
	CASE IS >= 3
		PRINTFORMW 
	ENDSELECT
	RETURN 1
CASE "축유환"
	;薬を使用した後の状態で分岐
	SELECTCASE TALENT:거유
	;並乳이하
	CASE 0
		IF TALENT:빈유
			PRINTFORMW 
		;並乳
		ELSE
			PRINTFORMW 
		ENDIF
	;거유
	CASE 1
		PRINTFORMW 
	;폭유
	CASE 2
		PRINTFORMW 
	ENDSELECT
	RETURN 1
CASE "거인의약"
	;薬を使用した後の状態で分岐
	IF TALENT:장신
		;病み時
		IF GETBIT(CFLAG:201, 30)
			PRINTFORMW 「후후후……이 몸이라면,당신를 간단하게 잡아 통그렇게……」
		ELSEIF TALENT:연모
			PRINTFORMW 「역시 이 정도 있던 (분)편이 침착하네요♪」
			PRINTFORMW 「…………저, 저, 큰 여자라든지 말하지 말아 주세요」
		ELSE
			PRINTFORMW 「후후, 이 몸이라면 얼마든지 힘을 낼 수 있어요……」
		ENDIF
	;普通신장
	ELSE
		;病み時
		IF GETBIT(CFLAG:201, 30)
			PRINTFORMW 「후후………작은 여자는 싫다……？」
		ELSEIF TALENT:연모
			PRINTFORMW 「왓……(와)과와 이런 간단하게 키가 자라다니……」
		ELSE
			PRINTFORMW 「이 몸은 3할이라는 곳일까……」
			PRINTFORMW %CALLNAME:TARGET%의손의 히등을 잡으면, 의치와 소리가 났다……
		ENDIF
	ENDIF
	RETURN 1
CASE "소인의약"
	;薬を使用した後の状態で分岐
	IF TALENT:작은몸집
		;病み時
		IF GETBIT(CFLAG:201, 30)
			PRINTFORMW 「후후후……작은 여자가 기호인 것이지요……？」
		ELSEIF TALENT:연모
			PRINTFORMW 「후훗, 이 정도가 사랑스럽고 좋아합니까？」
		ELSE
			PRINTFORMW 「이런 몸으로 해손를 내자고 하는거야？절실히 곤란한 사람이네……」
		ENDIF
	;普通신장
	ELSE
		;病み時
		IF GETBIT(CFLAG:201, 30)
			PRINTFORMW 「후후……큰 것뿐의 여자는 싫다……？」
		ELSEIF TALENT:연모
			PRINTFORMW 「후응……이 정도의 신장도 나쁘지 않네요……♪」
		ELSE
			PRINTFORMW 「이 몸은 3할이라는 곳일까……」
			PRINTFORMW %CALLNAME:TARGET%의손의 히등을 잡으면, 의치와 소리가 났다……
		ENDIF
	ENDIF
	RETURN 1
CASE "무지개의약"
	;薬を使用した後の状態で分岐
	SELECTCASE CONDS("피부색")
	CASE "갈색피부"
		PRINTFORMW 
	CASE "백옥"
		PRINTFORMW 
	CASE "파란피부"
		PRINTFORMW 
	;普通
	CASEELSE
		PRINTFORMW 
	ENDSELECT
	RETURN 1
CASE "풍둔약"
	;薬を使用した後の状態で分岐
	IF TALENT:큰엉덩이 >= 2
		PRINTFORMW 
	ELSEIF TALENT:큰엉덩이
		PRINTFORMW 
	;普通の大きさ
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1
CASE "축둔약"
	;薬を使用した後の状態で分岐
	IF TALENT:큰엉덩이
		PRINTFORMW 
	ELSEIF TALENT:작은엉덩이
		PRINTFORMW 
	;普通の大きさ
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1
