			PRINTFORMW 「이군요…한번 더 %CNAME("二人称")%의 것을 정대%KOJO_K5_HT()%」
			PRINTFORMW 「몇번이나 몇번이나, 나의 질내에 내 버려서…%KOJO_K5_HT()%」
		ELSE
			PRINTFORMW 「아앙…흠뻑 줄줄예요 %KOJO_K5_HT()%」
			PRINTFORMW 「그렇다면은…%CNAME("二人称")%의 것을 정대%KOJO_K5_HT()%」
		ENDIF
		DITEMTYPE:TARGET:60 = 0
		DITEMTYPE:TARGET:61 = 0
		DITEMTYPE:TARGET:62 = 0
		DITEMTYPE:TARGET:63 = 0
		DITEMTYPE:TARGET:64 = 0
		DITEMTYPE:TARGET:65 = 0
		DITEMTYPE:TARGET:66 = 0
		DITEMTYPE:TARGET:67 = 0
		DITEMTYPE:TARGET:68 = 0
	ENDIF
;삽입 중
ELSE
	IF NOWEX:Ｖ절정 >= 2
		IF RAND:3 == 0
			PRINTFORMW 「히아아 %KOJO_K5_HT()% 후아∼~%KOJO_K5_HT()%」
			PRINTFORMW 쾌락의 별로, %타겟은()% 황홀로 하고 있다…
		ELSEIF RAND:2
			PRINTFORMW 「이대로 마음대로 해 %KOJO_K5_HT()%」
		ELSE
			PRINTFORMW 「은―%KOJO_K5_HT()% 하―%KOJO_K5_HT()%」
			PRINTFORMW %타겟은()% 절정의 여운에 잠겨 있다…
		ENDIF
		
		DITEMTYPE:TARGET:0 = 0
		SIF INSERT("Ｖ후배위") == 0
			DITEMTYPE:TARGET:3 = 0
		DITEMTYPE:TARGET:5 = 0
		DITEMTYPE:TARGET:6 = 0
		DITEMTYPE:TARGET:10 = 0
		IF ABL:노출벽 >= 3
			DITEMTYPE:TARGET:3 = 0
		ENDIF
		IF ABL:마조끼 >= 3
			DITEMTYPE:TARGET:130 = 0
		ENDIF
		
		DITEMTYPE:TARGET:60 = 0
		DITEMTYPE:TARGET:61 = 0
		DITEMTYPE:TARGET:62 = 0
		DITEMTYPE:TARGET:63 = 0
		DITEMTYPE:TARGET:64 = 0
		DITEMTYPE:TARGET:65 = 0
		DITEMTYPE:TARGET:66 = 0
		DITEMTYPE:TARGET:67 = 0
		DITEMTYPE:TARGET:68 = 0
		
	ELSE
		;今回の체위を許可
		SIF INSERT("Ｖ정상위")
			DITEMTYPE:TARGET:60 = 0
		SIF INSERT("Ｖ후배위")
			DITEMTYPE:TARGET:61 = 0
		SIF INSERT("Ｖ굴곡위")
			DITEMTYPE:TARGET:62 = 0
		SIF INSERT("Ｖ대면좌위")
			DITEMTYPE:TARGET:63 = 0
		SIF INSERT("Ｖ배면좌위")
			DITEMTYPE:TARGET:64 = 0
		SIF INSERT("Ｖ기승위")
			DITEMTYPE:TARGET:65 = 0
		
		SIF GETBIT(TALENT:선호체위, 0)
			DITEMTYPE:TARGET:60 = 0
		SIF GETBIT(TALENT:선호체위, 1)
			DITEMTYPE:TARGET:61 = 0
		SIF GETBIT(TALENT:선호체위, 2)
			DITEMTYPE:TARGET:62 = 0
		SIF GETBIT(TALENT:선호체위, 3)
			DITEMTYPE:TARGET:63 = 0
		SIF GETBIT(TALENT:선호체위, 4)
			DITEMTYPE:TARGET:64 = 0
		SIF (GETBIT(TALENT:선호체위, 5) && TEQUIP:넘어뜨리기 == 0)
			DITEMTYPE:TARGET:65 = 0
		SIF (GETBIT(TALENT:선호체위, 6) && TEQUIP:넘어뜨리기)
			DITEMTYPE:TARGET:65 = 0
		DITEMTYPE:TARGET:66 = 0
		DITEMTYPE:TARGET:67 = 0
		DITEMTYPE:TARGET:68 = 0

		;기승위おねだり
		IF STATE("황홀",PLAYER)
			;넘어뜨리기
			IF TEQUIP:넘어뜨리기
				IF RAND:2
					PRINTFORMW 「후후 %KOJO_K5_HT()% %CNAME("二人称")%,  사랑해 주는 %KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「%CNAME("二人称")%,  내가 사랑해 줄게%KOJO_K5_HT()%」
				ENDIF
				NEXTCOM = 65
			ELSE
				IF RAND:2
					PRINTFORMW 「%CNAME("二人称", "응석부리고", "かな")%…나, 아래로부터 쳐 올리기라고 가지고 싶은데…」
					PRINTFORMW 「둘이서 함께 움직이면, 매우 기분이 좋다고 생각하는 것…%KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「%CNAME("二人称")%…나, %CNAME("二人称")%에 기분이 좋아져서 가지고 싶은 것…%KOJO_K5_HT()%」
				ENDIF
			ENDIF
			DITEMTYPE:TARGET:65 = 0
		ELSE
			;랜덤値
			IF CONDS("체위") == "정상위" || CONDS("체위") == "굴곡위"
				LOCAL = RANDN(NUM_STR(CSTR:2, "정상위", "후배위", "굴곡위", "대면좌위", "배면좌위", "기승위", "밀어쓰러뜨려기승위") ) + 1
			ELSE
				LOCAL = RANDN(NUM_STR(CSTR:2, "정상위", "후배위", "대면좌위", "배면좌위", "기승위", "밀어쓰러뜨려기승위") ) + 1
			ENDIF
			
			SELECTCASE LOCAL
			;정상위おねだり
			CASE IS <= NUM_STR(CSTR:2, "정상위")
				IF RAND:2
					PRINTFORMW 「%CNAME("二人称", "甘え", "かな")%%KOJO_K5_HT()% 앞에서 부터 안아 %KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「저기…%CNAME("二人称")%%KOJO_K5_HT()% 서방님의 얼굴을 보면서 사랑 하자…%KOJO_K5_HT()%」
				ENDIF
				DITEMTYPE:TARGET:60 = 0
			;후배위おねだり
			CASE IS <= NUM_STR(CSTR:2, "정상위", "후배위")
				;마조끼Lv3이상
				IF ABL:마조끼 >= 3
					IF RAND:2
						PRINTFORMW 「%CNAME("二人称", "甘え", "かな")%, 동물처럼 뒤에서 잡혀 범해줘 %KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「뒤에서 팍팍하면서 범해 줬으면 해…%KOJO_K5_HT()%」
					ENDIF
					IF RAND:2
						PRINTFORMW 「꼼짝 못한 채 당하게 되는 것은, 굉장히 흥분하게 되…%KOJO_K5_HT()%」
					ENDIF
				ELSE
					IF RAND:2
						PRINTFORMW 「뒤에서 꼭 껴안으면서 사랑하자…%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「뒤에서 넣어 줬으면 해…%KOJO_K5_HT()%」
					ENDIF
				ENDIF
				DITEMTYPE:TARGET:61 = 0
			;대면좌위おねだり
			CASE IS <= NUM_STR(CSTR:2, "정상위", "후배위", "대면좌위")
				IF RAND:2
					PRINTFORMW 「키스하면서 하자…%KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「꼭 껴안으면서 하자…%KOJO_K5_HT()%」
				ENDIF
				DITEMTYPE:TARGET:63 = 0
			;배면좌위おねだり
			CASE IS <= NUM_STR(CSTR:2, "정상위", "후배위", "대면좌위", "배면좌위")
				IF RAND:2
					PRINTFORMW 「뒤에서 꼭 껴안으면서 사랑하자 %KOJO_K5_HT()%」
				ELSE
					PRINTFORMW 「남편님의 무릎에 앉아, 여러 부분을 만져지면서 했으면 좋겠어…%KOJO_K5_HT()%」
				ENDIF
				DITEMTYPE:TARGET:64 = 0
			;기승위おねだり
			CASE IS <= NUM_STR(CSTR:2, "정상위", "후배위", "대면좌위", "배면좌위", "기승위", "밀어쓰러뜨려기승위")
				;넘어뜨리기
				IF TEQUIP:넘어뜨리기
					IF RAND:2
						PRINTFORMW 「후후 %KOJO_K5_HT()% %CNAME("二人称")%, 사랑해주겠어 %KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「%CNAME("二人称")%, 내가 사랑해 주겠어%KOJO_K5_HT()%」
					ENDIF
					NEXTCOM = 65
				ELSE
					IF RAND:2
						PRINTFORMW 「%CNAME("二人称", "甘え", "かな")%…나, 아래에서 밀어 올려 줬으면 좋겠어…」
						PRINTFORMW 「둘이서 함께 움직이면, 매우 기분 좋을 것 같다고 생각해…%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「%CNAME("二人称")%…내가, %CNAME("二人称")%에게 기분 좋게 되엇으면 좋겠어…%KOJO_K5_HT()%」
					ENDIF
				ENDIF
				DITEMTYPE:TARGET:65 = 0
			;굴곡위おねだり
			CASEELSE
				;마조끼Lv3이상
				IF ABL:마조끼 >= 3
					IF RAND:2
						PRINTFORMW 「저기…%KOJO_K5_HT()% 이대로 나를 기울여서 범해줘…%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「%CNAME("二人称")%, 이대로 나를 굴곡위 해줘…%KOJO_K5_HT()%」
					ENDIF
					IF RAND:2
						PRINTFORMW 「꼼짝 못한 채 당하게 되는 것은, 굉장히 흥분하게 되…%KOJO_K5_HT()%」
					ENDIF
				ELSE
					IF RAND:2
						PRINTFORMW 「저기…이어져 있는 부분, 부끄러운 부분을 눈앞에 볼 수 있게 해줘…%KOJO_K5_HT()%」
					ELSE
						PRINTFORMW 「음…저기, 이어진 곳을 잘 보이게 해줘…%KOJO_K5_HT()%」
					ENDIF
				ENDIF
				DITEMTYPE:TARGET:62 = 0
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

