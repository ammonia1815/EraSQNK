		DITEMTYPE:TARGET:4 = 0
		DITEMTYPE:TARGET:5 = 0
		DITEMTYPE:TARGET:6 = 0
		DITEMTYPE:TARGET:7 = 0
		DITEMTYPE:TARGET:8 = 0
		DITEMTYPE:TARGET:10 = 0
		DITEMTYPE:TARGET:11 = 0
