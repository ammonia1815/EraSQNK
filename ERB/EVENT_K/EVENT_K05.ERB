@KOJO_RAND_5(ARGS, ARG)
#FUNCTIONS
LOCALS = 
FOR LOCAL, 0, ARG + RAND:2
	SIF ARG == 0
		BREAK
	SELECTCASE ARGS
	CASE "키스", "키스IN"
		SELECTCASE RAND:13
		CASE 9 TO 12
			LOCALS = %LOCALS%%TEXTR("응", "쪽", "쪽")%%TEXTR("음", "～음", "～～음")%%TEXTR(", ", "…", KOJO_K5_HT())%
		CASE 6 TO 8
			LOCALS = %LOCALS%%TEXTR("쥬우웁", "으무읍", "쪽")%%TEXTR(", ", "…", KOJO_K5_HT())%
		CASE 4, 5
			LOCALS:1 = 음%KOJO_K5_HT()%
			LOCALS = %LOCALS%%TEXTR("아음", "하음")%%TEXTR("……", "음…", LOCALS:1)%
		CASE 3
			LOCALS = %LOCALS%으음%TEXTR("음, ", "…", "～음", "～음…")%
		CASE 2
			LOCALS = %LOCALS%므츕～%TEXTR("…", "～음", "～…음")%%KOJO_K5_HT()%%BL(1)%
		CASE 1
			LOCALS = %LOCALS%움%TEXTR("＿", "후", "후후")%음…%KOJO_K5_HT()%%BL(1)%
		CASE 0
			LOCALS = %LOCALS%츄웁%TEXTR("＿", "우", "우우", "우")%っ%TEXTR(", ", "…", KOJO_K5_HT())%
		ENDSELECT
	;ちゅぱ音生成
	CASE "펠라치오", "펠라치오IN"
		SELECTCASE RAND:8
		CASE 7
			LOCALS = %LOCALS%음%TEXTR("음", "음, ", "음…", "쿠읍")%
		CASE 6
			LOCALS = %LOCALS%츕%TEXTR("음", "…", "음, ", "음…")%
		CASE 5
			LOCALS = %LOCALS%츄합%TEXTR("음", "음, ", "음…")%
		CASE 4
			LOCALS = %LOCALS%춥%TEXTR("…", "읍…", "읍읍…", "읍읍읍…", "읍읍읍읍…", "읍읍읍읍읍…")%
		CASE 3
			LOCALS = %LOCALS%쥬릅%TEXTR("…", "음…", "루……", "룹……")%
		CASE 2
			LOCALS = %LOCALS%레로%TEXTR(", ", "……")%
		CASE 1
			LOCALS = %LOCALS%페로%TEXTR("…", "음…")%
		CASE 0
			LOCALS = %LOCALS%%TEXTR("아", "와", "나웁")%む……
		ENDSELECT
	;이라마치오音生成
	CASE "이라마치오", "이라마치오IN"
		SELECTCASE RAND:4
		CASE 3
			LOCALS = %LOCALS%%TEXTR("아무읍", "우무웁", "하우웁")%%TEXTR(", ", KOJO_K5_HT())%
		CASE 2
			LOCALS = %LOCALS%%TEXTR("므우웁", "무푸웁")%%TEXTR(", ", KOJO_K5_HT())%
