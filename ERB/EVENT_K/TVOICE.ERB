;自動喘ぎ関数@TRAINVOICE
;及びその아이関数
;好きな구상ファイルの
;@KOJO_MESSAGE_COM_X1
;の下に
;CALL TRAINVOICE
;RETURN 0
;の二行を加えるだけで簡易喘ぎ声をある程度シチュに応じて自動生成
;현재, 調整不測のためネタ機能の域を出ないと思います
;구상未実装のキャラに自動で喘ぎ声を付けるようにしてもいいですが
;その場合소질を見て性格分岐させたいところ. 요정改良
;--------------------------------------------------
@TRAINVOICE
;Aに喘ぎ回数を入れる処理
;Bに行為레벨をとりあえず0～3で規定

A = 0
B = 0
C = 0

SELECTCASE SELECTCOM
;칭찬하기구상
CASE 24
	CALL KOJO_MESSAGE_COM24
	RETURN 1
;달콤한말
CASE 300
	CALL KOJO_MESSAGE_COM300
	RETURN 1
CASE 7, 31, 33, 75, 138, 139, 140, 145, 149, 230 TO 259, 511
	CALL TRAIN_MESSAGE_C
	RETURN 1
CASEELSE
	;컨피그설정필요
	SIF FLAG:206 != 1
		RETURN 0

	;커맨드ごとの喘ぎ数・基本行為레벨（激しさ数値）を설정
	;애무・性交系は욕정が大きいと喘ぎ数が+3まで増えます
	;애무・性交系
	SELECTCASE SELECTCOM
	;最も軽い　애무・가슴애무・애널핥기・스마타
	CASE 0, 5, 9, 93
		A = 1
		B = 0
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	;軽め　컨닐링구스・로터・손가락넣기・애널애무・펠라한다・거품춤・자위
	CASE 1, 30, 7, 2, 4, 94, 3
		A = 2
		B = 1
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	;ちょい重め　Ｇ스팟자극・전기안마기ー
	CASE 8, 32
		A = 3
		B = 2
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	;本番　섹스系全部
	CASE 60 TO 79
		A = 3
		B = 3
		SIF PALAMLV:2 > 1
			A += 1
		SIF PALAMLV:2 > 2
			A += 1
		SIF PALAMLV:2 > 4
			A += 1
	ENDSELECT
ENDSELECT

;どれくらい感じてるか체크
;ソースに対応した감각が2이상なら1, 4이상なら2. 高い方を優先
IF A
	SIF SOURCE:쾌Ｃ && ABL:Ｃ감각 > 1
		C = 1
	SIF SOURCE:쾌Ｃ && ABL:Ｃ감각 > 3
		C = 2
	SIF SOURCE:쾌Ｖ && ABL:Ｖ감각 > 1 && C == 0
		C = 1
	SIF SOURCE:쾌Ｖ && ABL:Ｖ감각 > 3
		C = 2
	SIF SOURCE:쾌Ａ && ABL:Ａ감각 > 1 && C == 0
		C = 1
	SIF SOURCE:쾌Ａ && ABL:Ａ감각 > 3
		C = 2
	SIF SOURCE:쾌Ｂ && ABL:Ｂ감각 > 1 && C == 0
		C = 1
	SIF SOURCE:쾌Ｂ && ABL:Ｂ감각 > 3
		C = 2
	;感じていない구상へ
	SIF C == 0
		CALL VOICE_B
	;감각4이상だと激しさ+1
	SIF C == 2
		B += 1
	;そして開発済み구상へ
	SIF C > 0
		CALL VOICE_A
ENDIF


;玩具装備・解除系
;クリップ系　클리캡・니플캡
IF SELECTCOM == 34 || SELECTCOM == 35
	A = 1
	B = 1
;重め系　착유기・오나홀・애널비즈
ELSEIF SELECTCOM == 36 || SELECTCOM == 37 || SELECTCOM == 39
	A = 2
	B = 3
;삽입系　바이브・애널바이브
ELSEIF SELECTCOM == 31 || SELECTCOM == 33
	A = 1
	B = 2
ENDIF

;どれくらい感じてるか체크
;ソースに対応した감각が2이상なら1, 4이상なら2. 高い方を優先
IF A
	SIF SOURCE:쾌Ｃ && ABL:Ｃ감각 > 1
		C = 1
	SIF SOURCE:쾌Ｃ && ABL:Ｃ감각 > 3
		C = 2
	SIF SOURCE:쾌Ｖ && ABL:Ｖ감각 > 1 && C == 0
		C = 1
	SIF SOURCE:쾌Ｖ && ABL:Ｖ감각 > 3
		C = 2
	SIF SOURCE:쾌Ａ && ABL:Ａ감각 > 1 && C == 0
		C = 1
	SIF SOURCE:쾌Ａ && ABL:Ａ감각 > 3
		C = 2
	SIF SOURCE:쾌Ｂ && ABL:Ｂ감각 > 1 && C == 0
		C = 1
	SIF SOURCE:쾌Ｂ && ABL:Ｂ감각 > 3
		C = 2
	;感じていない구상へ
	SIF C == 0
		CALL VOICE_B
	;감각4이상だと激しさ+1
	SIF C == 2
		B += 1
	;そして開発済み구상へ
	SIF C > 0
		CALL VOICE_A
ENDIF

;봉사系
;키스한다
IF SELECTCOM == 6
	A = 6
	B = 3
;수음
ELSEIF SELECTCOM == 90
	A = 3
	B = 0
;파이즈리
ELSEIF SELECTCOM == 92
	A = 4
	B = 1
;펠라・식스나인・Ｗ펠라
ELSEIF SELECTCOM == 91 || SELECTCOM == 99
	A = 6
	B = 2
ENDIF
;봉사系구상へ
SIF A
	CALL VOICE_C

RETURN 0

;고통系
;痛いの
IF SELECTCOM == 130 || SELECTCOM == 131 || SELECTCOM == 132
	A = 3
	B = 1
	SIF SELECTCOM == 132
		B += 1
;苦しいの
ELSEIF SELECTCOM == 136 || SELECTCOM == 140
	A = 2
	B = 3
ENDIF
;고통系구상へ
SIF A
	CALL VOICE_D

RETURN 0


