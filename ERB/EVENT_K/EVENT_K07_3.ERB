;-------------------------------------------------
;구상の랜덤生成関数
;함수@TEXTR를 이용해 짧게 했습니다. 예를 들면 PRINTFORM %TEXTR("아", "있고", "")%는
;SELECTCASE RAND:3
;CASE 2
;	PRINTFORM 아
;CASE 1
;	PRINTFORM 있고
;CASE 0
;	PRINTFORM
;ENDSELECT
;と同じです
;また, ARG == 0 とすると前半部分を出力しません
;-------------------------------------------------
@KOJO_RAND_7_3(ARGS, ARG)
#FUNCTIONS
LOCALS = 
FOR LOCAL, 0, ARG + RAND:2
	SIF ARG == 0
		BREAK
	SELECTCASE ARGS
	;ちゅぱ音生成
	CASE "펠라치오", "펠라치오IN"
		SELECTCASE RAND:6
		CASE 5
			LOCALS = %LOCALS%じゅ%TEXTR("る", "るる")%%TEXTR("＿", "っ")%%TEXTR(", ", "…", "……")%
		CASE 4
			LOCALS = %LOCALS%ん%TEXTR("＿", "ぐ", "じゅ", "む")%%TEXTR("＿", "っ")%%TEXTR(", ", "…", "……")%
		CASE 3
			LOCALS = %LOCALS%%TEXTR("ぁむっ", "れろ", "れろれろ", "ちろ, ちろ")%%TEXTR(", ", "…", "……")%
		CASE 2
			LOCALS = %LOCALS%ちゅ%TEXTR("＿", "ぷ", "る", "ぽ", "ぽぉ", "っちゅ")%っ%TEXTR(", ", "…", "……")%
		CASE 1
			LOCALS = %LOCALS%ちゅ%TEXTR("＿", "う", "ぅ", "ぅっ", "うぅっ")%%TEXTR(", ", "…", "……")%
		CASE 0
			LOCALS = %LOCALS%ん%TEXTR("＿", "ちゅ", "ちゅる", "ちゅう", "ちゅぅ")%%TEXTR("＿", "っ")%%TEXTR(", ", "…", "……")%
		ENDSELECT
	ENDSELECT
NEXT
SELECTCASE ARGS
;ちゅぱ音生成
CASE "펠라치오"
	SELECTCASE RAND:4
	CASE 3
		LOCALS = %LOCALS%ぷはぁ%TEXTR("＿", "っ", "…", "……")%%TEXTR("＿", "♪")%
	CASE 2
		LOCALS = %LOCALS%ちゅ%TEXTR("＿", "ぷ", "る", "ぽ", "ぽぉ", "っちゅ")%っ%TEXTR("＿", "…", "♪")%
	CASE 1
		LOCALS = %LOCALS%ちゅ%TEXTR("＿", "う", "ぅ", "ぅっ", "うぅっ")%%TEXTR("＿", "～", "～～")%%TEXTR("＿", "…", "♪")%
	CASE 0
		LOCALS = %LOCALS%ん%TEXTR("＿", "ちゅ", "ちゅる", "ちゅう", "ちゅぅ")%%TEXTR("＿", "～", "～～")%%TEXTR("＿", "っ")%%TEXTR( "…", "♪")%
	ENDSELECT
ENDSELECT
RETURNF LOCALS

