;-------------------------------------------------
;애무 CFLAG221
;-------------------------------------------------
IF SELECTCOM == 0
	;初めて
	IF CFLAG:221 == 0
		;반발각인Lv3
		IF MARK:반발각인 == 3
			PRINTFORMW 
		;굴복각인Lv2이상
