			PRINTFORMW 
		;굴복각인Lv3
		ELSEIF MARK:굴복각인 == 3
			PRINTFORMW 
		;굴복각인Lv2
		ELSEIF MARK:굴복각인 == 2
			PRINTFORMW 
		;それ以外
		ELSEIF MARK:굴복각인 <= 1
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF

