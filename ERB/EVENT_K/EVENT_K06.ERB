;-------------------------------------------------
;네코마타구상 2009_3_6
;製作者　eraSQ製作スレッド>>97
;
;カウンターとか촉수分の구상とかその他いろいろ
;
;…역조교ですか？　デスネ. 
;--------------------------------------------------
;EVENTTRAIN関係
;조교개시時のセリフ CFLAG 201～210を使用
;-------------------------------------------------
@EVENTTRAIN
SIF NO:TARGET != 6
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

;-------------------------------------------------
;※カウンター行動の플래그ON／OFF　CFLAG:9
;CFLAG:9 = 1 に変更するとOFF
;-------------------------------------------------
;CFLAG:9 = 1

;애널봉사の台詞分岐でCFLAG:462を操作しています, 필요ならばその部分を削除してかまいません
;마스터の마조끼の操作, 参照をやめ, CFLAG:461を利用して分岐するように変更しています
;플래그が0の時み, 「쓰러뜨려짐중に조교を終わる」を避ければ上昇を回避できます

;-------------------------------------------------
;初조교時 CFLAG201
;-------------------------------------------------

IF GETBIT(CFLAG:201, 0) == 0
	DRAWLINE
;OriginalString : 「うにゅ, ここはどこかニャ？」
	PRINTFORML 「우뉴, 여기는 어딘가 냐？」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 周りを見回し, こちらに振り向いた
	PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 주위를 둘러봐, 이쪽에 뒤돌아 보았다
;OriginalString : 「にゃふ…ボクを조교한다にゃ？できるかにゃ～？」
	PRINTFORML 「…나를조교한다냐？그리고 올까에～？」
;OriginalString : 「にゃはは, たべちゃうニャ～」
	PRINTFORMW 「냐하하, 먹어 버리는 냐～」
	SETBIT CFLAG:201, 0
	RETURN 1

;-------------------------------------------------
;조교개시時（一回のみ） CFLAG201
;-------------------------------------------------
;반발각인Lv3
ELSEIF MARK:반발각인 == 3
	CALL K6_KOJO2_1

;굴복각인Lv1
ELSEIF GETBIT(CFLAG:201, 1) == 0 && MARK:굴복각인 == 1
	DRAWLINE
;OriginalString : 「にゃは, 来たにゃ？」
	PRINTFORML 「냐하, 래？」
;OriginalString : 「もっとキモチイイことするにゃ～」
	PRINTFORMW 「좀 더 키모치이이 일 하려면～」
	SETBIT CFLAG:201, 1
	RETURN 1

;굴복각인Lv2
ELSEIF GETBIT(CFLAG:201, 2) == 0 && MARK:굴복각인 == 2
	DRAWLINE
;OriginalString : 「にゃは, 待ってたにゃ…」
	PRINTFORML 「냐하, 대…」
;OriginalString : 「もっと…キモチイイことするニャ…♪」
	PRINTFORML 「좀 더…키모치이이 일 하는 냐…♪」
	;새드ッ気Lv2이상
	IF ABL:새드끼 >= 2
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の옷に손をかけ, 脱がそうとしてきた
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%의옷에손를 써라, 벗기려고 해 왔다
	ELSE
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の姿を認めると, 옷を脱ぎ始めた
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%의 모습을 인정하면, 옷를 벗기 시작했다
		CALL SETFLAG, "衣装全脱ぎ", TARGET
	ENDIF
	SETBIT CFLAG:201, 2
	RETURN 1

;굴복각인Lv3
ELSEIF GETBIT(CFLAG:201, 3) == 0 && MARK:굴복각인 == 3 && TALENT:연모 == 0
	DRAWLINE
;OriginalString : 部屋に入ると, %조사처리(CALLNAME:TARGET,"는")% 息を荒くしてこちらを見つめてきた
	PRINTFORML 방에 들어가면, %조사처리(CALLNAME:TARGET,"는")% 숨을 난폭하게 해 이쪽을 응시해 왔다
;OriginalString : 「うにゃあ……はやく……するにゃ…」
	PRINTFORML 「우냐아……빠르게……하려면…」
;OriginalString : 「カラダが熱くて……疼くのにゃ…」
	PRINTFORML 「몸이 뜨거워서……아픔구…」
	;새드ッ気Lv3이상
	IF ABL:새드끼 >= 3
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% ベッドに넘어뜨리기, 抱きしめてきた
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 침대에넘어뜨리기, 껴안아 왔다
	ELSE
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% ベッドに横たわり, 熱く%조사처리(CALLNAME:MASTER,"를")% 見つめている
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 침대에 가로놓여, 뜨겁고%조사처리(CALLNAME:MASTER,"를")% 응시하고 있다
	ENDIF
	SETBIT CFLAG:201, 3
	RETURN 1

;굴복각인Lv3+연모
ELSEIF GETBIT(CFLAG:201, 4) == 0 && TALENT:연모
	DRAWLINE
;OriginalString : 「にゃ……は♪」
	PRINTFORML 「냐……하♪」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 瞳を潤ませてこちらを見つめている
	PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 눈동자를 물기를 띠게 해 이쪽을 응시하고 있다
;OriginalString : 「…ボクと…ずっと, キモチイイことして…」
	PRINTFORML 「…나와…쭉, 키모치이이 일 해…」
;OriginalString : 「いっしょにいるにゃ……♪」
	PRINTFORML 「함께 있으려면……♪」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% そう言うと, ゆっくり%CALLNAME:MASTER%の方へ歩みよって来た
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 그렇게 말하면, 천천히%CALLNAME:MASTER%의 (분)편에 다가왔다
	SETBIT CFLAG:201, 4
	RETURN 1

;조수の有無を체크（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI <= 0
	CALL K6_KOJO2_1

;구상のある조수が居ない場合は, 通常の二回目以降の구상へ飛ぶ
ELSE
	CALL K6_KOJO2_1
ENDIF

;-------------------------------------------------
;조교개시時（2回目以降, @KXXX_KOJO2のXXXを캐릭터번호に変更する） CFLAGなし 
;-------------------------------------------------

@K6_KOJO2_1
;반발각인Lv3
IF MARK:반발각인 == 3
	DRAWLINE
;OriginalString : 「む, 何しに来たニャ？ 無駄だからさっさと帰れにゃ」
	PRINTFORMW 「무, 뭐하러 온 냐？ 쓸데없기 때문에 마을 돌아갈 수 있는 것에」
	RETURN 1
;굴복각인Lv1이하
ELSEIF MARK:굴복각인 <= 1
	DRAWLINE
;OriginalString : 「にゃは…来たかニャ？」
	PRINTFORML 「냐하…왔는지 냐？」
;OriginalString : 「もっと……キモチイイことするにゃ…♪」
	PRINTFORMW 「좀 더……키모치이이 일 하려면…♪」
;굴복각인Lv2이하
ELSEIF MARK:굴복각인 <= 2
	DRAWLINE
;OriginalString : 「にゃは…来たかニャ？」
	PRINTFORML 「냐하…왔는지 냐？」
;OriginalString : 「もっと……キモチイイことするにゃ…♪」
	PRINTFORML 「좀 더……키모치이이 일 하려면…♪」
	;새드ッ気Lv2이상, 역조교進行Lv1이상
	SIF ABL:새드끼 >= 2 && CFLAG:461 >= 1
;OriginalString : 「それとも……ボクにされたいのかニャ～？」
		PRINTFORML 「그렇지 않으면……나로 되고 싶은 것인지 냐～？」
;OriginalString : 「カクゴするにゃ～♪」
	PRINTFORMW 「각오 하려면～♪」
	RETURN 1
;굴복각인Lv3
ELSEIF MARK:굴복각인 == 3 && TALENT:연모 == 0
	DRAWLINE
	P = RAND:15
	;새드ッ気Lv3이상, 역조교進行Lv2이상, ５分の１, または両方Lv4이상で３分の１
	IF ((ABL:새드끼 >= 3 && CFLAG:461 >= 2) && P < 3) || ((ABL:새드끼 >= 4 && CFLAG:461 >= 4) && P < 5)
		CALL SELECT_6_1
	;ねこみみめいど
	ELSEIF CHECK_SHIRT("메이드복")
;OriginalString : 「にゃは, %CNAME("二人称")%, いらっしゃいにゃ～」
		PRINTFORML 「냐하, %CNAME("二人称")%, 등사 있어에～」
;OriginalString : 「何をなさいますにゃ～？にゃはは♪」
		PRINTFORMW 「무엇을 없음 있습니다～？냐하하♪」
	;자위중독Lv3이상で３分の１
	ELSEIF ABL:자위중독 >= 3 && RAND:3 == 0
;OriginalString : 「にゃう…, はぁっ, うにゃっ, にゃ…ふにゃぁ…」
		PRINTFORML 「냐우…, 하앗, 우냣, 냐…후냐아…」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 一人でオナニーに耽っていた
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 혼자서 자위에 빠지고 있었다
;OriginalString : ドアをノックすると, %조사처리(CALLNAME:TARGET,"는")% ゆっくりこちらに振り向いた
		PRINTFORML 문을 노크 하면, %조사처리(CALLNAME:TARGET,"는")% 천천히 이쪽에 뒤돌아 보았다
;OriginalString : 「……にゃはぁ, 来たにゃ……はやくするにゃ…」
		PRINTFORMW 「……냐하아, 래……빨리 하려면…」
	ELSE
		IF RAND:2 == 0
;OriginalString : 「にゃはは, いらっしゃいにゃ～」
			PRINTFORML 「냐하하, 등사 있어에～」
;OriginalString : 「にゃふ……キモチイイことするにゃ…♪」
			PRINTFORML 「……키모치이이 일 하려면…♪」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の姿を認めると, こちらを熱く見つめてきた
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%의 모습을 인정하면, 이쪽을 뜨겁게 응시해 왔다
		ELSE
;OriginalString : 「…にゃは, 今일はナニするにゃ？」
			PRINTFORML 「…냐하, 지금일는 뭐 하려면？」
;OriginalString : 「……それとも……, して欲しいにゃ……？」
			PRINTFORML 「……그렇지 않으면……, 했으면 좋은……？」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 見つめ, 蕩けた笑みを浮かべた
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 응시하고, 녹은 미소를 띄웠다
		ENDIF
	ENDIF
	RETURN 1
;굴복각인Lv3+연모
ELSEIF TALENT:연모
	DRAWLINE
	P = RAND:4
	;새드ッ気Lv3이상, 역조교進行Lv2이상, ４分の１, または両方Lv4이상で２分の１
	IF ((ABL:새드끼 >= 3 && CFLAG:461 >= 2) && P == 0) || ((ABL:새드끼 >= 4 && CFLAG:461 >= 4) && P < 2)
		CALL SELECT_6_1
	;자위중독Lv3이상で３分の１
	ELSEIF ABL:자위중독 >= 3 && RAND:3 == 0
;OriginalString : 「にゃ…, あっ, ……あ, にゃんっ」
		PRINTFORML 「냐…, 앗, ……아, 냐읏」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 一人でオナニーに耽っていた
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 혼자서 자위에 빠지고 있었다
		;ねこみみめいど
		IF CHECK_SHIRT("메이드복")
;OriginalString : %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると, 손を止めて얼굴を伏せた
			PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 방에 들어가면, 손를 멈추어얼굴을 덮었다
;OriginalString : 「……にゃう, えっちなメイドに……お仕置き, してください…にゃ♪」
			PRINTFORMW 「……냐우, 엣치한 메이드에게……벌, 해 주세요…냐♪」
		ELSE
;OriginalString : が, %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると, 素早く飛びついてきた
			PRINTFORML 하지만, %조사처리(CALLNAME:MASTER,"가")% 방에 들어가면, 재빠르게 달려들어 왔다
;OriginalString : 「……にゃは, ボク…寂しかったにゃあ……」
			PRINTFORMW 「……냐하, 나…적 꾸짖은 아……」
		ENDIF
	ELSEIF RAND:3 == 0
;OriginalString : 「ふにゃ～, 来てくれたにゃ～」
		PRINTFORML 「후냐～, 와 구～」
;OriginalString : 「いっしょにキモチよくなるにゃ～♪」
		PRINTFORML 「함께 기분 좋아지려면～♪」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")%, %CALLNAME:MASTER%の体に両손を回し, 抱きしめてきた
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")%, %CALLNAME:MASTER%의 몸에 양손를 돌리기, 껴안아 왔다
	ELSEIF RAND:2 == 0
;OriginalString : 「うにゃ～っ, ちゅ～っ♪」
		PRINTFORML 「우냐～읏, 츄～읏♪」
;OriginalString : %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると, %조사처리(CALLNAME:TARGET,"는")% 突然키스してきた
		PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 방에 들어가면, %조사처리(CALLNAME:TARGET,"는")% 돌연키스해 왔다
;OriginalString : 「にゃはは～, ビックリしたかにゃ～？」
		PRINTFORMW 「냐하하～, 깜짝 혀나～？」
	ELSE
;OriginalString : 「く～………にゃ……ぅ……」
		PRINTFORMW 「쿠～………냐……우……」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 眠っていたが, %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると気付いて起きたようだ
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 자고 있었지만, %조사처리(CALLNAME:MASTER,"가")% 방에 들어가면 깨달아 일어난 것 같다
		;ねこみみめいど
		IF CHECK_SHIRT("메이드복")
;OriginalString : 「ふにゃ…おはよーございますー……？」
			PRINTFORMW 「후냐…안녕하세요―……？」
		ELSE
;OriginalString : 「ふにゃ…おはよー……？」
			PRINTFORMW 「후냐…안녕……？」
		ENDIF
;OriginalString : %조사처리(CALLNAME:MASTER,"는")% 苦笑しつつ, %조사처리(CALLNAME:TARGET,"를")% 抱きしめた
		PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% 쓴웃음하면서, %조사처리(CALLNAME:TARGET,"를")% 껴안았다
	ENDIF
	RETURN 1
ENDIF
RETURN 1

;いきなり押し倒され？
@SELECT_6_1
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%に近寄ると, 妖艶な笑みを浮かべ, 見上げながら尋ねてきた
	PRINTFORML %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%에 접근하면, 요염한 미소를 띄우고, 올려보면서 물어 왔다
	;ねこみみめいど
	IF CHECK_SHIRT("메이드복")
;OriginalString : 「にゃはは, %CNAME("二人称")%…, 犯して欲しいニャ～？」
		PRINTFORML 「냐하하, %CNAME("二人称")%…, 범해 주었으면 하는 냐～？」
	ELSE
;OriginalString : 「にゃは, 犯して欲しいニャ～？」
		PRINTFORML 「냐하, 범해 주었으면 하는 냐～？」
	ENDIF
;OriginalString : [0]はい
PRINTFORML [0]네
;OriginalString : [1]いいえ
PRINTFORML [1]아니오
CALL INPUT_SELECT, 2
IF RESULT == 0
;OriginalString : 「にゃはは…犯してアゲルにゃ～♪」
	PRINTFORMW 「냐하하…범해 아겔에는～♪」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기た! 
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기! 
	TEQUIP:넘어뜨리기 = 1
	IF CFLAG:461 < 4
		CALL K6_UP_F461
;		PRINTFORMW %CALLNAME:MASTER%の마조끼が{ABL:MASTER:마조끼}になった
	ENDIF	
ELSEIF RESULT == 1
	;새드ッ気Lv4이상, 역조교進行Lv4이상, ３分の２
	IF ((ABL:새드끼 >= 4 && CFLAG:461 >= 4) && RAND:3 > 0) || CFLAG:461 >= 6
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 嗜虐的な笑みを浮かべながら%조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기, 上から見下ろしてきた
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 기학적인 미소를 띄우면서%조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기, 위로부터 내려다 봐 왔다
;OriginalString : 「にゃはは…嘘ついてもわかるニャ♪」
		PRINTFORMW 「냐하하…거짓말 해도 아는 냐♪」
		;めいどか본디지
		IF CHECK_SHIRT("메이드복") || CHECK_BODYS("본디지")
;OriginalString : 「おしおきにゃ～♪」
			PRINTFORMW 「사형～♪」
		ELSE
;OriginalString : 「ホントはボクに犯して欲しいにゃ～♪」
			PRINTFORMW 「사실은 나에게 범해 바라 있어～♪」
		ENDIF
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기た! 
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기! 
		TEQUIP:넘어뜨리기 = 1
	ELSE
;OriginalString : 「にゃう……ほんとに～？」
		PRINTFORMW 「냐우……정말～？」
;OriginalString : %조사처리(CALLNAME:TARGET,"는")% 残念そうに言った
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 유감스러운 듯이 말했다
	ENDIF
ENDIF

RETURN 1

;-------------------------------------------------
;EVENTEND関係
;조교종료時のセリフ CFLAG 211～220を使用
;-------------------------------------------------
@EVENTEND
SIF NO:TARGET != 6
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

