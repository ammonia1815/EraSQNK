				CALL SET_CEVENT, "二人称주인かな：ごしゅじん"
			CASE 2
				CALL SET_CEVENT, "二人称주인：CALLNAME_MASTER"
				CALL SET_CEVENT, "二人称주인かな：CALLNAME_MASTER"
