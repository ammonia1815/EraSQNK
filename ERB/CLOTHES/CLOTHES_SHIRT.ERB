;-------------------------------------------------
;지시されたとおりの셔츠の플래그を설정するために使う
;-------------------------------------------------
@SETFLAG_SHIRT(ARGS:1, ARGS:2, ARGS:3, ARGS:4, ARGS:5, ARGS:6, ARGS:7, ARGS:8, ARGS:9, ARGS:10)
#FUNCTION
#DIM LCOUNT

LOCAL = 0
FOR LCOUNT, 1, 11
	ARGS = %ARGS:LCOUNT%
	SELECTCASE ARGS
	CASE "Ｙ셔츠"
		SETBIT LOCAL, 0
	CASE "여교사", "슈츠"
		SETBIT LOCAL, 1
	CASE "기모노", "일본전통옷"
		SETBIT LOCAL, 2
	CASE "메이드", "메이드복"
		SETBIT LOCAL, 3
	CASE "체조복"
		SETBIT LOCAL, 4
	CASE "간호사", "ナース", "간호사복"
		SETBIT LOCAL, 5
	CASE "차이나", "차이나드레스"
		SETBIT LOCAL, 6
		LOCAL |= SETFLAG_COMMON("원피스")
	CASE "세라", "세라복"
		SETBIT LOCAL, 7
	CASE "무녀", "무녀복"
		SETBIT LOCAL, 8
	CASE "드레스"
		SETBIT LOCAL, 9
	CASE "웨딩", "웨딩드레스"
		SETBIT LOCAL, 10
		SETBIT LOCAL, 9
	CASE "시스터", "수도복", "수녀복"
		SETBIT LOCAL, 11
	CASE "천조각"
		SETBIT LOCAL, 12
	CASE "뷔스티에"
		SETBIT LOCAL, 13
	CASE "베이비돌"
		SETBIT LOCAL, 14
	CASE "스커트着用"
		SETBIT LOCAL, 15
	CASE "웨이트리스"
		SETBIT LOCAL, 16
	CASE "고스로리드레스"
		SETBIT LOCAL, 17
	CASE "치어리더"
		SETBIT LOCAL, 18
	CASE "파자마"
		SETBIT LOCAL, 19
	CASEELSE
		SIF STRLENS(ARGS)
			LOCAL |= SETFLAG_COMMON(ARGS)
	ENDSELECT
	SIF STRLENS(ARGS:(LCOUNT + 1) ) == 0
		RETURNF LOCAL
NEXT
RETURNF LOCAL

;-------------------------------------------------
;ARGの셔츠が条件ARGSを満たすか確認する
;-------------------------------------------------
@CHECK_SHIRT(ARGS, ARG)
#FUNCTION
SIF ARG == 0 && TARGET
	ARG = TARGET
SIF EQUIP:ARG:웃옷 == 0
	RETURNF 0
IF ARGS == "はだけ可"
	SIF SETFLAG_SHIRT("はだけた")&EQUIP:ARG:웃옷
		RETURNF 0
	SIF SETFLAG_SHIRT("はだけ불가")&EQUIP:ARG:웃옷
		RETURNF 0
	RETURNF 1
ELSEIF (SETFLAG_SHIRT(ARGS)&EQUIP:ARG:웃옷) == SETFLAG_SHIRT(ARGS)
	RETURNF 1
ENDIF
RETURNF 0

