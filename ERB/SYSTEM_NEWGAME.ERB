	IF IS_SELECT:CHOICE
		;ポイントに変えたLVを戻す
		IF GETBIT(FLAG:33, CHOICE - 40)
			CLEARBIT FLAG:33, CHOICE - 40
			ARG -= IS_SELECT:CHOICE
			FLAG:255 += IS_SELECT:CHOICE * 2
		;LVをポイントに変換
		ELSE
			SETBIT FLAG:33, CHOICE - 40
			ARG += IS_SELECT:CHOICE
			FLAG:255 -= IS_SELECT:CHOICE * 2
		ENDIF
	ENDIF

