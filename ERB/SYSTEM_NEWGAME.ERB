			CLEARBIT FLAG:33, CHOICE
			ARG += IS_SELECT:CHOICE
		;ボーナス獲得処理
		ELSEIF ARG >= IS_SELECT:CHOICE
			SETBIT FLAG:33, CHOICE
			ARG -= IS_SELECT:CHOICE

			;相反する소질を獲得した場合の処理
			SELECTCASE CHOICE
			;조루と지루
			CASE 7
				IF GETBIT(FLAG:33, 8)
					CLEARBIT FLAG:33, 8
					ARG += IS_SELECT:8
				ENDIF
			CASE 8
				IF GETBIT(FLAG:33, 7)
					CLEARBIT FLAG:33, 7
					ARG += IS_SELECT:7
				ENDIF
