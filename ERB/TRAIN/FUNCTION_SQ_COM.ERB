	;使用者がMASTERのみ
	ELSEIF GETBIT(MAGIC_USER(ARG, "커맨드"), 1)
		LOCALS = %TALENT_NAME(ARG, TALENT:MASTER:ARG)%
	ENDIF

CASE 260
	SIF CHECK_COM("로션마사지", ARG)
		LOCALS = 로션尻애무

CASE 280
	SIF TEQUIP:PLAYER:구속 || STATE("바인드", PLAYER)
