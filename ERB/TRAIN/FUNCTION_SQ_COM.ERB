IF EQUIP:겉옷 && CHECK_OUTER("Ｖ노출") == 0
	;通気性なし(濡れないけど, 蒸れやすい)
	IF CHECK_OUTER("通気性없음")
		SIF LOCAL >= 1 || (NUM("턴수") == 0 && PALAM:Ｖ윤 >= 50)
			STAIN:겉옷 = SET_STAIN(STAIN:겉옷, "蒸れ")
	;直穿き
