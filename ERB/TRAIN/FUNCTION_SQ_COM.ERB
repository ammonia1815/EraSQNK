;자위한다
CASE 282
	SELECTCASE PREVCOM
	CASE 3
		LOCALS = 相互자위
	CASE 1, 4, 9, 13, 14
	CASEELSE
		SIF TEQUIP:PLAYER:자위한다 == 3
			LOCALS = 相互자위
	ENDSELECT

CASE 301
	SIF TEQUIP:손대중
		LOCALS = 손대중을멈춘다
CASE 302
	SIF TEQUIP:연주중
