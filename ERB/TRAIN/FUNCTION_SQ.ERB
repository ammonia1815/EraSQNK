@INPUT_SCROLL, ARGS, ARG
#DIM LCOUNT

SIF ARG == 0
	ARG = NUM("折り返し文字数")

IF STRLENS(ARGS) <= ARG
	PRINTFORML %ARGS%
	INPUT
	RETURN RESULT
ENDIF

REDRAW 0

VARSET LOCALS

FOR LCOUNT, 0, STRLENS(ARGS)
	LOCALS:(LCOUNT%2) = %SUBSTRING(ARGS, LCOUNT*2)%%ARGS%
	LOCALS:(LCOUNT%2) = %SUBSTRING(LOCALS:(LCOUNT%2), 0, ARG)%

	SIF LOCALS == LOCALS:1
		CONTINUE

	PRINTFORM %LOCALS:(LCOUNT%2)%

	TINPUT 110, 9999, 0, ""

	SELECTCASE RESULT
	CASE 9999
		CLEARLINE 2
	CASEELSE
		REDRAW 1
		RETURN RESULT
	ENDSELECT
NEXT
