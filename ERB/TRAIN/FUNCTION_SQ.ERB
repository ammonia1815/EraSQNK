			RETURNF 1
		ELSEIF GETBIT(TALENT:ARG:선호체위, 23) && INSERT("대면좌위", ARG)
			RETURNF 1
		ELSEIF GETBIT(TALENT:ARG:선호체위, 24) && INSERT("배면좌위", ARG)
