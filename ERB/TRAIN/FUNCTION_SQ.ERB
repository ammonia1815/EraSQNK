;Emueraの命令SWAPCHARAと異なりTARGETとASSIを保つ
;-------------------------------------------------
@SWAP_CHARA, ARG, ARG:1
;TARGETとASSIの番号調整
SELECTCASE TARGET
CASE 0
CASE ARG
	TARGET = ARG:1
CASE ARG:1
	TARGET = ARG
ENDSELECT
SELECTCASE ASSI
CASE 0
CASE ARG
	ASSI = ARG:1
CASE ARG:1
	ASSI = ARG
ENDSELECT

