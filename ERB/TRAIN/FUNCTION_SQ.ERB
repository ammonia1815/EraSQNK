	SIF TALENT:ARG:동물다리 == 2 || TALENT:ARG:동물다리 == 5 || TALENT:ARG:동물다리 == 8 || TALENT:ARG:동물다리 == 11
		RETURNF 0
	RETURNF 1
CASE "뿔ハンドル"
	;악마の角だと掴み易い
	SIF TALENT:ARG:뿔 == 6
