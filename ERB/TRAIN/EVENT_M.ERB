;럭셔리어스히트と황홀判定
;ARGSは今のところ"커맨드"と"정액"がある
;PLAYERかTARGETかどうかで別物だけどここで一括
;-------------------------------------------------
@LUXURIOUS_HIT, ARGS, ARG
#DIM LCOUNT
#DIM WEEKNESS

SIF ARG == 0 && TARGET
	ARG = TARGET

WEEKNESS = 0
VARSET LOCAL

SELECTCASE ARGS
CASE "커맨드"
	SELECTCASE ARG
	CASE PLAYER
		;今のところ플레이ヤーで判定が起きるのは주인だけ
		SIF PLAYER != MASTER
			RETURN 0

		;성감대の処理
		SELECTCASE SELECTCOM
		;손에약함
		CASE 90
			SETBIT WEEKNESS, 0
		CASE 97
			SETBIT WEEKNESS, 0
			SIF TALENT:남자 == 0
				SETBIT WEEKNESS, 4
		;입에약함
		CASE 91, 96, 98, 99, 140
			SETBIT WEEKNESS, 1
		;가슴에약함
		CASE 5, 81, 92, 104
			SETBIT WEEKNESS, 2
		;다리에약함
		CASE 95
			SETBIT WEEKNESS, 3
		;♀에약함
		CASE 1
			SIF TEQUIP:넘어뜨리기 || TEQUIP:안면기승 || TEQUIP:식스나인
				SETBIT WEEKNESS, 4
		CASE 13, 65, 99
			SETBIT WEEKNESS, 4
		;키스에약함
		CASE 6
			SIF NUM("턴수")
				SETBIT WEEKNESS, 5
		CASE 63, 73
			SIF CHECK_COM("키스") && TALENT:연모
				SETBIT WEEKNESS, 5
		;애널에약함
		CASE 9
			SIF TEQUIP:넘어뜨리기 || TEQUIP:안면기승 || TEQUIP:식스나인
				SETBIT WEEKNESS, 6
		CASE 14, 75
			SETBIT WEEKNESS, 6
		;♂에약함
		CASE 4
			SIF TEQUIP:넘어뜨리기 || TEQUIP:안면기승 || TEQUIP:식스나인
				SETBIT WEEKNESS, 7
		CASE 120 TO 129
			SETBIT WEEKNESS, 7
		ENDSELECT

		;자위中
		SELECTCASE TEQUIP:PLAYER:자위한다
		;컨닐링구스
		CASE 1
			SETBIT WEEKNESS, 4
		;자위
		CASE 3

		;펠라한다
		CASE 4
			SETBIT WEEKNESS, 7
		;애널핥기
		CASE 9
			SETBIT WEEKNESS, 6
		;조개벌리기
		CASE 13
			SETBIT WEEKNESS, 4
		;Ａ조개벌리기
		CASE 14
			SETBIT WEEKNESS, 6
		ENDSELECT

		LOCAL = TCVAR:PLAYER:101

		VARSET LOCALS
		FOR LCOUNT, 0, NUM("성감대")
			SIF GETBIT(TALENT:MASTER:성감대, LCOUNT) == 0 || GETBIT(WEEKNESS, LCOUNT) == 0
				CONTINUE
			LOCAL:1 += 1
			SIF RAND:(LOCAL:1)
				CONTINUE
			LOCALS = %NAMES("성감대", LCOUNT)%
			LOCAL += 1000 * LOCAL:1
		NEXT

		SIF LOCALS == ""
			RETURN 0

		;모유を飲んでいると황홀度合い上昇
		LOCAL = MULTIPLY(LOCAL, 100 + EXP_GAIN(74, PLAYER)*10)

		;성벽合致度
		FOR LCOUNT, 0, NUM("속성")
			SIF GETBIT(BASE:속성, LCOUNT) && GETBIT(TALENT:PLAYER:성벽, LCOUNT)
				LOCAL += LOCAL*20/100 + 1000
		NEXT
		;마음에듬or상사상애
		IF PLAYER == MASTER
			IF TALENT:상사상애
				LOCAL += LOCAL*30/100 + 2000
			ELSEIF TALENT:마음에듬
				LOCAL += LOCAL*20/100 + 1000
			ENDIF
		ENDIF

		SIF LOCAL < 1000 || LOCAL + BASE:PLAYER:절정 < MAX(MAXBASE:PLAYER:절정/4, 5000) || RAND:3
			RETURN 0

		SETCOLOR DEF_COLOR("하트핑크")

		PRINTFORML ＜럭셔리어스 히트! （%CALLNAME:PLAYER%）＞
		PRINTFORML %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%에게 빠져버리고 말았다! 

		NOWEX:PLAYER:럭셔리어스히트 = 1
		EX:PLAYER:럭셔리어스히트 += 1
		BASE:PLAYER:절정 += LOCAL
		TCVAR:PLAYER:101 += LOCAL

		IF LOCAL >= MAX(MAXBASE:PLAYER:절정/3, 5000) && RAND:2 == 0
			PRINTFORML %조사처리(CALLNAME:PLAYER,"는")% 황홀한 기분이 되어버렸다……
			TEQUIP:넘어뜨리기 = 1
			CALL ADD_STATE, PLAYER, "황홀"
			FORCEWAIT
		ENDIF
		RESETCOLOR
	CASE TARGET
		;성감대等を用意してから설정する（PALAM:연속절정COMBO수でも判定）
		;まずは성감대を突かれたかで判定
		;GET_PALAMLV(ARG)の値は、順に1:100,2:500,3:3000,4:1万,5:3万,6:6万,7:10万,8:15万,9:25万

		;快感での범용判定
		SIF TALENT:Ｃ민감 || TALENT:음핵
			LOCAL = MAX(GET_PALAMLV(CUP:쾌Ｃ), LOCAL)
		SIF TALENT:Ｖ민감 || TALENT:음호
			LOCAL = MAX(GET_PALAMLV(CUP:쾌Ｖ), LOCAL)
		SIF TALENT:Ａ민감 || TALENT:애널광
