;-------------------------------------------------
;快楽조교関数
;몸을맡김からの역조교で使用
;봉사系メイン
;-------------------------------------------------
@EVENT_EXPLAY
PRINTFORML 
;조교メニュー일람
VARSET LOCALS
LOCAL = ABL:기교 * 1000 + MAX(ABL:ＬＶ - ABL:PLAYER:ＬＶ, 0) * 1000 + ABL:ＬＶ * 500 + (MARK:PLAYER:쾌락각인 + TALENT:PLAYER:음란) * 1500

TRYCALLFORM SET_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}
LOCALS = %STR:100%

SELECTCASE LOCALS
CASE "손가락挿れ", "바이브責め", "자위"
	LOCAL += ABL:손가락놀림 * 1000 + EXP:봉사쾌락경험 + RAND:10
	EXP:수음경험 += ABL:기교/2 + 1
CASE "快楽레이프"
	LOCAL += ABL:허리놀림 * 1000 + EXP:봉사쾌락경험 + RAND:10
	EXP:Ｖ삽입경험 += ABL:기교/2 + 1
	EXP:PLAYER:Ｖ성교경험 += ABL:TARGET:기교/2 + 1
	EXP:PLAYER:Ｖ경험 += ABL:TARGET:기교/2 + 1
CASE "컨닐링구스"
	LOCAL += ABL:혀놀림 * 1000 + EXP:봉사쾌락경험 + RAND:10
	EXP:커닐링구스경험 += ABL:기교/2 + 1
CASE "펠라치오"
	LOCAL += ABL:혀놀림 * 1000 + EXP:봉사쾌락경험 + RAND:100 + RAND:10
	EXP:펠라경험 += ABL:기교/2 + 1
CASE "파이즈리"
	LOCAL += (ABL:혀놀림 + ABL:마유) * 1000 +  EXP:봉사쾌락경험 + RAND:10
	EXP:파이즈리경험 += ABL:기교/2 + 1
CASE "수음", "오나홀훑기"
	LOCAL += ABL:손가락놀림 * 1000 + EXP:봉사쾌락경험 + RAND:10
	EXP:수음경험 += ABL:기교/2 + 1
CASE "키스"
	LOCAL += ABL:혀놀림 * 1000 + EXP:봉사쾌락경험 + RAND:10
	EXP:키스경험 += ABL:기교/2 + 1
	EXP:PLAYER:키스경험 += ABL:기교/2 + 1
CASE "가슴애무", "搾乳플레이"
	LOCAL += (ABL:손가락놀림 + ABL:마유) * 1000 + EXP:봉사쾌락경험 + RAND:10
	EXP:수음경험 += ABL:기교/2 + 1
CASE "꼬리페니스자극", "꼬리責め"
	LOCAL += ABL:기교 * 1000 + MIN(EXP:봉사쾌락경험 + CFLAG:2, MAXBASE:PLAYER:절정) + RAND:1000
	LOCAL *= 9
	EXP:가학쾌락경험 += ABL:기교*9
ENDSELECT
;その他経験
IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
	EXP:레즈경험 += RAND:5 + 1
	EXP:PLAYER:레즈경험 += RAND:5 + 1
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
	EXP:ＢＬ경험 += RAND:5 + 1
	EXP:PLAYER:ＢＬ경험 += RAND:5 + 1
ENDIF
EXP:가학쾌락경험 += ABL:기교

TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS

LOCAL = MULTIPLY(CALCF("基礎보정", LOCAL), CALCF("快楽보정", 100) )

CALL TEXT, "절정計算", LOCAL
;절정できなかった
IF CALCF("절정", PLAYER) == 0
	RETURN 0
;절정した
ELSE
	LOCALS:1 = 절정：%LOCALS%
	TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS:1

	CALL BOUHATSU
	;追撃判定
	;残정력20이하だと一定확률で追撃, それ이상だとより低확률で追撃. それとは別に스톡でも判定
	IF (BASE:PLAYER:정력 <= 20 && RAND:2) || (BASE:PLAYER:정력 > 20 && RAND:4 == 0) || (BASE:PLAYER:스톡 && RAND:5 == 0)
		IF BASE:PLAYER:스톡 <= 0 && BASE:PLAYER:정력 <= 0
			PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 이미 한계지만, %조사처리(CALLNAME:TARGET,"는")% 그 손을 풀지 않는다! 
		ELSEIF BASE:PLAYER:정력 <= 0
			PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 정력을 남김없이 %CALLNAME:TARGET%에가 바쳤다! 
		ELSE
			PRINTFORMW %CALLNAME:PLAYER%의 정력잔량 …… {BASE:PLAYER:정력}! 
		ENDIF
		PRINTFORML 

		LOCALS:1 = 追撃：%LOCALS%
		TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS:1

		;追撃された場合は必殺
		LOCAL = 100000 + EXP:봉사쾌락경험 + RAND:10000
		CALL TEXT, "절정計算", LOCAL
		CALL BOUHATSU

		;余力がある場合はさらに追撃判定(CALL BOUHATUなどはこの台詞の中で行う)
		IF BASE:PLAYER:정력 >= 10
			LOCALS:1 = ＥＸ追撃：%LOCALS%
			TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS:1
		ENDIF

		IF BASE:PLAYER:정력 <= 0 && BASE:PLAYER:스톡 == 0 
			IF PENIS(PLAYER) && RAND:4 == 0
				PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 완전히 바닥을 드러낼 때까지 %CALLNAME:TARGET%에게 빨아먹혔다! 
			ELSEIF RAND:3 == 0
				PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 정력을 송두리째 %CALLNAME:TARGET%에게 짜여져버렸다! 
			ELSEIF RAND:2 == 0
				PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 모든 정력을 %CALLNAME:TARGET%에게 맛있게 먹히고 말았다! 
			ELSE
				PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 완전히 새하얗게 불타버렸다! 
			ENDIF
		ELSEIF BASE:PLAYER:정력 <= 0
			PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 정력을 남김없이 %CALLNAME:TARGET%에게 바쳤다! 
		ELSE
			PRINTFORMW %CALLNAME:PLAYER%의 정력잔량 …… {BASE:PLAYER:정력}! 
		ENDIF
		PRINTFORML 
	ENDIF
ENDIF
