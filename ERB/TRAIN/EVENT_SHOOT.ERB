@SAMEN_AND_EXTACY
#DIM SHOOT_CHANGE_VALUE

SIF TEQUIP:촉수
	RETURN 0

CALL SETFLAG, "사정＆절정체크", PLAYER
;３Ｐ
IF COND("３Ｐ")
	CALL SETFLAG, "조교자교대"
	CALL SETFLAG, "사정＆절정체크", PLAYER
	CALL SETFLAG, "조교자교대"
ENDIF

;PLAYERに페니스があるなら, どこに사정했다かor我慢したかのTFLAGをここで설정する. 
;３Ｐなら変更不可とする（今後変更가능にするかも）
IF PENIS(PLAYER) && COND("절정", PLAYER) && COND("３Ｐ") == 0 && COND("사정금지中", PLAYER) == 0
	;後のTFLAG判定のために代入
	SELECTCASE TCVAR:PLAYER:5
	CASE 1, 5
		SHOOT_CHANGE_VALUE = 92
	CASE 2
		SHOOT_CHANGE_VALUE = 93
	CASE 3
		SHOOT_CHANGE_VALUE = 94
	CASE 4
		SHOOT_CHANGE_VALUE = 95
	CASE 6, 7
		SHOOT_CHANGE_VALUE = 96
	ENDSELECT

	;정액箇所変更가능判定플래그. 基本値は허리놀림. 
	;TFLAG:7が1ならば何とか사정箇所変更가능で, 2이상ならば, 余裕ありと判定されます. 
	;4이상ならば, @SHOOT_CHANGEにおいて「こらえる」が선택できる. 

	;정력と기교ＬＶに応じて猶予
	TFLAG:7 = LIMIT(ABL:PLAYER:기교, 0, 5) + 4 - 4*BASE:PLAYER:정력/MAXBASE:PLAYER:정력

	;限界돌파
	SELECTCASE BASE:PLAYER:성감허용치남자
	CASE IS >= MAXBASE:PLAYER:성감허용치남자*3
		TFLAG:7 = 0
	CASE IS >= MAXBASE:PLAYER:성감허용치남자
		;정력ＭＡＸ(-3or-6), 정력が９割이상（-2or-4）, ３割이상で分岐(-1or-2)
		SELECTCASE 100 * BASE:PLAYER:정력 / MAXBASE:PLAYER:정력
		CASE IS >= 100
			TFLAG:7 -= BASE:PLAYER:성감허용치남자 / MAXBASE:PLAYER:성감허용치남자 * 3
		CASE IS >= 90
			TFLAG:7 -= BASE:PLAYER:성감허용치남자 / MAXBASE:PLAYER:성감허용치남자 * 2
		CASE IS >= 30
			TFLAG:7 -= BASE:PLAYER:성감허용치남자 / MAXBASE:PLAYER:성감허용치남자
		ENDSELECT
	CASE IS >= MAXBASE:PLAYER:성감허용치남자/2
		TFLAG:7 += 2
	CASEELSE
		TFLAG:7 += 4
	ENDSELECT
	;前回こらえた
	SIF EX:PLAYER:가는것을참았다
		TFLAG:7 -= 1 + RAND:5

	;사정箇所を選べる커맨드か？
	SELECTCASE TCVAR:PLAYER:5
	CASE 1 TO 7
		;余裕があったときのみ사정箇所変更としているなら
		IF TFLAG:7 < 2 && FLAG:SHOOT_CHANGE_VALUE == 40
			TFLAG:7 = 0
		;사정箇所変更ONで余裕なし
		ELSEIF FLAG:91 == 1 && TFLAG:7 == 0
			TFLAG:7 = 1
		ENDIF
	;자위한다
	CASE 20
		TFLAG:7 = 1
	CASEELSE
		TFLAG:7 = 0
		SIF TEQUIP:PLAYER:자위한다
			TFLAG:7 = 1
	ENDSELECT

	;사정箇所変更を許さない커맨드はここに記述. 今後変更される가능性大. 
	SELECTCASE SELECTCOM
	CASE 66, 67, 68, 96, 105, 160
		TFLAG:7 = 0
	ENDSELECT

	SIF TEQUIP:식스나인 || CHECK_COM("파후파후손훑기") || CHECK_COM("Ａ자극손훑기")
		TFLAG:7 = 0
	;사정箇所変更OFFor황홀or구상での사정箇所決定
	SIF FLAG:91 == 0 || SHOOT("強制변경") || STATE("황홀", PLAYER)
		TFLAG:7 = 0

	;가랑이にかけた時には, VA両方に汚れがつくものとする. 
	;사정箇所変更플래그が立っていて, 二人に押し倒されて無いなら, ここで変更. 
	IF TFLAG:7 && TEQUIP:Ｗ넘어뜨리기 == 0
		CALL SHOOT_CHANGE, SHOOT_CHANGE_VALUE
	ELSEIF COND("사정箇所변경", PLAYER)
		IF SHOOT("外")
			CALL RESET_INSERT, TARGET, PLAYER
		ELSEIF SHOOT("변경불가")
			TCVAR:PLAYER:3 = 0
		ENDIF
	ENDIF
ENDIF

;사정or절정
IF COND("절정", PLAYER) || (COND("３Ｐ") && COND("절정", PLAYER:1) )
	CALL CALC, "동시절정"

	IF COND("절정", PLAYER)
		SIF STATE("바인드：Ｐ", PLAYER)
			PRINTFORML %조사처리(CALLNAME:PLAYER,"는")% 이상할 정도로 높아진 사정에 구속을 깨부수고 사정했다! 
		PRINTFORML %TEXTS("절정", PLAYER)%
	ENDIF
	SIF COND("３Ｐ") && COND("절정", PLAYER:1)
		PRINTFORML %TEXTS("절정", PLAYER:1)%

	;절정した時の텍스트を出力します. 
	IF COND("절정", PLAYER)
		CALL SAMEN_SHOOT_MESSAGE
	ELSE
		CALL SETFLAG, "조교자교대"
		CALL SAMEN_SHOOT_MESSAGE
		CALL SETFLAG, "조교자교대"
	ENDIF

	IF COND("절정", PLAYER)
		CALL CALC, "절정させた処理", TARGET
		;W넘어뜨리기
		SIF SELECTCOM:1 >= 0
			CALL CALC, "절정させた処理", TARGET:1
		CALL CALC, "절정した処理", PLAYER
	ENDIF
	IF COND("３Ｐ") && COND("절정", PLAYER:1)
		CALL SETFLAG, "조교자교대"
		CALL CALC, "절정させた処理", TARGET
		CALL CALC, "절정した処理", PLAYER
		CALL SETFLAG, "조교자교대"
	ENDIF

	;受精확률の変動を表示
	CALL CHECK_NINSHIN_ALLACTOR

	;効果を及ぼすのはメ임프レイヤー一人のみ
	CALL CALC, "에너지드레인"
ELSEIF COND("사정금지中", PLAYER)
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 요도를 억눌려, 사정하는 것이 불가능해졌다! 
ELSEIF COND("焦らされ中", PLAYER)
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%\@ SELECTCOM:1 >= 0 ? 들 # \@에게 사로잡혀있다! 
;こらえていない＆イきそう
ELSEIF EX:PLAYER:가는것을참았다 == 0 && BASE:PLAYER:절정 >= MAXBASE:PLAYER:절정*4/5 && TCVAR:PLAYER:100 != BASE:PLAYER:절정
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 가버릴 것 같아졌다……
ENDIF

;봉사정신ＬＶ２이상, 기교ＬＶ２이상で실행치を30이상上回っているなら汚れをなめ取る
IF ABL:봉사정신 > MAX(1, RAND:5) && ABL:기교 >= 2 && SHOOT("입") && TCVAR:28 >= 30 && STAIN:PLAYER:Ｐ != SET_STAIN(0, "Ｐ")
	SIF COND("절정", PLAYER) && TFLAG:8 == 0 && ABL:봉사정신 > RAND:5
		TFLAG:8 = 1
ENDIF


@SHOOT_CHANGE, ARG
#DIM CHOICE
#DIM MEMO_LINECOUNT
;どこに사정했다のか
CHOICE = -1

;사정箇所が선택できるかの判定に使う
