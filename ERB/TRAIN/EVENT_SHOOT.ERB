;---------------------------------------------------------
;사정処理
;---------------------------------------------------------
;3PとＷ펠라は, COMFに直接記述します. 
;조수를범한다って間違って無い？
;촉수, 드래곤についても, COMFに直接記述します. 
@SAMEN_CHECK, ARG
SELECTCASE ARG
;マイナスの値が入っているときには캔슬
CASE IS < 0
	BASE:PLAYER:절정 -= TCVAR:PLAYER:101
	TCVAR:PLAYER:101 = 0
	TCVAR:PLAYER:1 = 0
	IF COND("３Ｐ")
		CALL SETFLAG, "조교자교대"
		BASE:PLAYER:절정 -= TCVAR:PLAYER:101
		TCVAR:PLAYER:101 = 0
		TCVAR:PLAYER:1 = 0
		CALL SETFLAG, "조교자교대"
	ENDIF
CASEELSE
	CALL SAMEN_CHECK_PLAYER, ARG
	IF COND("３Ｐ")
		CALL SETFLAG, "조교자교대"
		CALL SAMEN_CHECK_PLAYER, ARG
		CALL SETFLAG, "조교자교대"
	ENDIF
ENDSELECT

@SAMEN_CHECK_PLAYER, ARG
;-------------------------------------------------
;레벨리미터체크
;-------------------------------------------------
CALL SETFLAG, "레벨리미터"
;-------------------------------------------------
;사정・절정게이지체크
;-------------------------------------------------
;사정게이지の増加量をLOCAL, その倍率をBで見る
LOCAL = 0
B = 1000

;커맨드に対する基礎快楽を설정する
;안면기승や역용간などは発生していない場合にはそもそもここの値が０なので, 이하の分岐でTEQUIP:넘어뜨리기などは無視しても問題ない
SELECTCASE SELECTCOM
;키스（텐더키스は威力減）
CASE 6
	LOCAL = 100 + ABL:기교*200 + ABL:Ｍ감각*100
CASE 10
	SIF INSERT("요도인서트", TARGET, PLAYER)
		LOCAL = ABL:PLAYER:기교*100 + ABL_U(TARGET)*50
;음경맞대기
CASE 16
	LOCAL = 300 + ABL:기교*200 + ABL:Ｃ감각*100
;Ｖ인서트
CASE 60 TO 69
	LOCAL = ABL:기교*100 + ABL_V(TARGET)*100

	;페니스を全部受け入れられないと快感低め
	SELECTCASE SIZE("페니스", PLAYER) - ABL:Ｖ확장
	CASE IS >= 3
		TIMES LOCAL, 0.10
	CASE 2
		TIMES LOCAL, 0.30
	CASE 1
		TIMES LOCAL, 0.70
	ENDSELECT

	SELECTCASE SELECTCOM
	CASE 60
		LOCAL += ABL:순종*250
	CASE 61
		LOCAL += ABL:마조끼*500
	CASE 62
		LOCAL += ABL:노출벽*500
	CASE 63
		TIMES LOCAL, 0.50
		LOCAL += 50 + ABL:기교*100 + ABL:Ｍ감각*50
	CASE 64
		TIMES LOCAL, 0.60
	CASE 65
		TIMES LOCAL, 0.80
		LOCAL += ABL:허리놀림*1000
	ENDSELECT

	;삽입失敗
	SIF COND("삽입前폭발플래그", PLAYER)
		LOCAL = 0
;Ａ인서트
CASE 70 TO 79
	LOCAL = ABL:기교*100 + ABL_A(TARGET)*100

	;페니스を全部受け入れられないと快感低め
	SELECTCASE SIZE("페니스", PLAYER) - ABL:Ａ확장
	CASE IS >= 3
		TIMES LOCAL, 0.10
	CASE 2
		TIMES LOCAL, 0.30
	CASE 1
		TIMES LOCAL, 0.70
	ENDSELECT

	SELECTCASE SELECTCOM
	CASE 60
		LOCAL += ABL:순종*250
	CASE 61
		LOCAL += ABL:마조끼*500
	CASE 62
		LOCAL += ABL:노출벽*500
	CASE 63
		TIMES LOCAL, 0.50
		LOCAL += 50 + ABL:기교*100 + ABL:Ｍ감각*50
	CASE 64
		TIMES LOCAL, 0.60
	CASE 65
		TIMES LOCAL, 0.80
		LOCAL += ABL:허리놀림*1000
	ENDSELECT
;貝合わせ
CASE 80
	IF CHECK_COM("쌍두바이브플레이")
		LOCAL = 500 + ABL:기교*200 + ABL:Ｖ감각*100
		;締まり対決. 値が大きいほど快楽が少なくなる（TARGETに与える快楽は増える）
		SELECTCASE ABL:Ｖ놀림 - ABL:Ｖ확장 - (ABL:PLAYER:Ｖ놀림 - ABL:PLAYER:Ｖ확장)
		CASE IS >= 3
			TIMES LOCAL, 2.00
		CASE 2
			TIMES LOCAL, 1.50
		CASE 1
			TIMES LOCAL, 1.20
		CASE -1
			TIMES LOCAL, 0.80
		CASE -2
			TIMES LOCAL, 0.50
		CASE IS <= -3
			TIMES LOCAL, 0.30
		ENDSELECT
	ELSE
		LOCAL = 500 + ABL:기교*200 + ABL:Ｃ감각*100
		;음모の生え具合보정
		SIF COND("음모", PLAYER) <= 5 && COND("음모") >= 5
			TIMES LOCAL, 1.20
	ENDIF
;가슴계곡을범한다
CASE 81
	;컵の大きさで강화
	LOCAL = 500 + ABL:기교*200 + ABL:Ｂ감각*100 + BASE:컵*100
;니플퍽
CASE 82
	;컵の大きさで강화
	LOCAL = 500 + ABL:기교*200 + ABL:Ｂ감각*100 + BASE:컵*200
	SIF TALENT:슬라임
		LOCAL += 500
;요도삽입
CASE 83
	LOCAL = ABL:기교*100 + ABL_U(TARGET)*100 + ABL:마조끼*500
	IF TEQUIP:넘어뜨리기
		TIMES LOCAL, 0.80
		LOCAL += ABL:허리놀림*1000
	ENDIF

;봉사系
CASE 90 TO 119
	LOCAL = 500 + ABL:기교*600
	SELECTCASE SELECTCOM
	CASE 91, 99
		LOCAL += ABL:정액중독*200
		SIF TEQUIP:딥스로트
			LOCAL += ABL_T(TARGET)*50
		SIF SELECTCOM == 99
			LOCAL += 300
	CASE 92, 94, 104
		LOCAL += ABL:Ｂ감각*100
	CASE 93, 100
		LOCAL += ABL:Ｃ감각*100
	CASE 95, 96
		LOCAL += ABL:새드끼*200
	;オナホ
	CASE 105
		LOCAL /= 2
		LOCAL += 3000
	CASEELSE
		LOCAL += ABL:기교*200
	ENDSELECT
;조교자인서트
CASE 120 TO 129
	LOCAL = 1000 + ABL:기교*400
;이라마치오
CASE 140
	IF TEQUIP:딥스로트
		LOCAL = ABL_T(TARGET)*100
	ELSE
		LOCAL = ABL_T(TARGET)*30
	ENDIF

	;페니스を全部受け入れられないと快感低め
	SELECTCASE SIZE("페니스", PLAYER) - ABL:목확장
	CASE IS >= 3
		TIMES LOCAL, 0.10
	CASE 2
		TIMES LOCAL, 0.30
	CASE 1
		TIMES LOCAL, 0.70
	ENDSELECT
;딥스로트
CASE 150
	IF TEQUIP:딥스로트
		LOCAL = ABL_T(TARGET)*50
	ELSE
		LOCAL = ABL_T(TARGET)*10
	ENDIF

	;페니스を全部受け入れられないと快感低め
	SELECTCASE SIZE("페니스", PLAYER) - ABL:목확장
	CASE IS >= 3
		TIMES LOCAL, 0.10
	CASE 2
		TIMES LOCAL, 0.30
	CASE 1
		TIMES LOCAL, 0.70
	ENDSELECT
;조수系
CASE 160, 162 TO 169
	SIF ASSI
		LOCAL = 1000 + ABL:ASSI:기교*300
;러브트랩
CASE 177
	LOCAL = ABL:기교*200 + ABL_V(TARGET)*100
;자위한다
CASE 282
	LOCAL = MAXBASE:PLAYER:절정/2
	SIF SELECTCOM == PREVCOM
		LOCAL += MAXBASE:PLAYER:절정/2
	;オカズによってボーナス有
	SELECTCASE TEQUIP:PLAYER:자위한다
	;컨닐링구스
	CASE 1
		SIF COND("♀에약함", PLAYER)
			LOCAL += MAXBASE:PLAYER:절정/2
	;자위
	CASE 3
		LOCAL += MAXBASE:PLAYER:절정/4
	;펠라한다
	CASE 4
		SIF COND("♂에약함", PLAYER)
			LOCAL += MAXBASE:PLAYER:절정/2
	;애널핥기
	CASE 9
		SIF COND("엉덩이에약함", PLAYER)
			LOCAL += MAXBASE:PLAYER:절정/2
	;조개벌리기
	CASE 13
		SIF COND("♀에약함", PLAYER)
			LOCAL += MAXBASE:PLAYER:절정/2
	;Ａ조개벌리기
	CASE 14
		SIF COND("엉덩이에약함", PLAYER)
			LOCAL += MAXBASE:PLAYER:절정/2
	ENDSELECT

;押し倒され
CASEELSE
	IF TEQUIP:넘어뜨리기
		;펠라한다
		SELECTCASE SELECTCOM
		CASE 4
			LOCAL = 10 + ABL:기교*100
		;안면기승
		CASE 1
			LOCAL = 300 + ABL:기교*300 + ABL:Ｃ감각*100
		CASE 9
			LOCAL = 300 + ABL:기교*300 + ABL:Ａ감각*100
		;역용간
		CASE 304, 305
			LOCAL = 1000 + ABL:기교*200
		ENDSELECT
	ENDIF
ENDSELECT

;조교지식による보너스
SELECTCASE TALENT_NAME(185, TALENT:조교지식, TARGET)
CASE "조교지식(애무・봉사)"
	SELECTCASE SELECTCOM
	CASE 6, 65, 75, 90 TO 119, 120, 121, 125, 126, 140
		LOCAL = MAX(MULTIPLY(LOCAL, 150), LOCAL + 500)
	ENDSELECT
CASE "조교지식(애널)"
	SELECTCASE SELECTCOM
	CASE 96, 125 TO 129
		LOCAL = MAX(MULTIPLY(LOCAL, 150), LOCAL + 1000)
	ENDSELECT
CASE "조교지식(도구)"
	SELECTCASE SELECTCOM
	CASE 105 TO 107
		LOCAL = MAX(MULTIPLY(LOCAL, 150), LOCAL + 1000)
	ENDSELECT
ENDSELECT

;인서트커맨드で, 삽입している人がペニバン使用してる時
IF PENIS(PLAYER) == 0
	SELECTCASE SELECTCOM
	CASE 60 TO 79, 81 TO 83, 140, 177
		LOCAL = 0
	ENDSELECT
ENDIF

SIF LOCAL == 0
	RETURN 0

;TCVAR:PLAYER:2でこのターンにPLAYERに사정절정するような刺激があったかを調べる. 女は키스でもイく事にしてみる. 
;TCVAR:PLAYER:2が0でも, 快感が사정절정게이지の최대치の2倍이상あれば耐え切れず사정절정する. 
;また, カウンターがあった場合にもTCVAR:PLAYER:2はONとなる. 
SELECTCASE SELECTCOM
CASE 4, 6
	SIF TALENT:PLAYER:여자 || TALENT:PLAYER:후타나리
		TCVAR:PLAYER:2 = 1
CASE 10, 16, 60 TO 79, 81 TO 83, 140, 150, 162, 177
	SIF PENIS(PLAYER)
		TCVAR:PLAYER:2 = 1
CASE 80, 90 TO 129, 282, 304, 305
	TCVAR:PLAYER:2 = 1
ENDSELECT

;PALAM:Ｖ윤, PALAM:Ａ윤をみる
SELECTCASE SELECTCOM
;例外的に윤활が関係ないもの
CASE 82, 83
;안면기승系
CASE 1, 9
	B = MULTIPLY(B, MIN(80 + GET_PALAMLV(PALAM:Ｖ윤)*10, 120) + ABL:욕망*10)
;기승위系
CASE 65, 75, 177
	B = MULTIPLY(B, MIN(20 + GET_PALAMLV(PALAM:Ｖ윤)*30, 120) + ABL:욕망*10)
;Ｖ인서트
CASE 60 TO 69
	B = MULTIPLY(B, MIN(60 + GET_PALAMLV(PALAM:Ｖ윤)*20, 120) + ABL:욕망*10)
;Ａ인서트
CASE 70 TO 79
	B = MULTIPLY(B, MIN(40 + GET_PALAMLV(PALAM:Ａ윤)*20, 120) + ABL:욕망*10)
;摩擦인서트
CASE 80
	B = MULTIPLY(B, MIN(10 + GET_PALAMLV(PALAM:Ｖ윤)*40, 120) + ABL:욕망*10)
CASE 16, 81 TO 89, 93, 94, 100
	B = MULTIPLY(B, MIN(10 + GET_PALAMLV(PALAM:PLAYER:Ｐ윤)*40, 120) + ABL:욕망*10)
ENDSELECT

;主に技能による보정
CALL SAMEN_CHECK_EX

;연속절정コンボ数による보정
IF PALAM:보정치
	;봉사커맨드は影響強い
	SELECTCASE SELECTCOM
	CASE 6, 90 TO 119, 140
		B = DIVIDE(B, 100 + PALAM:보정치*20)
	CASEELSE
		B = DIVIDE(B, 100 + PALAM:보정치*5)
	ENDSELECT
ENDIF

;２倍이상の場合には, ２のn乗 < 보정倍率 < ２の(n+1)乗 →　(n+1)倍 < 보정倍率 < (n+2)倍
;効果が上がりすぎると上がり幅の減少著しい対数関数的な보정
B = CALCF("快楽보정", B)

LOCAL = MULTIPLY(LOCAL, B/10)

;기교や마음에듬や성감대, ＬＶ差等絶対にかかる보정
;非常に効果が高い
LOCAL = CALCF("基礎보정", LOCAL)

;넘어뜨리기orＷ넘어뜨리기or３Ｐの時の보정
IF TEQUIP:Ｗ넘어뜨리기 || SELECTCOM:1 >= 0 || COND("３Ｐ")
	TIMES LOCAL, 0.70
ELSEIF TEQUIP:넘어뜨리기
	IF ABL:PLAYER:마조끼
		LOCAL = MULTIPLY(LOCAL, 100 + MIN(ABL:새드끼 + TALENT:새드, ABL:PLAYER:마조끼 + TALENT:PLAYER:마조)*10)
	ELSE
		LOCAL = MULTIPLY(LOCAL, 100 - (ABL:PLAYER:새드끼 + TALENT:PLAYER:새드)*5)
	ENDIF
ENDIF

;かなり強めの減少보정（조교中のみ）
IF COND("조교中")
	SELECTCASE SELECTCOM
	CASE 6
		;텐더키스
		SIF NUM("턴수") == 0
			TIMES LOCAL, 0.10
	CASE 91
		SIF CHECK_COM("청소펠라", 91)
			TIMES LOCAL, 0.20
	CASE 98
		SIF CHECK_COM("청소컨닐링구스", 98)
			TIMES LOCAL, 0.20
	CASE 99
		SIF CHECK_COM("청소펠라", 91) + CHECK_COM("청소컨닐링구스", 98) && CHECK_COM("청소펠라", 4) + CHECK_COM("청소컨닐링구스", 1)
			TIMES LOCAL, 0.20
	ENDSELECT
ENDIF

;プレイヤーが自分でやっていない場合にはオミット
SELECTCASE SELECTCOM
;자위한다
CASE 282

CASEELSE
	;미약を使われてると、기교減少
	SIF TEQUIP:미약
		LOCAL = DIVIDE(LOCAL, 100 + TEQUIP:미약)
	;흐물흐물
	SIF STATE("흐물흐물", TARGET)
		TIMES LOCAL, 0.50
	;스팽킹섹스
	SIF COND("스팽킹섹스플래그") && COND("스팽킹섹스플래그", PLAYER)
		TIMES LOCAL, 1.10
	;투와이스(TARGETが主導権を握っているもののみ。안면기승と키스は効果無しとする)
	IF STATE("투와이스", TARGET)
		SELECTCASE SELECTCOM
		CASE 65, 75, 90 TO 119, 120 TO 129
			TIMES LOCAL, 2.00
		ENDSELECT
	ENDIF
ENDSELECT

;呼び出し時の보정
SIF ARG > 0
	LOCAL = MULTIPLY(LOCAL, ARG)

;PRINTFORML 플레이ヤーの사정게이지増加 = {LOCAL}
BASE:PLAYER:절정 += LOCAL

;사정게이지増加の一時保存
TCVAR:PLAYER:101 += LOCAL

;사정ＬＶの計算
TCVAR:PLAYER:1 = CALCF("절정", PLAYER)

;PRINTFORML 플레이ヤーの사정게이지 = {BASE:PLAYER:절정}


;기교以外のABLの値による사정게이지増加보정
@SAMEN_CHECK_EX
SELECTCASE SELECTCOM
CASE 6
	;ABL:Ｍ감각をみる
	SELECTCASE ABL:PLAYER:Ｍ감각
	CASE 0
		TIMES B, 0.50
	CASE 1
		TIMES B, 0.70
	CASE 2
		TIMES B, 1.00
	CASE 3
		TIMES B, 1.20
	CASE 4
		TIMES B, 1.50
	CASEELSE
		TIMES B, 2.00
	ENDSELECT
	;민감具合
	IF TALENT:PLAYER:음탕한입술
		TIMES B, 2.00
	ELSEIF TALENT:PLAYER:Ｍ민감
		TIMES B, 1.50
	ELSEIF TALENT:PLAYER:Ｍ둔감
		TIMES B, 0.50
	ENDIF
;인서트커맨드
CASE 60 TO 69, 177
	;삽입中애무で締め付け増加
	SIF COND("締め付け플래그")
		TIMES B, 1.20
	;절정すると, 締め付け増加
	SIF NOWEX:절정수합계
		B = MULTIPLY(B, 100 + NOWEX:절정수합계*20)
	SIF INSERT("와일드스윙")
		TIMES B, 1.50
	;PLAYERが동정
	SIF EXP:PLAYER:Ｖ삽입경험 + EXP:PLAYER:Ａ삽입경험 == 0
		TIMES B, 3.00
	;EXP:Ｖ성교경험をみる. 経験が多くなると少し上がる
	IF TALENT:처녀 == 1
		TIMES B, 1.50
	ELSE
		B = MULTIPLY(B, 100 + GET_EXP_LV(EXP:Ｖ성교경험)*5)
	ENDIF
	;페니스の大きさとの差で보너스
	SELECTCASE ABL:Ｖ확장 - SIZE("페니스", PLAYER)
	CASE IS <= 0
		TIMES B, 1.20
	CASE IS <= ABL:Ｖ놀림
		LOCAL = LIMIT(150 * BASE:체력 / MAXBASE:체력, 25, 100)
		B = MULTIPLY(B, LOCAL)
	CASEELSE
		LOCAL = LIMIT(120 * BASE:체력 / MAXBASE:체력, 10, 100)
		B = MULTIPLY(B, LOCAL)
	ENDSELECT
	;JUST FIT
	SIF CHECK("ＶJUST FIT")
		TIMES B, 2.00
CASE 70 TO 79
	;삽입中애무で締め付け増加
	SIF COND("締め付け플래그")
		TIMES B, 1.20
	;절정すると, 締め付け増加
	SIF NOWEX:절정수합계
		B = MULTIPLY(B, 100 + NOWEX:절정수합계*20)
	SIF INSERT("와일드스윙")
		TIMES B, 1.50
	;PLAYERが동정
	SIF EXP:PLAYER:Ｖ삽입경험 + EXP:PLAYER:Ａ삽입경험 == 0
		TIMES B, 3.00
	;EXP:Ａ성교경험をみる. 経験が多くなると少し上がる
	IF TALENT:Ａ처녀
		TIMES B, 1.50
	ELSE
		B = MULTIPLY(B, 100 + GET_EXP_LV(EXP:Ａ성교경험)*5)
	ENDIF
	;페니스の大きさとの差で보너스
	SELECTCASE ABL:Ａ확장 - SIZE("페니스", PLAYER)
	CASE IS <= 0
		TIMES B, 1.20
	CASE IS <= ABL:Ａ놀림
		LOCAL = LIMIT(150 * BASE:체력 / MAXBASE:체력, 25, 100)
		B = MULTIPLY(B, LOCAL)
	CASEELSE
		LOCAL = LIMIT(120 * BASE:체력 / MAXBASE:체력, 10, 100)
		B = MULTIPLY(B, LOCAL)
	ENDSELECT
	;JUST FIT
	SIF CHECK("ＡJUST FIT")
		TIMES B, 2.00
CASE 83
	;삽입中애무で締め付け増加
	SIF COND("締め付け플래그")
		TIMES B, 1.20
	;절정すると, 締め付け増加
	SIF NOWEX:절정수합계
		B = MULTIPLY(B, 100 + NOWEX:절정수합계*20)
	SIF INSERT("와일드스윙")
		TIMES B, 1.50
	;PLAYERが동정
	SIF EXP:PLAYER:Ｖ삽입경험 + EXP:PLAYER:요도삽입경험 == 0
		TIMES B, 3.00
;PLAYERのＡを刺激
CASE 96, 107, 109, 110, 125 TO 129, 305
	;ABL:Ａ감각をみる
	SELECTCASE ABL:PLAYER:Ａ감각
	CASE 0
		TIMES B, 0.50
	CASE 1
		TIMES B, 0.70
	CASE 2
		TIMES B, 1.00
	CASE 3
		TIMES B, 1.20
	CASE 4
		TIMES B, 1.50
	CASEELSE
		TIMES B, 2.00
	ENDSELECT
	;플레이ヤーのEXP:Ａ경험をみる
	SELECTCASE EXP:PLAYER:Ａ경험
	CASE IS < 5
		TIMES B, 0.10
	CASE IS < 20
		TIMES B, 0.30
	CASE IS < 50
		TIMES B, 0.50
	CASE IS < 150
		TIMES B, 0.80
	ENDSELECT
	;민감具合
	IF TALENT:PLAYER:애널광
		TIMES B, 2.00
	ELSEIF TALENT:PLAYER:Ａ민감
		TIMES B, 1.50
	ELSEIF TALENT:PLAYER:Ａ둔감
		TIMES B, 0.50
	ENDIF
	;플레이ヤーが남자
	SIF TALENT:PLAYER:남자
		TIMES B, 1.50
	;Ａ확장具合による보정
	IF SELECTCOM == 107
		LOCAL = SIZE("Ａ바이브", PLAYER) - ABL:PLAYER:Ａ확장
		;緩い
		IF LOCAL <= -2
			TIMES B, 0.80
		;余裕あり
		ELSEIF LOCAL == -1
			TIMES B, 1.20
		;余裕無し
		ELSE
			B /= MAX(LOCAL*2, 1)
		ENDIF
	ENDIF
;PLAYERのＣを刺激
CASE 95, 98, 99
	IF TALENT:PLAYER:남자 == 0
		;ABL:Ｃ감각をみる
		SELECTCASE ABL:PLAYER:Ｃ감각
		CASE 0
			TIMES B, 0.50
		CASE 1
			TIMES B, 0.70
		CASE 2
			TIMES B, 1.00
		CASE 3
			TIMES B, 1.20
		CASE 4
			TIMES B, 1.50
		CASEELSE
			TIMES B, 2.00
		ENDSELECT
		;민감具合
		IF TALENT:PLAYER:음핵
			TIMES B, 2.00
		ELSEIF TALENT:PLAYER:Ｃ민감
			TIMES B, 1.50
		ELSEIF TALENT:PLAYER:Ｃ둔감
			TIMES B, 0.50
		ENDIF
	ENDIF
;PLAYERのＶを刺激
CASE 106, 108, 110, 120 TO 124, 304
	;ABL:V감각をみる
	SELECTCASE ABL:PLAYER:Ｖ감각
	CASE 0
		TIMES B, 0.50
	CASE 1
		TIMES B, 0.70
	CASE 2
		TIMES B, 1.00
	CASE 3
		TIMES B, 1.20
	CASE 4
		TIMES B, 1.50
	CASEELSE
		TIMES B, 2.00
	ENDSELECT
	;플레이ヤーのEXP:Ｖ경험をみる
	SELECTCASE EXP:PLAYER:Ｖ경험
	CASE IS < 5
		TIMES B, 0.20
	CASE IS < 20
		TIMES B, 0.40
	CASE IS < 50
		TIMES B, 0.60
	CASE IS < 150
		TIMES B, 0.80
	ENDSELECT
	;민감具合
	IF TALENT:PLAYER:음호
		TIMES B, 2.00
	ELSEIF TALENT:PLAYER:Ｖ민감
		TIMES B, 1.50
	ELSEIF TALENT:PLAYER:Ｖ둔감
		TIMES B, 0.50
	ENDIF
	;Ｖ확장具合による보정
	IF SELECTCOM == 106
		LOCAL = SIZE("바이브", PLAYER) - ABL:PLAYER:Ｖ확장
		;緩い
		IF LOCAL <= -2
			TIMES B, 0.80
		;余裕あり
		ELSEIF LOCAL == -1
			TIMES B, 1.20
		;余裕無し
		ELSE
			B /= MAX(LOCAL*2, 1)
		ENDIF
	ENDIF
ENDSELECT

;部位特化能力. 
SELECTCASE SELECTCOM
;舌を使う커맨드
CASE 6, 63, 73, 91, 96, 98, 99, 140, 150
	;음탕한입술
	SIF TALENT:음탕한입술
		TIMES B, 1.50
	;ABL:혀놀림をみる
	B = SOURCE_EXABL(B, 30, TARGET)
;손가락を使う커맨드
CASE 90, 97
	;ABL:손가락놀림
	B = SOURCE_EXABL(B, 31, TARGET)
	;팔많음
	SIF TALENT:팔많음
		TIMES B, 2.00
;가슴을使う커맨드（例外的に가슴을揉んだりするのもＯＫ）
CASE 64, 74, 81, 82, 92, 94, 104
	;음유
	SIF TALENT:음유
		TIMES B, 1.50
	;ABL:마유
	B = SOURCE_EXABL(B, 34, TARGET)
	;폭유, 거유, 빈유
	IF TALENT:거유 >= 3
		TIMES B, 2.00
	ELSEIF TALENT:거유 == 2
		TIMES B, 1.50
	ELSEIF TALENT:거유
		TIMES B, 1.20
	ELSEIF TALENT:빈유
		TIMES B, 0.70
	ENDIF
;발を使う커맨드
CASE 95
	;ABL:발놀림
	B = SOURCE_EXABL(B, 32, TARGET)
ENDSELECT

;허리를 使う커맨드
SELECTCASE SELECTCOM
CASE 1, 4, 9, 16, 93, 94, 120 TO 129, 177
	;ABL:허리놀림
	B = SOURCE_EXABL(B, 33, TARGET)
;기승위は섹스系の中では허리놀림の影響が強い（허리놀림ＬＶが０だとペナルティ）
CASE 65, 75
	;ABL:허리놀림
	IF ABL:허리놀림
		B = SOURCE_EXABL(B, 33, TARGET)
	ELSE
		TIMES B, 0.70
	ENDIF
;기승위以外の섹스系はちょい弱め(삽입국소자극系は허리놀림の影響なし)
CASE 60 TO 64, 70 TO 74
	B += (SOURCE_EXABL(B, 33, TARGET) - B)/3
CASE 83
	IF TEQUIP:넘어뜨리기
		IF ABL:허리놀림
			B = SOURCE_EXABL(B, 33, TARGET)
		ELSE
			TIMES B, 0.70
		ENDIF
	ELSE
		B += (SOURCE_EXABL(B, 33, TARGET) - B)/3
	ENDIF
ENDSELECT

;기력０だとペナルティ
SIF BASE:기력 <= 0
	B /= 2

