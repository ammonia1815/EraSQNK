		SOURCE:욕정추가 += 800
		SOURCE:굴복 += 1000

		NOWEX:사정직전멈춤 = 2
	ELSEIF NOWEX:사정
		SOURCE:욕정추가 += 300
		SOURCE:굴복 += 500

		NOWEX:사정직전멈춤 = 1
	ENDIF
	NOWEX:사정 = 0
ELSEIF NOWEX:사정
	SELECTCASE EXP:사정경험
	CASE IS < EXPLV:1
		SOURCE:굴복 += 650 * NOWEX:사정
	CASE IS < EXPLV:2
		SOURCE:굴복 += 550 * NOWEX:사정
	CASE IS < EXPLV:3
		SOURCE:굴복 += 450 * NOWEX:사정
	CASE IS < EXPLV:4
		SOURCE:굴복 += 400 * NOWEX:사정
	CASE IS < EXPLV:5
		SOURCE:굴복 += 300 * NOWEX:사정
	CASEELSE
		SOURCE:굴복 += 200 * NOWEX:사정
	ENDSELECT

	EXP:사정경험 += NOWEX:사정
	BASE:절정 = (PALAM:쾌Ｃ + CUP:쾌Ｃ - CDOWN:0) * MAXBASE:절정 / 10000
	;정력減少(Ｃ절정でも減っているはずなのでここでは純粋に사정で減るだけを扱う)
	LOCAL = MAX(NOWEX:사정 - TALENT:몽마 - TALENT:상위몽마, 1)
	BASE:정력 = LIMIT(BASE:정력 - LOCAL, 0, MAXBASE:정력)
	TCVAR:109 += LOCAL

	;사정했다場所に정액汚れを付加
	IF INSERT("Ｖ삽입中")
		PARTNER = INSERT("Ｖ삽입中")
		EXP:PARTNER:정액경험 += NOWEX:사정 * 2
		CALL SETFLAG, "질내정액경험", NOWEX:사정, PARTNER, TARGET
	ELSEIF INSERT("Ａ삽입中")
		PARTNER = INSERT("Ａ삽입中")
		EXP:PARTNER:정액경험 += NOWEX:사정 * 2
		CALL SETFLAG, "장내정액경험", NOWEX:사정, PARTNER, TARGET
	ELSE
		SELECTCASE SELECTCOM
		;입
		CASE 4, 11
			EXP:PLAYER:정액경험 += NOWEX:사정 * 3
			CALL SETFLAG, "정음경험", NOWEX:사정, PLAYER, TARGET
			SIF SELECTCOM == 11
				CALL ADD_STAIN, PLAYER, "Ｂ", "정액"
		;손
		CASE 10
			EXP:PLAYER:정액경험 += NOWEX:사정
			CALL ADD_STAIN, PLAYER, "손", "정액"
		;보지
		CASE 120 TO 124
			EXP:PLAYER:정액경험 += NOWEX:사정 * 2
			CALL SETFLAG, "질내정액경험", NOWEX:사정, PLAYER, TARGET
		;애널
		CASE 125 TO 129
			EXP:PLAYER:정액경험 += NOWEX:사정 * 2
			CALL SETFLAG, "장내정액경험", NOWEX:사정, PLAYER, TARGET
		;조수の보지
		CASE 161
			EXP:ASSI:정액경험 += NOWEX:사정 * 2
			CALL SETFLAG, "질내정액경험", NOWEX:사정, ASSI, TARGET
		ENDSELECT
	ENDIF

	;Ｐに정액汚れ
	CALL ADD_STAIN, TARGET, "Ｐ", "정액"
ENDIF
SIF FLAG:8
	CALL TARGET_EJAC_CHECK_TEXT, TARGET


@TARGET_EJAC_CHECK_TEXT, ARG
VARSET LOCALS
IF NOWEX:ARG:사정직전멈춤
	PRINTFORML %CALLNAME:ARG%\@ ARG == ASSI ? (%TEXTS("조수の명칭")%) # \@\@ NOWEX:ARG:사정직전멈춤 >= 2 ? 대량 # \@사정직전멈춤
ELSEIF NOWEX:ARG:사정
	LOCALS = \@ NOWEX:ARG:사정 >= 3 ? 초 # \@\@ NOWEX:ARG:사정 >= 2 ? 대량 # \@사정
	IF ARG == ASSI
		STR:6 = %STR:6%(%LOCALS%)
	ELSEIF ARG == TARGET
		STR:5 = %STR:5%(%LOCALS%)
	ENDIF
	PRINTFORML %CALLNAME:ARG%%LOCALS%
	PRINTFORML 사정경험＋{NOWEX:ARG:사정} \@ ARG == ASSI ? (%TEXTS("조수の명칭")%) # \@
	LOCAL = MAX(NOWEX:ARG:사정 - TALENT:ARG:몽마 - TALENT:ARG:상위몽마, 1)
	PRINTFORML 정력-{LOCAL}
ENDIF


;-------------------------------------------------
;조교대상の분유체크
;-------------------------------------------------
@TARGET_MILK_CHECK

SIF TALENT:모유체질 == 0
	RETURN 0

CALL SETFLAG, "사정게이지", TARGET

;母乳生産量
LOCAL = RAND:1000 + (CUP:쾌Ｂ * MAXBASE:분유 / EJAC:TARGET)
LOCAL += TALENT:거유 * 500
SIF TALENT:음유
	LOCAL += 500
SIF BASE:육아중
	LOCAL += 1000

BASE:분유 += LOCAL
;모유게이지増加量の記録
TCVAR:103 += LOCAL

IF CUP:쾌Ｂ && NOWEX:Ｂ절정 && BASE:분유 >= MAXBASE:분유 && (NOWEX:Ｂ절정 >= 2 || BASE:분유 >= EJAC:TARGET)
	SELECTCASE BASE:분유
	CASE IS >= 10000 * 4
		NOWEX:분유 = 3
	CASE IS >= 10000 * 2
		NOWEX:분유 = 2
	CASE IS >= 10000
		NOWEX:분유 = 1
	ENDSELECT
	NOWEX:분유 = MIN(NOWEX:분유, NOWEX:Ｂ절정 + 1)
ENDIF

EX:분유 += NOWEX:분유

IF NOWEX:분유
	SOURCE:노출 += 500 * NOWEX:분유

	EXP:분유경험 += 1

	;Ｂに모유汚れ
	CALL ADD_STAIN, TARGET, "Ｂ", "乳"

	IF NOWEX:Ｂ절정
		BASE:분유 = MAX(BASE:분유 - NOWEX:분유*10000, 0)
	ELSE
		BASE:분유 = MAX(BASE:분유 - NOWEX:분유*5000, 0)
	ENDIF
	;착유기で모유が集められているか체크
	SIF TEQUIP:착유기
		TFLAG:35 += NOWEX:분유
ENDIF

SIF FLAG:8
	CALL TARGET_MILK_CHECK_TEXT, TARGET


@TARGET_MILK_CHECK_TEXT, ARG
IF NOWEX:ARG:분유
	IF NOWEX:ARG:분유 >= 2 && NOWEX:Ｂ절정
		PRINTFORML %조사처리(CALLNAME:ARG,"는")%  유두에서 대량의 모유를 분출했다
	ELSEIF NOWEX:Ｂ절정
		PRINTFORML %조사처리(CALLNAME:ARG,"는")%  유두에서 모유를 흘렸다
	ELSE
		PRINTFORML %CALLNAME:ARG%의 유두에서 모유가 배어나왔다
	ENDIF
	PRINTFORML 분유경험＋{NOWEX:ARG:분유} \@ ARG == ASSI ? (%TEXTS("조수の명칭")%) # \@
ENDIF
