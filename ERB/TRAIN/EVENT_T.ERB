;-------------------------------------------------
;各種, 특수な텍스트を管理する
;-------------------------------------------------
@TEXT, ARGS, ARG
#DIM LCOUNT

SELECTCASE ARGS
CASE "正の字"
	FOR LCOUNT, 0, (ARG / 5)
		PRINTFORM 정
	NEXT
	;正の字フォントがあるなら１～４画目も表示
	IF ARG%5
		GETFONT
		CHKFONT "seinoji"
		SIF RESULT
			SETFONT "seinoji"
		PRINTFORM {ARG%5}
		SETFONT RESULTS
	ENDIF

CASE "절정計算"
	PRINTFORML 
	DRAWLINE
	PRINTFORM   절정     = {BASE:PLAYER:절정, 6} + {ARG, 6} = 
	BASE:PLAYER:절정 += ARG
	PRINTFORML {BASE:PLAYER:절정, 6} 절정
	DRAWLINE
CASE "무드ハート"
	;무드をハートの数にする
	LOCAL = MOOD_HEART()

	FOR LCOUNT, 1, 7
		IF LOCAL >= LCOUNT
			SETCOLOR DEF_COLOR("하트핑크")
		ELSE
			SETCOLOR DEF_COLOR("회색")
		ENDIF
		CALL HEARTMARK
	NEXT
	IF LOCAL >= 6
		PRINT  
		CALL HEARTMARK
		PRINT MAX
		CALL HEARTMARK
	ENDIF
	RESETCOLOR

CASE "生理周期"
	;---------------------------------------------------------
	;위험일안전일の表示
	;---------------------------------------------------------
	SIF CONFIG("임신패치") == 0 || TALENT:ARG:남자 || TALENT:ARG:마도인형
		RETURN 0
	SIF ARG == MASTER && CONFIG("임신패치：주인임신가능") == 0
		RETURN 0

	IF TALENT:ARG:임신 == 3
		PRINTFORM (출산임박) 
	ELSEIF TALENT:ARG:임신 == 2
		PRINTFORM (산월) 
	ELSEIF TALENT:ARG:임신
		PRINTFORM (임신) 
	;피임中
	ELSEIF COND("피임中", ARG)
		PRINTFORM (피임) 
	ELSEIF COND("발정기", ARG)
		PRINTFORM (
		SETCOLOR DEF_COLOR("하트핑크")
		PRINTFORM 발정기
		RESETCOLOR
		PRINTFORM ) 
	;ELSEIF BASE:ARG:태아
	ELSEIF COND("안전일", ARG)
		PRINTFORM (안전일) 
	ELSEIF COND("배란中", ARG)
		PRINTFORM (
		SETCOLOR DEF_COLOR("하트핑크")
		PRINTFORM 배란일
		RESETCOLOR
		PRINTFORM ) 
	ELSEIF COND("위험일", ARG)
		PRINTFORM (
		SETCOLOR DEF_COLOR("노랑")
		PRINTFORM 위험일
		RESETCOLOR
		PRINTFORM ) 
	ENDIF
