@SHOW_USERCOM
#DIM LCOUNT
#DIM MEMO_LINECOUNT
;存在している조교のみを数えていって몇번째か
#DIM NUM_TRAIN
#DIM NEXT_LINE
#DIM NUM_COLUMN
#DIMS COM_NAMES
#DIMS COM_NAMES_HIDDEN

REDRAW 0
;主導権が無い플래그を一時的に消去
CLEARBIT FLAG:99, 11

;갈아입히기部分の高速化플래그消去
TCVAR:26 = 0

IF FLAG:238 || CONFIG("조교컨피그모드")
	FLAG:238 = 0

	NUM_TRAIN = -1

	NEXT_LINE = 0
	NUM_COLUMN = 3 + (CONFIG("画面표시：조교커맨드４列") > 0)

	;直接선택できる커맨드は499番までだが, 現状は320までで十分すぎる
	FOR LCOUNT, 0, 320
		SIF STRLENS(TRAINNAME:LCOUNT) == 0
			CONTINUE

		NUM_TRAIN += 1

		IF CONFIG("조교컨피그모드：全커맨드표시") == 0
			TRYCALLFORM COM_ABLE{LCOUNT}
			SIF RESULT == 0
				CONTINUE
		ENDIF

		RESETCOLOR
		SIF CONFIG("画面표시：未実行커맨드の色") && FIRSTCOM(LCOUNT) == 0
			SETCOLOR DEF_COLOR("明회색")

		IF CONFIG("조교컨피그모드") == 0
			;커맨드표시필터
			SELECTCASE DITEMTYPE:0:LCOUNT
			;비표시설정
			CASE 1
				SIF CONFIG("비표시커맨드の표시") == 0
					CONTINUE
				SETCOLOR DEF_COLOR("파랑")
			;禁止설정
			CASE 2
				CONTINUE
			ENDSELECT
		ELSE
		 	SELECTCASE TRAINNAME:LCOUNT
			CASE "～예비～"
				CONTINUE
			ENDSELECT
		ENDIF

		IF CONFIG("ＨＡＲＤ") && TEQUIP:넘어뜨리기
			SELECTCASE LCOUNT
			CASE 178, 280, 281
				PRINTFORMC %TRAIN_NAME_SELECT(LCOUNT)%[{NUM_TRAIN, 3}]
			CASEELSE
				SETCOLOR DEF_COLOR("회색")
				PRINTFORMC %TRAIN_NAME_SELECT(LCOUNT)%[XXX]
				RESETCOLOR
			ENDSELECT
		ELSE

			IF CONFIG("조교컨피그모드") == 0
				;커맨드名が通常と違う場合には色を変える(넘어뜨리기状態での基本커맨드は色を変えない)
				SIF TRAIN_NAME_SELECT(LCOUNT) != TRAIN_NAME_STANDARD(LCOUNT)
					SETCOLOR DEF_COLOR("하트핑크")

				;플레이を終わらせられる時には色を変える
				SELECTCASE LCOUNT
				CASE 31, 201
					SIF TEQUIP:바이브
						SETCOLOR DEF_COLOR("황색")
				CASE 33, 202
					SIF TEQUIP:애널바이브
						SETCOLOR DEF_COLOR("황색")
				CASE 34, 203
					SIF TEQUIP:클리캡
						SETCOLOR DEF_COLOR("황색")
				CASE 35, 204
					SIF TEQUIP:니플캡
						SETCOLOR DEF_COLOR("황색")
				CASE 36, 205
					SIF TEQUIP:착유기
						SETCOLOR DEF_COLOR("황색")
				CASE 38
					SIF TEQUIP:샤워
						SETCOLOR DEF_COLOR("황색")
				CASE 39
					SIF TEQUIP:로터삽입
						SETCOLOR DEF_COLOR("황색")
				CASE 99
					SIF TEQUIP:식스나인
						SETCOLOR DEF_COLOR("황색")
				CASE 133
					SIF TEQUIP:아이마스크
						SETCOLOR DEF_COLOR("황색")
				CASE 134
					SIF TEQUIP:구속
						SETCOLOR DEF_COLOR("황색")
				CASE 135
					SIF TEQUIP:재갈
						SETCOLOR DEF_COLOR("황색")
				CASE 136
					SIF COND("애널栓")
						SETCOLOR DEF_COLOR("황색")
				CASE 137
					SIF TEQUIP:확장벌룬
						SETCOLOR DEF_COLOR("황색")
				CASE 149
					SIF TEQUIP:카테터
						SETCOLOR DEF_COLOR("황색")
				CASE 170
					SIF TEQUIP:비디오카메라
						SETCOLOR DEF_COLOR("황색")
				CASE 171
					SIF TEQUIP:야외플레이
						SETCOLOR DEF_COLOR("황색")
				CASE 172
					SIF TEQUIP:목욕탕플레이
						SETCOLOR DEF_COLOR("황색")
				CASE 173
					SIF TEQUIP:새댁플레이
						SETCOLOR DEF_COLOR("황색")
				CASE 174
					SIF TEQUIP:관내노출플레이
						SETCOLOR DEF_COLOR("황색")
				CASE 179
					SIF TEQUIP:수치플레이
						SETCOLOR DEF_COLOR("황색")
				CASE 182
					SIF TEQUIP:PLAYER:넘어뜨리기
						SETCOLOR DEF_COLOR("황색")
				CASE 200
					SIF TEQUIP:촉수
						SETCOLOR DEF_COLOR("황색")
				CASE 208
					SIF TEQUIP:촉수구욕
						SETCOLOR DEF_COLOR("황색")
				CASE 209
					SIF TEQUIP:오나홀
						SETCOLOR DEF_COLOR("황색")
				CASE 301
					SIF TEQUIP:손대중
						SETCOLOR DEF_COLOR("황색")
				CASE 302
					SIF TEQUIP:연주중
						SETCOLOR DEF_COLOR("황색")
				CASE 306
					SIF STATE("하드스킨", PLAYER)
						SETCOLOR DEF_COLOR("황색")
				ENDSELECT
			ELSE
				SELECTCASE DITEMTYPE:0:LCOUNT
				;通常状態
				CASE 0
					SELECTCASE LCOUNT
					CASE 0 TO 9, 60, 61, 65, 71, 75, 80 TO 95, 98, 99, 230 TO 289
						SETCOLOR DEF_COLOR("暗황색")
					CASEELSE
						RESETCOLOR
					ENDSELECT
				;비표시설정
				CASE 1
					SIF CONFIG("조교컨피그모드：비표시커맨드を숨김")
						CONTINUE
					SETCOLOR DEF_COLOR("파랑")
				;禁止설정
				CASE 2
					SIF CONFIG("조교컨피그모드：금지커맨드を숨김")
						CONTINUE
					SETCOLOR DEF_COLOR("초록")
				ENDSELECT
			ENDIF

			IF GETCOLOR() != DEF_COLOR("황색") && GETCOLOR() != DEF_COLOR("하트핑크")
				;표시するかどうか
				SELECTCASE LCOUNT
				CASE 0 TO 29
					SIF CONFIG("애무系비표시") == 0
						CONTINUE
				CASE 30 TO 59
					SIF CONFIG("도구系비표시") == 0
						CONTINUE
				CASE 60 TO 69
					SIF CONFIG("Ｖ섹스系비표시") == 0
						CONTINUE
				CASE 70 TO 79
					SIF CONFIG("Ａ섹스系비표시") == 0
						CONTINUE
				CASE 80 TO 89
					SIF CONFIG("특수섹스系비표시") == 0
						CONTINUE
				CASE 90 TO 119
					SIF CONFIG("봉사系비표시") == 0
						CONTINUE
				CASE 120 TO 129
					SIF CONFIG("조교자に삽입系비표시") == 0
						CONTINUE
				CASE 130 TO 159
					SIF CONFIG("ＳＭ・하드코어系비표시") == 0
						CONTINUE
				CASE 160 TO 169
					SIF CONFIG("조수系비표시") == 0
						CONTINUE
				CASE 170 TO 199
					SIF CONFIG("특수系비표시") == 0
						CONTINUE
				CASE 200 TO 229
					SIF CONFIG("촉수系비표시") == 0
						CONTINUE
				CASE 230 TO 259
					SIF CONFIG("아르카나系비표시") == 0
						CONTINUE
				CASE 260 TO 279
					SIF CONFIG("소비아이템系비표시") == 0
						CONTINUE
				CASEELSE
					SIF CONFIG("その他비표시") == 0
						CONTINUE
				ENDSELECT
			ENDIF

			PRINTFORMC %TRAIN_NAME_SELECT(LCOUNT)%[{NUM_TRAIN, 3}]
		ENDIF

		NEXT_LINE += 1

		SIF NEXT_LINE % NUM_COLUMN == 0
			PRINTL 
	NEXT
	SIF NEXT_LINE % NUM_COLUMN
		PRINTL 
ENDIF
RESETCOLOR

CALL SETFLAG, "조수상대の연모外し종료"

;-------------------------------------------------
;특수な커맨드
;-------------------------------------------------
PRINTL 

;HARDで押し倒されているなら, 主導権が無い플래그をセット
IF CONFIG("ＨＡＲＤ") && TEQUIP:넘어뜨리기
	SETBIT FLAG:99, 11
ELSE
	CLEARBIT FLAG:99, 11
ENDIF


NEXT_LINE = 0
VARSET COM_NAMES
VARSET COM_NAMES_HIDDEN

