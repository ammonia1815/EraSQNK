	SIF CONFIG("소비아이템系비표시")
		PRINTC 소비아이템계[312]
	SIF CONFIG("その他비표시")
		PRINTC 그 외[313]
	PRINTL 
	RESETCOLOR
ENDIF

IF CONFIG("조교컨피그모드") == 0
	CALL COM_ABLE500
	SIF RESULT == 0
		SETCOLOR DEF_COLOR("회색")
	PRINTC 아르카나일람[340]
	RESETCOLOR

	CALL COM_ABLE501
	SIF RESULT == 0
		SETCOLOR DEF_COLOR("회색")
	PRINTC 소비아이템일람[341]
	RESETCOLOR

	IF CONFIG("비표시커맨드の표시") == 0
		PRINTC 비표시커맨드를표시[350]
	ELSE
		SETCOLOR DEF_COLOR("파랑")
		PRINTC 비표시커맨드를숨긴다[350]
		RESETCOLOR
	ENDIF
	PRINTL 

	PRINTC 능력표시[400]

	IF CONFIG("항목표시필터：Ｃ") == 0
