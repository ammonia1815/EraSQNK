		SETCOLOR DEF_COLOR("황색")
	PRINTC 특수계[309]
	RESETCOLOR
	SIF CONFIG("촉수系비표시")
		SETCOLOR DEF_COLOR("황색")
	PRINTC 촉수계[310]
	RESETCOLOR
	SIF CONFIG("아르카나系비표시")
		SETCOLOR DEF_COLOR("황색")
	PRINTC 아르카나계[311]
	RESETCOLOR
	PRINTL 

	SIF CONFIG("소비아이템系비표시")
		SETCOLOR DEF_COLOR("황색")
	PRINTC 소비아이템계[312]
	RESETCOLOR
	SIF CONFIG("その他비표시")
		SETCOLOR DEF_COLOR("황색")
	PRINTC 그 외[313]
	RESETCOLOR
	SIF (CONFIG("항목표시필터：Ｂ") == 0 && CONFIG("항목표시필터：Ｃ") == 0)
	    PRINTC 표시커맨드군의반전[320]
	PRINTL

;켜져있는 메뉴만 표시
ELSE
	SETCOLOR DEF_COLOR("황색")
	SIF CONFIG("애무系비표시")
		PRINTC 애무계[300]
	SIF CONFIG("도구系비표시")
		PRINTC 도구계[301]
	SIF CONFIG("Ｖ섹스系비표시")
		PRINTC Ｖ섹스계[302] 

	SIF CONFIG("Ａ섹스系비표시")
		PRINTC Ａ섹스계[303]
	SIF CONFIG("특수섹스系비표시")
		PRINTC 특수섹스계[304]
	SIF CONFIG("봉사系비표시")
		PRINTC 봉사계[305]

	SIF CONFIG("조교자に삽입系비표시")
		PRINTC 조교자에삽입계[306]
	SIF CONFIG("ＳＭ・하드코어系비표시")
		PRINTC ＳＭ·하드코어계[307]
	SIF CONFIG("조수系비표시")
	    PRINTFORMC 조수계[308]

	SIF CONFIG("특수系비표시")
		PRINTC 특수계[309]
	SIF CONFIG("촉수系비표시")
