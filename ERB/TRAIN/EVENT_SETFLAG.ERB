			CASE 9 TO 13
				BASE:ARG:겨털 = 300
			CASE IS >= 14
				BASE:ARG:겨털 = 200
			ENDSELECT
		;보수적なら, お손入れに저항があったり
		ELSEIF TALENT:ARG:보수적 && RAND:10
			BASE:ARG:겨털 = 700
