	IF CONFIG("겨털") && BASE:ARG:겨털 == 0
		;남자なら処理しない
		IF TALENT:ARG:남자 && COND("오토코노코", ARG) == 0
			SELECTCASE AGE(ARG)
			CASE IS >= 4
				BASE:ARG:겨털 = 700
			CASE 3
				BASE:ARG:겨털 = 400
			CASEELSE
				BASE:ARG:겨털 = 100
			ENDSELECT
		;음모が無いなら겨털もありません
		ELSEIF BASE:ARG:음모 == 0 || RAND:20 == 0
			BASE:ARG:겨털 = 100
		;아이や소녀なら, 高확률で無毛orつるつる
		ELSEIF AGE(ARG) <= 2 || (TALENT:ARG:작은몸집 && RAND:3)
			SELECTCASE RAND:10 + (AGE(ARG) - 1)*6
			CASE IS <= 7
