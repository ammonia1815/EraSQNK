	SIF EJAC:ARG <= 0
		EJAC:ARG = 10000

	IF CONFIG("우르즈군없이도대량사정") || TALENT:ARG:일반인
		SELECTCASE BASE:ARG:절정
		CASE IS >= EJAC:ARG*4
			LOCAL = BASE:ARG:절정/(EJAC:ARG*3) + 2
		CASE IS >= EJAC:ARG*2
			LOCAL = 2
		CASE IS >= EJAC:ARG
			LOCAL = 1
		CASEELSE
			RETURNF 0
		ENDSELECT
	ELSE
		SELECTCASE BASE:ARG:절정
		CASE IS >= EJAC:ARG*2
