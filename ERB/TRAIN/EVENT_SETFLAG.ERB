		;Ｖ자위
		SETBIT LOCAL, 1
	ELSEIF LOCAL:4 >= MAX(LOCAL:3, LOCAL:5)
		;Ｂ자위
		SETBIT LOCAL, 3
	ELSEIF LOCAL:3 >= LOCAL:5
		;Ａ자위
		SETBIT LOCAL, 2
	ELSE
		;자위
		SETBIT LOCAL, 4
