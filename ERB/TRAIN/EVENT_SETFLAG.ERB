CASE "절정"
	;사정条件を満たしているか？
	SELECTCASE BASE:ARG:절정
	CASE IS < MAXBASE:ARG:절정
		RETURNF 0
	CASE IS < MAXBASE:ARG:절정*2
		SIF COND("절정許可", PLAYER) == 0 && TEQUIP:ARG:역조교 == 0
			RETURNF 0
	ENDSELECT

