	SIF NOWEX:ARG:사정 || NOWEX:ARG:절정수합계 == 0 || BASE:ARG:요의 < 1000 || TALENT:ARG:남자 || TEQUIP:ARG:확장벌룬 == 1 || COND("요도사용中", ARG)
