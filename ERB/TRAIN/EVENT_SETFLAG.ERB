	FOR LCOUNT, 2, CHARANUM
		SIF TALENT:LCOUNT:연모 == 0 || TALENT:LCOUNT:상사상애 == 0
			CONTINUE
		SIF LOCAL == 0 || CFLAG:LCOUNT:2 > CFLAG:LOCAL:2
			LOCAL = LCOUNT
	NEXT
CASE "다방の給금"
	;다방のバイトで稼ぐ額. 100万までは100分の一. 
	LOCAL:1 = CFLAG:ARG:40 / 100
	SELECTCASE LOCAL:1
	CASE IS < 10000
		LOCAL = LOCAL:1
	CASE IS < 100000
		LOCAL = LOCAL:1/9 + 9000
	CASE IS < 1000000
		LOCAL = LOCAL:1/90 + 19000
	CASEELSE
		LOCAL = 30000
	ENDSELECT
CASEELSE
	THROW %GAMEBASE_YEAR%버젼 BUG : "%ARGS%" 를 찾을 수 없습니다.
ENDSELECT
RETURNF LOCAL

