				LOCAL = 3
			;Ａ섹스
			CASE 70 TO 79
				LOCAL = 4
			;가슴系
			CASE 81, 92
				LOCAL = 5
				SIF CHECK_COM("파이즈리펠라", ARG:1)
					LOCAL = 1
			;니플퍽
			CASE 82
				LOCAL = 18
			;요도삽입
			CASE 83
				LOCAL = 19
			;풋잡系
			CASE 95
				LOCAL = 6
			;스마타や身体系
			CASE 16, 93, 94, 100
				LOCAL = 7
			;키스, 컨닐링구스, 애널핥기など, 입への刺激
			CASE 1, 9
				LOCAL = 16
				SIF TEQUIP:ARG:넘어뜨리기
					LOCAL = 14
			CASE 4, 6
