@GIVE_NAME
#DIM MOTHER
#DIM IDEA
#DIM PREVIDEA

VARSET LOCAL
MOTHER = 0
IF COND("모친") > MASTER
	MOTHER = COND("모친")
	PRINTFORMW %CALLNAME:MOTHER%에게 이름을 생각해보도록 부탁했습니다
ENDIF
PREVIDEA = 0
$START

;名前はジャンルで分ける
