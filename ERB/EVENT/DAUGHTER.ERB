			PRINTFORMW . 분명 동쪽나라를 다스리겠지요…」
		ELSE
			PRINTFORMW 」
		ENDIF
	CASE 29
		PRINTFORMW 「이 아이의 이름은, 푸루푸루～웃♪…이 아니라, %ARGS%, 같은 건 어떨까? 」
	CASE 2, 4, 31, 33, 40, 50, 55
		PRINTFORMW 「에에-……%ARGS%, 귀엽지? 」
	CASE 1, 3, 5, 10, 11, 14, 17, 18, 25, 37, 38, 48, 49
		PRINTFORMW 「그래……%ARGS%, 같은 건 어떨까? 」
	CASEELSE
		PRINTFORMW 상의한 결과, 아이의 이름은 %조사처리(ARGS,"가")% 된 것 같습니다. 
	ENDSELECT
ELSE
	PRINTFORMW 그렇군…%ARGS%, 같은 건 어떨까? 
ENDIF

;ARG, ARG:1は両親. ARGの소질を優先的に得る
@D_TALENT_INHERITANCE, ARG, ARG:1
#DIM LCOUNT
#DIM PATTERN
#DIM GENE, 3

;親が成長済みでない場合, 一時的に成長
VARSET LOCAL
IF !COND("성장끝", ARG)
	FOR LCOUNT, 1, 300
		LOCAL:LCOUNT = TALENT:ARG:LCOUNT
	NEXT
	CALL CALC, "아이の성장", ARG, CFLAG:ARG:484
	LOCAL = 1
ENDIF

;遺伝の処理(성격に関係のないもの)
FOR LCOUNT, 1, 300
	SIF CALCF("遺伝가능な소질", LCOUNT) == 0 || CALCF("성격소질", LCOUNT)
		CONTINUE
	IF RAND:4 == 0 && ARG:1 > 0
		TALENT:LCOUNT = TALENT:(ARG:1):LCOUNT
	ELSE
		TALENT:LCOUNT = TALENT:ARG:LCOUNT
	ENDIF
NEXT
;大마법사は無し
SIF TALENT:마법사 >= 2
	TALENT:마법사 = 1

;親の能力によって○놀림に보너스
FOR LCOUNT, 30, 36
	IF RAND:4 == 0 && ARG:1 > 0
		SIF ABL:(ARG:1):LCOUNT - TALENT:(ARG:1):(150+LCOUNT) >= 5
			TALENT:(150+LCOUNT) += 1
	ELSE
		SIF ABL:ARG:LCOUNT - TALENT:ARG:(150+LCOUNT) >= 5
			TALENT:(150+LCOUNT) += 1
	ENDIF
NEXT

;親から遺伝する속성を1つ선택
GENE:0 = 0
PATTERN = 1
FOR LCOUNT, 0, NUM("속성")
	SIF GETBIT(BASE:ARG:속성, LCOUNT) == 0
		CONTINUE
	SELECTCASE LCOUNT
	CASE 1 TO 7, 10 TO 12, 14
		SIF RAND:PATTERN == 0
			GENE:0 = LCOUNT
		PATTERN += 1
	;ここでは[에로]は出やすい
	CASE 17
		SIF RAND:PATTERN <= 1
			GENE:0 = LCOUNT
		PATTERN += 1
	ENDSELECT
NEXT

;親から遺伝できる속성がない場合, 適当に설정
;20%ほどで랜덤
$CHOICE_ELEMENT1
IF GENE:0 == 0 || RAND:5 == 0
	GENE:0 = CHOICE_NEWELEMENT()
	GOTO CHOICE_ELEMENT1
ENDIF

;25%の확률で성격が반전する
IF RAND:4 == 0
	SELECTCASE GENE:0
	CASE 1
		IF RAND:2
			GENE:0 = 6
		ELSE
			GENE:0 = 7
		ENDIF
	CASE 2
		GENE:0 = 17
	CASE 3
		IF RAND:2
			GENE:0 = 5
		ELSE
			GENE:0 = 11
		ENDIF
	CASE 4
		GENE:0 = 10
	CASE 5
		GENE:0 = 3
	CASE 6
		GENE:0 = 1
	CASE 7
		GENE:0 = 1
	CASE 10
		IF RAND:3
			GENE:0 = 4
		ELSE
			GENE:0 = 14
		ENDIF
	CASE 11
