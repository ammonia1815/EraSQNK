@EVENT_DESKWORK
#DIM LCOUNT
;이벤트대상
#DIM ACTOR
;該当する캐릭터의人数
#DIM NUM_FIND

;まず、メインキャラを選ぶ
ACTOR = 0
NUM_FIND = 0
FOR LCOUNT, 2, CHARANUM
	;登場できるかどうか
	SELECTCASE NO:LCOUNT
	;머메이드は다리無いから…
	CASE 13, 37
		CONTINUE
	;息子は…誰か書いて！
	CASE 94
		CONTINUE
	ENDSELECT


	;요바이가능ぐらいが条件か
	SIF COND("요바이가능", LCOUNT) == 0
		CONTINUE

	NUM_FIND += 1
	SIF RAND:NUM_FIND == 0
		ACTOR = LCOUNT
NEXT

SIF ACTOR == 0
	RETURN 0

CALL EVENT_DESKWORK_MAIN, ACTOR

