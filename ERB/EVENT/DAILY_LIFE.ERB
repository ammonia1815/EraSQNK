@DAILY_LIFE
#DIM LCOUNT
#DIM MEMO_TARGET

;서류작업して無かったら起きるかも
IF FLAG:12 == 0 && RAND:10 == 0 && CHARA_NUM("서류작업")
	CALL EVENT_DESKWORK
	RETURN 1
ENDIF

;キャラ登録数が2이하or조교を行ってない時は引き返す
SIF CHARANUM <= MASTER + 1 || CHARA_NUM("조교경험あり") == 0 || COND("同衾이벤트", MASTER)
	RETURN 1

MEMO_TARGET = TARGET

FOR LCOUNT, 0, MIN(1 + CHARA_NUM("日常이벤트출연가능")/10, 3)
	TARGET = MEMO_TARGET
	CALL DAILY_LIFE_MAIN
NEXT

TARGET = MEMO_TARGET

@DAILY_LIFE_MAIN
#DIM LCOUNT
#DIM GUEST
#DIM GUEST2
#DIM GUEST3
#DIM MEMO_LINECOUNT
;該当する캐릭터의人数
#DIM NUM_FIND
;이벤트が起きる人
#DIM ACTOR

DRAWLINE

$REPLAY0

;まず, メインキャラを選ぶ
ACTOR = 0
NUM_FIND = 0

FOR LCOUNT, 2, CHARANUM
	SIF EXP:LCOUNT:조교횟수 == 0 || COND("日常이벤트출연끝", LCOUNT)
		CONTINUE

	NUM_FIND += 1
	SIF RAND:NUM_FIND == 0
		ACTOR = LCOUNT
NEXT

SIF ACTOR == 0
	RETURN 0

;PRINTFORML ACTOR = [{ACTOR, 2}]%CALLNAME:ACTOR%, TARGET = [{TARGET, 2}]%CALLNAME:TARGET%

;TARGETとASSIの選ばれる확률を優遇してみる
IF RAND:3 == 0 && EXP:조교횟수 && COND("日常이벤트출연끝") == 0
	ACTOR = TARGET
ELSEIF ASSI && RAND:2 && EXP:ASSI:조교횟수 && COND("日常이벤트출연끝", ASSI) == 0
	ACTOR = ASSI
ENDIF

TARGET = ACTOR

;日常이벤트出演済みフラグON
SETBIT CFLAG:95, 4

;"조교한다"を선택された人数を参照する時には, CHARA_NUM("조교경험あり")で分岐させてください. 
;例）조교に呼び出した몽마が3人이상いないとダメ, という条件なら,  CHARA_NUM("조교경험あり") >= 3 を用いる. 

MEMO_LINECOUNT = LINECOUNT
;キャラ登録番号ごとに구상を呼び出す
CALL DAILY_LIFE_MESSAGE
;이벤트発生したならここで終了
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

MEMO_LINECOUNT = LINECOUNT
;NTR이벤트
CALL EVENT_DRAGON_NTR, "일상"
;이벤트発生したならここで終了
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;出演者用フラグ
VARSET GUEST
VARSET GUEST2
VARSET GUEST3

;낮(TIME == 0)と밤(TIME == 1)で分岐することが多い
;체력が低め（MAXの半分or1000の低い方이하）の時は休ませる
IF BASE:체력 <= MIN(MAXBASE:체력/2, 1000)
	;낮の흡혈귀
	IF COND("낮") && TALENT:흡혈귀
		PRINTFORMW 지나는 길에 문득 %CALLNAME:TARGET%의 방을 들여다보았다……
		PRINTFORMW 커튼으로 가려진 깜깜한 방에, 뚜껑이 빈틈없이 닫혀진 관이 놓여져있다
		PRINTFORMW 아마 관 속에서 자고 있겠지……
		RETURN 1
	;낮の일광욕
	ELSEIF COND("낮") && TALENT:일광욕
		;동물귀の時・머메이드は…多分ないと思う
		IF TALENT:동물귀
			PRINTFORMW 지나는 길에, 뜰의 해가 드는 곳에서 %조사처리(CALLNAME:TARGET,"를")% 발견했다……
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 모포를 깔고 그 위에서 몸을 둥글게 말고 자고있다
		ELSE
			PRINTFORMW 지나는 길에, 뜰의 해가 드는 곳에서 자고 있는 %조사처리(CALLNAME:TARGET,"를")% 발견했다……
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 나무에 걸린 해먹에 모포를 깔고 자고있다
		ENDIF
	;밤の월광욕
	ELSEIF COND("밤") && TALENT:월광욕
		PRINTFORMW 문득 창밖에 보이는 지붕 꼭대기에, %조사처리(CALLNAME:TARGET,"가")% 있는 것이 보였다
		PRINTFORMW 달을 올려다보고있다……
		IF MARK:반발각인 == 3
			PRINTFORMW 이쪽을 깨닫고, 모습을 감추었다……
		ELSEIF MARK:굴복각인 >= 2 && RAND:2 == 0
			PRINTFORMW 이쪽을 깨닫고, 미소지어 보였다……
		ELSE
			PRINTFORMW 이쪽을 깨닫고, 요염한 미소를 지었다……
		ENDIF
		RETURN 1
	;동물귀の時
	ELSEIF TALENT:동물귀
		PRINTFORMW 지나는 길에 문득 %CALLNAME:TARGET%의 방을 들여다보았다……
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 침대 위에서 몸을 둥글게 말고 자고있다
	;머메이드の時
	ELSEIF TALENT:머메이드
		PRINTFORMW 지나는 길에 문득 %CALLNAME:TARGET%의 방을 들여다보았다……
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 수조안에서 동동 떠서 자고있다
	;その他
	ELSE
		PRINTFORMW 지나는 길에 문득 %CALLNAME:TARGET%의 방을 들여다보았다……
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 침대에서 자고있는 것같다
	ENDIF

	;반발각인3Lvの時
	IF MARK:반발각인 == 3
		;겁쟁이or悲観的のとき
		IF TALENT:겁쟁이 || TALENT:음기
			PRINTFORMW 그 표정은 슬퍼보인다……
		ELSEIF RAND:2 == 0
			PRINTFORMW 그 표정은 험하다……
		ELSE
			PRINTFORMW 그 표정은 딱딱하다……
		ENDIF
	;연모の時
	ELSEIF TALENT:연모
		IF RAND:8 == 0
			PRINTFORMW 흐트러진 모습이 요염하다……
		;건방짐or소악마の時
		ELSEIF (TALENT:건방짐 || TALENT:소악마) && RAND:7 == 0
			PRINTFORMW 자면서 히죽히죽거리고있다……
			PRINTFORMW ……무슨 꿈을 꾸고 있는 걸까……
		;수수께끼의매력or매혹の時
		ELSEIF (TALENT:매혹 || TALENT:매혹) && RAND:6 == 0
			PRINTFORMW 문득 그 잠자는 얼굴에 매료되어버릴 것같은 기분이 들었다……
		;유치の時
		ELSEIF TALENT:유치 && RAND:5 == 0
			PRINTFORMW 발로차서 흐트러진 모포를 고쳐 덮어 주었습니다
		;라미아の時
		ELSEIF TALENT:라미아 && RAND:4 == 0
			PRINTFORMW 침대에서 비져나온 뱀의 몸이 왠지 섹시해보입니다
		;머메이드の時
		ELSEIF TALENT:머메이드 && RAND:4 == 0
			IF COND("낮")
				PRINTFORMW 창에서 비쳐들어오는 빛이 물안에서 빛나, 어딘가 신비한 분위기마저 느껴졌다……
			ELSE
				PRINTFORMW 촛대의 오렌지 색의 빛 안에서 어딘가 신비한 분위기마저 느껴졌다……
			ENDIF
		ELSEIF RAND:3 == 0
			PRINTFORMW 그 표정은 편안하다……
		ELSEIF RAND:2 == 0
			PRINTFORMW 잠자면서 미소짓고 있는 것처럼 보인다……
			PRINTFORMW 뭔가 꿈을 꾸고 있는 걸까？
		ELSE
			PRINTFORMW 희미하게 잠꼬대로 %조사처리(CALLNAME:MASTER,"를")% 불렀다……
		ENDIF
	;굴복각인Lv2이상
	ELSEIF MARK:굴복각인 >= 2
		IF RAND:8 == 0
			PRINTFORMW 흐트러진 모습이 요염하다……
		;건방짐or소악마の時
		ELSEIF (TALENT:건방짐 || TALENT:소악마) && RAND:7 == 0
			PRINTFORMW 자면서 히죽히죽거리고있다……
			PRINTFORMW ……무슨 꿈을 꾸고 있는 걸까……
		;수수께끼의매력or매혹の時
		ELSEIF (TALENT:매혹 || TALENT:매혹) && RAND:6 == 0
			PRINTFORMW 문득 그 잠자는 얼굴에 매료되어버릴 것같은 기분이 들었다……
		;유치の時
		ELSEIF TALENT:유치 && RAND:5 == 0
			PRINTFORMW 발로차서 흐트러진 모포를 고쳐 덮어 주었습니다
		;라미아の時
		ELSEIF TALENT:라미아 && RAND:4 == 0
			PRINTFORMW 침대에서 비져나온 뱀의 몸이 왠지 섹시해보입니다
		;머메이드の時
		ELSEIF TALENT:머메이드 && RAND:4 == 0
			IF COND("낮")
				PRINTFORMW 창에서 비쳐들어오는 빛이 물안에서 빛나, 어딘가 신비한 분위기마저 느껴졌다……
			ELSE
				PRINTFORMW 촛대의 오렌지 색의 빛 안에서 어딘가 신비한 분위기마저 느껴졌다……
			ENDIF
		ELSEIF RAND:3 == 0
			PRINTFORMW 조용히 자고있는 모양이다……
		ELSEIF RAND:2 == 0
			PRINTFORMW 숨소리에 맞춰 상하로 움직이는 가슴이 요염하다……
		ELSE
			PRINTFORMW 정신없이 자고있다……
			PRINTFORMW 상당히 지쳐있는 것 같다
		ENDIF
	;굴복각인Lv1이하
	ELSE
		IF RAND:3 == 0
			SIF TALENT:머메이드 == 0
				PRINTFORMW 몸을 지키도록(듯이) 모포를 쓰고 있다
			PRINTFORMW 아직 이곳의 생활이 익숙하지 않은 느낌이다
		ELSEIF RAND:2 == 0
			PRINTFORMW 몸을 뒤척이며 등을 돌렸다……
		ELSE
			PRINTFORMW 정신없이 자고있다……
			PRINTFORMW 상당히 지쳐있는 것 같다
		ENDIF
	ENDIF
ELSE
	CALL CALC, "랜덤선택"

	;お風呂
	;SIF (ABL:순종 >= 3 || TALENT:연모) && MARK:반발각인 == 0
	;	CALL CALC, "ランダム선택", 39
	;수유場面に遭遇
	SIF BASE:육아중
		CALL CALC, "랜덤선택", 38
	;羞恥デート이벤트（로터所持, 남자でない非처녀かつ마조끼Lv3이상かつ자위중독3LVかつ스커트かつ주인새드끼Lv3이상, 발がある人限定）
	SIF ITEM:로터 && EQUIP:스커트 && TALENT:처녀 + TALENT:남자 == 0 && MIN(ABL:마조끼, ABL:자위중독, ABL:MASTER:새드끼) >= 3 && COND("다리が無い") == 0
		CALL CALC, "랜덤선택", 37
	;동물귀の時に一緒にお散歩(낮)
	SIF COND("낮") && TALENT:동물귀 && MARK:굴복각인 >= 2 && MARK:반발각인 == 0
		CALL CALC, "랜덤선택", 36
	;みんなでお茶会（3人이상, 현명함, 反発0LV, 호감도500が条件）
	SIF CHARA_NUM("조교경험あり") >= 3 && TALENT:현명함 && CFLAG:2 >= 500 && MARK:반발각인 == 0
		CALL CALC, "랜덤선택", 35
	;육아중の時, 아이登場…
	SIF BASE:육아중
		CALL CALC, "랜덤선택", 34
	;아이で, 임신していないなら
	SIF NO:TARGET == 97 && !COND("성장끝") && TALENT:임신 == 0
		CALL CALC, "랜덤선택", 33
	;금단의지식の時, 妖しい儀式を……
	SIF TALENT:금단의지식
		CALL CALC, "랜덤선택", 32
	;가창기능持ちの時, 歌の練習を……
	SIF TALENT:가창기능
		CALL CALC, "랜덤선택", 31
	;광기の時, ぶつぶつ言ってみる
	SIF TALENT:광기
		CALL CALC, "랜덤선택", 30
	;마법사の時, 마법の儀式をorタロット占い
	SIF TALENT:마법사
		CALL CALC, "랜덤선택", 29
	;조합지식持ちの時, 実験をしてみるor原料育成
	SIF TALENT:조합지식
		CALL CALC, "랜덤선택", 28
	;조합지식, 大人しいの時本を読ませてみる
	SIF TALENT:내성적 || TALENT:조합지식
		CALL CALC, "랜덤선택", 27
	;오줌싸개버릇の時, 世界地図を広げる(낮)
	SIF COND("낮") && TALENT:흡혈귀 == 0 && TALENT:오줌싸개버릇
		CALL CALC, "랜덤선택", 26
	;일광욕の時, 일向ぼっこ(낮)
	SIF COND("낮") && TALENT:일광욕
		CALL CALC, "랜덤선택", 25
	;월광욕の時, 월見（밤）
	SIF COND("밤") && TALENT:월광욕
		CALL CALC, "랜덤선택", 24
	;동물귀の時, 庭を歩いてみる
	SIF TALENT:동물귀
		CALL CALC, "랜덤선택", 23
	;바늘다루기持ちのとき, お裁縫
	SIF TALENT:바늘다루기
		CALL CALC, "랜덤선택", 22
	;헌신的のとき, 家事をさせてみる
	SIF TALENT:헌신
		CALL CALC, "랜덤선택", 21
	;헌신的, 치료もちの場合お祈りしてみる
	SIF COND("낮") && TALENT:흡혈귀 == 0 && (TALENT:헌신 || TALENT:치료)
		CALL CALC, "랜덤선택", 20
	;감정결핍or무관심の時はぼーっとしてみる
	SIF TALENT:무관심 || TALENT:감정결핍
		CALL CALC, "랜덤선택", 19
	;눈에띄고싶어함のとき, 踊ってみる
	SIF TALENT:눈에띄고싶어함 && MARK:반발각인 != 3 && CHARA_NUM("조교경험あり") >= 2
		CALL CALC, "랜덤선택", 18
	;겁쟁이・꿋꿋함の時, 밤間見回り中に遭遇する
	SIF COND("밤") && (TALENT:겁쟁이 || TALENT:꿋꿋함)
		CALL CALC, "랜덤선택", 17
	;고양이혀の時, 목に昇ってみるor개다래ワインで酔っ払ってみる
	SIF COND("낮") && TALENT:흡혈귀 == 0 && TALENT:고양이혀
		CALL CALC, "랜덤선택", 16
	;반항적・솔직の時, ちょっとした用事を頼まれてみる
	SIF TALENT:반항적 || TALENT:솔직
		CALL CALC, "랜덤선택", 15
	;요정の時, 花壇で遊んでみる
	SIF COND("낮") && TALENT:흡혈귀 == 0 && TALENT:요정
		CALL CALC, "랜덤선택", 14
	;とったビデオを見る…変数P条件にして簡易にやっちゃうかな…부끄럼쟁이の時
	SIF TALENT:부끄럼쟁이 && ABL:욕망 >= 2 && P > 0
		CALL CALC, "랜덤선택", 13
	;부끄럼없음の時…裸でうろつく
	SIF TALENT:부끄럼없음
		CALL CALC, "랜덤선택", 12
	;호기심の時…珍しいものに心をとらわれる
	SIF TALENT:호기심
		CALL CALC, "랜덤선택", 11
	;습득遅い, 유치の時 本を読んでみるけど寝ちゃうorおやつをつまみ食いする
	SIF TALENT:미련함 || TALENT:유치
		CALL CALC, "랜덤선택", 10
	;습득早いが練習しているor습득遅いもちに何か教える…
	SIF TALENT:현명함 && MARK:반발각인 != 3
		CALL CALC, "랜덤선택", 9
	;새드が, SM조교の練習をしている
	SIF TALENT:새드
		CALL CALC, "랜덤선택", 8
	;楽観的が, 鼻노래한다or悲観的を慰めている…
	SIF TALENT:양기
		CALL CALC, "랜덤선택", 7
	;悲観的が, 悲しげに外を見つめている
	SIF TALENT:음기
		CALL CALC, "랜덤선택", 6
	;자위중독の時, 자위したり
	SIF COND("身嗜み：자위금지") == 0 && ABL:자위중독 >= RAND:6
		CALL CALC, "랜덤선택", 5
	;순종Lv3이상+待ち焦がれている+お낮のとき, お買い物ネタ
	SIF COND("낮") && ABL:순종 >= 3 && EXP:Ｖ경험 >= 20 && COND("욕구불만") && MARK:반발각인 != 3
		CALL CALC, "랜덤선택", 4
	;순종Lv3이상のとき, 料理をつくる・いろいろ分岐
	SIF ABL:순종 >= 3 && MARK:반발각인 != 3
		CALL CALC, "랜덤선택", 3
	;계절ネタ（낮）
	SIF COND("낮")
		CALL CALC, "랜덤선택", 2
	;自室でポージング
	SIF TALENT:남자 == 0
		CALL CALC, "랜덤선택", 1

	SELECTCASE RESULT
	;お風呂
	CASE 39

	;수유場面に遭遇
	CASE 38
		IF RAND:2
			LOCALS = 아이
		ELSEIF COND("육아중：아들")
			LOCALS = 아들
		ELSE
			LOCALS = 딸
		ENDIF

		IF TALENT:거유 >= 2
			LOCALS:1 = 아이의 머리보다도 커다랗게 보이는 가슴
		ELSEIF TALENT:거유
			LOCALS:1 = 커다란 가슴
		ELSEIF TALENT:빈유
			LOCALS:1 = 평탄한 가슴
		ELSE
			LOCALS:1 = 가슴
		ENDIF

		PRINTFORM %조사처리(CALLNAME:MASTER,"가")% 관내를 
		IF RAND:2
			PRINTFORM 산책하고 있을 때, 
