	SIF TALENT:ARG:남자 == 0
		EXP:ARG:절정경험 += ARG:1
	IF TALENT:ARG:처녀 == 0 && ABL:ARG:Ｖ감각 >= 3 && EXP:ARG:Ｖ성교경험 >= MAX(20, EXP:ARG:Ａ성교경험/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		;MASTERが페니스持ちなら경험多め
		SIF PENIS(MASTER) == 0 || EXP:MASTER:Ｖ삽입경험 < MAX(20, EXP:MASTER:Ａ삽입경험/4)
			LOCAL /= 3
		EXP:ARG:Ｖ경험 += LOCAL
		EXP:ARG:Ｖ성교경험 += LOCAL
		EXP:ARG:허리사용경험 += LOCAL

		SELECTCASE ARG
		CASE TARGET
			IF PENIS(PLAYER)
				EXP:ARG:정액경험 += LOCAL
				CALL SETFLAG, "질내정액경험", LOCAL/2, ARG
			ENDIF
		CASE PLAYER
			IF PENIS(TARGET)
				EXP:ARG:정액경험 += LOCAL
				CALL SETFLAG, "질내정액경험", LOCAL/2, ARG, TARGET
			ENDIF
		ENDSELECT

		IF PENIS(MASTER) && EXP:MASTER:Ｖ삽입경험 >= MAX(20, EXP:MASTER:Ａ삽입경험/4)
			EXP:MASTER:Ｖ삽입경험 += LOCAL/2
			EXP:MASTER:허리사용경험 += LOCAL/2
			EXP:MASTER:사정경험 += LOCAL/3
		ENDIF
	ENDIF
	IF TALENT:ARG:Ａ처녀 == 0 && ABL:ARG:Ａ감각 >= 3 && EXP:ARG:Ａ성교경험 >= MAX(20, EXP:ARG:Ｖ성교경험/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		;MASTERが페니스持ちなら경험多め
		SIF PENIS(MASTER) == 0 || EXP:MASTER:Ａ삽입경험 < MAX(20, EXP:MASTER:Ｖ삽입경험/4)
			LOCAL /= 3
		EXP:ARG:Ａ경험 += LOCAL
		EXP:ARG:Ａ성교경험 += LOCAL
		EXP:ARG:허리사용경험 += LOCAL

		SELECTCASE ARG
		CASE TARGET
			IF PENIS(PLAYER)
				EXP:ARG:정액경험 += LOCAL
				CALL SETFLAG, "장내정액경험", LOCAL/2, ARG
			ENDIF
		CASE PLAYER
			IF PENIS(TARGET)
				EXP:ARG:정액경험 += LOCAL
				CALL SETFLAG, "장내정액경험", LOCAL/2, ARG, TARGET
			ENDIF
		ENDSELECT

		IF PENIS(MASTER) && EXP:MASTER:Ａ삽입경험 >= MAX(20, EXP:MASTER:Ｖ삽입경험/4)
			EXP:MASTER:Ａ삽입경험 += LOCAL/2
			EXP:MASTER:허리사용경험 += LOCAL/2
			EXP:MASTER:사정경험 += LOCAL/3
		ENDIF
	ENDIF
	IF PENIS(ARG) && EXP:ARG:Ｖ삽입경험 >= MAX(20, EXP:ARG:Ａ삽입경험/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		EXP:ARG:Ｖ삽입경험 += LOCAL
		EXP:ARG:허리사용경험 += LOCAL
		EXP:ARG:사정경험 += LOCAL/3
	ENDIF
	IF PENIS(ARG) && EXP:ARG:Ａ삽입경험 >= MAX(20, EXP:ARG:Ｖ삽입경험/4)
		LOCAL = RAND:(1 + ARG:1) + ARG:1
		EXP:ARG:Ａ삽입경험 += LOCAL
		EXP:ARG:허리사용경험 += LOCAL
		EXP:ARG:사정경험 += LOCAL/3
	ENDIF
	SIF ABL:ARG:ＢＬ끼 >= 3 || TALENT:ARG:남자 == 0
		EXP:ARG:펠라경험 += RAND:(1 + ARG:1)
	SIF ABL:ARG:레즈끼 || TALENT:ARG:남자
		EXP:ARG:커닐링구스경험 += RAND:(1 + ARG:1)
	SIF COND("손가락 사용", ARG)
		EXP:ARG:수음경험 += RAND:(1 + ARG:1)
	SIF COND("다리 사용", ARG) && ABL:ARG:발놀림
		EXP:ARG:풋잡경험 += RAND:(1 + ARG:1)
	SIF TALENT:ARG:남자 == 0 && TALENT:ARG:빈유 == 0 && ABL:ARG:마유
		EXP:ARG:파이즈리경험 += RAND:(1 + ARG:1)
	SIF ABL:ARG:새드끼 && EXP:ARG:Ａ자극경험 >= 20
		EXP:ARG:Ａ자극경험 += RAND:(1 + ARG:1)
ENDSELECT

SIF SELECTCOM < 0
	RETURN 0

SIF ARG != PLAYER
	SWAP TARGET, ARG

CALL CALC, "경험취득", SELECTCOM, ARG:1

SIF ARG != PLAYER
	SWAP TARGET, ARG
