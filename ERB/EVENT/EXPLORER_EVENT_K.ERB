@KOJO_MESSAGE_SEXBATTLE_A_1
IF RAND:2 == 0
	PRINTFORML 「후후후…안녕♪」
ELSE
	IF PALAM_EXPLORER("敵キャラ数") > 1
		IF RAND:2 == 0
			PRINTFORML 「언니들이랑 같이 놀아요♪」
		ELSE
			PRINTFORML 「후후, 빙글빙글돌아요오♪」
		ENDIF
	ELSE
		PRINTFORML 「언니랑 같이 놀아요♪」
	ENDIF
ENDIF

@KOJO_MESSAGE_SEXBATTLE_B_1
;몽마がイかされた
IF BASE:정력 <= 0
	IF PALAM_EXPLORER("Ｖ인서트")
		IF RAND:5 == 0
			PRINTFORML 「좀 더, 좀 더 안까지 밀어넣어줘어♪ ……으으응!! ♪」
		ELSEIF RAND:4 == 0
			PRINTFORML 「안에, 부딛쳐져서어, 녹아버려어어♪」
		ELSEIF RAND:3 == 0
			PRINTFORML 「앗, 아앙! 기분 너무 좋아서…이젠 안돼, 가버려어♪」
		ELSEIF RAND:2 == 0
			PRINTFORML 「아아, 가앗, 가버려어엇!」
		ELSE
			PRINTFORML 「앗, 으응, 다, 안됏! 가! 가버려엇!」
		ENDIF
	ELSEIF PALAM_EXPLORER("Ａ인서트")
		IF RAND:3 == 0
			PRINTFORML 「응웃, 굉장한 것!  나의엉덩이의 안…읏, 당신의 자지의 형태로 되어 버린다아♪」
		ELSEIF RAND:2
			PRINTFORML 「아앗, 느껴 버린다!  보지보다 훨씬 기분이 좋다…, 아아앗♪」
		ELSE
			PRINTFORML 「앗, 으응, 다, 안됏! 가! 가버려엇!」
		ENDIF
	;ELSEIF PALAM_EXPLORER("에이스오브스페이드")
	ELSEIF PALAM_EXPLORER("ヴァッジ")
		;컨닐링구스
		IF R == 2
			PRINTFORML 「앗, 아앗, 아힛, , 기다려, 클리토리스씹습니다, 아히이이이♪」
		;손가락で애무
		ELSE
			PRINTFORML 「아응…앗! 거, 거긴…약해애♪」
		ENDIF
	ELSEIF PALAM_EXPLORER("애널")
		;애널핥기
		IF R == 2
			PRINTFORML 「앗, 히잇, 하와아아앙♪」
		;손가락で애무
		ELSE
			PRINTFORML 「싫엇, 엉덩이 따위로, 그러……언」
		ENDIF
	ELSEIF PALAM_EXPLORER("바스트")
		;유두舐め
		IF R == 2
			PRINTFORML 「응아앗, 흐아…그렇게 꼭지만 괴롭히지 말아…♪」
		;가슴揉み
		ELSE
			PRINTFORML 「가슴 녹아버려, 녹아버려엇♪」
		ENDIF
	ELSEIF PALAM_EXPLORER("키스")
		PRINTFORML 「하아……응읏, 무우, 추으읍, 츄릇……푸앗, 하아하아, 굉장, 해……♪」
	;ここは上が全部書かれていると喋らないが, 分岐のどれかをコメントアウトさせた時などに使用する
	ELSE
		PRINTFORML 「앗, 아-앙!  ㄴ, 나앗, 이젠, 안돼애…♪」
	ENDIF
;탐색側をイかせた
ELSE
	IF PALAM_EXPLORER("Ｖ인서트")
		IF RAND:3 == 0
			PRINTFORML 「응아…굉장한거네, 자궁에 끈적했던 것이 모여 있는 것이 알아요…♪」
		ELSEIF RAND:2 == 0
			PRINTFORML 「응…읏♪ 가득 나와 버렸어요…♪」
		ELSE
			PRINTFORML 「아핫, 굉장한 양이네…그렇게 기분좋았어? 」
		ENDIF
	ELSEIF PALAM_EXPLORER("Ａ인서트")
		PRINTFORML 「뜨거웟!  엉덩이중(안)에서 내다니……변태군요♪」
	ELSEIF PALAM_EXPLORER("에이스오브스페이드")
		PRINTFORML 「당신의 국물과 나의 국물로 질척질척이군요…♪」
	;ELSEIF PALAM_EXPLORER("키스")
	ELSEIF PALAM_EXPLORER("파이즈리")
		IF RAND:3 == 0 || TALENT:PLAYER:여자
			PRINTFORML 「어때？ 기분이 좋지요, 나의 젖가슴♪」
		ELSEIF RAND:2 == 0
			PRINTFORML 「푸앗, 이렇게 많이…아하아, 질척질척 하고 있어요」
		ELSE
			PRINTFORML 「왓, 언제까지나 뷰뷰 나와…우훗, 젖가슴진하양(이)예요♪」
		ENDIF
	;ELSEIF PALAM_EXPLORER("애널자극")
	ELSEIF PALAM_EXPLORER("안면기승")
		IF RAND:2 == 0
			PRINTFORML 「나의 농후한 보지의%TEXTS("匂い")%, 뇌수에 새겨 아·게·르♪」
		ELSE
			PRINTFORML 「몽롱해진 의식으로 바라보는 나의에로또한 보지……멋진 경치겠지？ 후후♪」
		ENDIF
	ELSEIF PALAM_EXPLORER("손훑기")
		IF RAND:2 == 0 && TALENT:PLAYER:여자 == 0
			PRINTFORML 「아핫, 팬티가 정액투성이♪」
		ELSEIF RAND:2 == 0
			PRINTFORML 「후후,  대딸따위로 가버리면 아까운걸? 」
		ELSE
			PRINTFORML 「그렇게 내 손이 기분 좋았던걸까나♪」
		ENDIF
	ELSEIF PALAM_EXPLORER("펠라치오")
		IF MAXBASE:PLAYER:정력 > 30 && RAND:2 == 0
			PRINTFORML 「!？ 읏!  우에에, 쿨럭, 쿨럭……굉장한 양이군요♪」
		ELSE
			PRINTFORML 「으응, …꿀꺽, …푸앗……후후, 잘먹었습니다♪」
		ENDIF
	ELSEIF PALAM_EXPLORER("풋잡")
		PRINTFORML 「후후, 터무니없는 변태씨네…♪」
	;ELSEIF PALAM_EXPLORER("식스나인")
	;ここは上が全部書かれていると喋らないが, 他の分岐のどれかをコメントアウトさせた時などに使用する
	ELSE
		IF RAND:2 == 0
			PRINTFORML 「당신의 가버린 얼굴…귀엽네에♪」
		ELSE
			PRINTFORML 「어머, 벌써 기브업?  우후후♪」
		ENDIF
	ENDIF
ENDIF

