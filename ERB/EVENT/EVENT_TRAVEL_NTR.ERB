;--------------------------------------------------------
;浮気中の経験算出・表示
;総질내사정回数か욕망を元に経験を算出
;ARGSは플레이内容・상대, ARGは総질내사정回数
;輪姦・난교系は総질내사정回数で, その他は욕망で算出
;--------------------------------------------------------
@EXP_CALC_TRAVEL_UWAKI, ARGS, ARG
#DIM LCOUNT

VARSET LOCAL
;計算
SELECTCASE ARGS
CASE "輪姦난교"
	;0,Ｖ경험
	SIF TALENT:남자 == 0
		LOCAL:0 = ARG
	;1,Ａ경험
	LOCAL:1 = ARG / 2
	;2,절정경험
	LOCAL:2 = ARG
	;3,사정경험
	SIF TALENT:후타나리
		LOCAL:3 = ARG / 3
	;12,Ｖ성교경험
	LOCAL:12 = LOCAL:0
	;13,Ａ성교경험
	LOCAL:13 = LOCAL:1
	;17,질내정액경험
	LOCAL:17 = ARG
	;18,장내정액경험
	LOCAL:18 = LOCAL:1
	;19,정음경험
	LOCAL:19 = (ARG * 5) / 3
	;20,정액경험
	LOCAL:20 = LOCAL:17 + LOCAL:18 + LOCAL:19 + LOCAL:22 + LOCAL:23 + LOCAL:26
	;22,펠라경험
	LOCAL:22 = LOCAL:19 
	;23,수음경험
	LOCAL:23 = ARG * 3 / 2
	;25,허리사용경험
	LOCAL:25 = LOCAL:12 + LOCAL:13
	;26,파이즈리경험
	LOCAL:26 = ARG * 3 / 2
	;54,분유경험
	SIF TALENT:모유체질
		LOCAL:54 = ARG * 3 / 2
	;70,키스경험
	LOCAL:70 = ARG * 3 / 4
CASE "애널"
	;1,Ａ경험
	LOCAL:1 = ABL:욕망 / 2
	;2,절정경험
	LOCAL:2 = ABL:욕망
	;3,사정경험
	SIF TALENT:후타나리
		LOCAL:3 = ABL:욕망 / 3
	;13,Ａ성교경험
	LOCAL:13 = LOCAL:1
	;18,장내정액경험
	LOCAL:18 = LOCAL:1
	;20,정액경험
	LOCAL:20 = LOCAL:18
	;54,분유경험
	SIF TALENT:모유체질
		LOCAL:54 = ABL:욕망 * 3 / 2
CASE "거근쇼타"
	;0,Ｖ경험
	LOCAL:0 = ABL:욕망
	;2,절정경험
	LOCAL:2 = ABL:욕망
	;3,사정경험
	SIF TALENT:후타나리
		LOCAL:3 = ABL:욕망 / 3
	;12,Ｖ성교경험
	LOCAL:12 = LOCAL:0
	;17,질내정액경험
	LOCAL:17 = ABL:욕망
	;19,정음경험
	LOCAL:19 = (ABL:욕망 * 5) / 3
	;20,정액경험
	LOCAL:20 = LOCAL:17 + LOCAL:18 + LOCAL:19 + LOCAL:22 + LOCAL:23 + LOCAL:26
	;22,펠라경험
	LOCAL:22 = LOCAL:19 
	;23,수음경험
	LOCAL:23 = ABL:욕망 * 3 / 2
	;25,허리사용경험
	LOCAL:25 = LOCAL:12 + LOCAL:13
	;26,파이즈리경험
	LOCAL:26 = ABL:욕망 * 3 / 2
	;53,Ｖ확장경험
	LOCAL:53 = LOCAL:0
	;54,분유경험
	SIF TALENT:모유체질
		LOCAL:54 = ABL:욕망 * 3 / 2
	;70,키스경험
	LOCAL:70 = ABL:욕망 * 3 / 4
;浮気などその他
CASEELSE
	;0,Ｖ경험
	LOCAL:0 = ABL:욕망
	;2,절정경험
	LOCAL:2 = ABL:욕망
	;3,사정경험
	SIF TALENT:후타나리
		LOCAL:3 = ABL:욕망 / 3
	;12,Ｖ성교경험
	LOCAL:12 = LOCAL:0
	;17,질내정액경험
	LOCAL:17 = ABL:욕망
	;19,정음경험
	LOCAL:19 = (ABL:욕망 * 5) / 3
	;20,정액경험
	LOCAL:20 = LOCAL:17 + LOCAL:18 + LOCAL:19 + LOCAL:22 + LOCAL:23 + LOCAL:26
	;22,펠라경험
	LOCAL:22 = LOCAL:19 
	;23,수음경험
	LOCAL:23 = ABL:욕망 * 3 / 2
	;25,허리사용경험
	LOCAL:25 = LOCAL:12 + LOCAL:13
	;26,파이즈리경험
	LOCAL:26 = ABL:욕망 * 3 / 2
	;54,분유경험
	SIF TALENT:모유체질
		LOCAL:54 = ABL:욕망 * 3 / 2
	;70,키스경험
	LOCAL:70 = ABL:욕망 * 3 / 4
ENDSELECT

;表示と加算
PRINTFORML  [%CALLNAME:TARGET%]
FOR LCOUNT, 0, 100
	SIF LOCAL:LCOUNT <= 0
		CONTINUE
	;表示するものを선택
	SELECTCASE LCOUNT
	;0,Ｖ경험, 1,Ａ경험, 2,절정경험, 3,사정경험, 12,Ｖ성교경험, 13,Ａ성교경험, 17,질내정액경험, 18,장내정액경험
	;19,정음경험, 20,정액경험, 22,펠라경험, 23,수음경험, 25,허리사용경험, 26,파이즈리경험, 54,분유경험, 70,키스경험
	CASE 0 TO 3, 12, 13, 17 TO 20, 22, 23, 25, 26, 54, 70
		PRINTFORML %TEXT_RJ(EXPNAME:LCOUNT, 14)% + {LOCAL:LCOUNT}
