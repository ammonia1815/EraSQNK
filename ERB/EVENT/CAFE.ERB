	CALL CALC, "랜덤선택", 7
;テーブルを拭く時に가슴が…
SIF TFLAG:8 == 0 && TALENT:남자 == 0 && TALENT:요정 == 0
	CALL CALC, "랜덤선택", 8
;頬をクリームで汚したお손님にサービス
SIF TFLAG:9 == 0 && ABL:봉사정신 >= 3
	CALL CALC, "랜덤선택", 9
