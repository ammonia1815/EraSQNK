;-------------------------------------------------
;다방
;#eraSQ製作スレＩＲＣメンバー合作! ｗ
;-------------------------------------------------
@SHOP_EX
#DIM LCOUNT
;FLAG:400 店舗数
;FLAG:401 제복の情報
;SAVESTR:1 各店舗の名前　デフォはDream Goblet

PRINTL 
PRINTL 길들인 몽마를 점원으로 해서, 귀여운 제복을 내세운 다방을 운영합니다
PRINTL 수입은 일요일 아침에 일주일분을 한꺼번에 얻습니다
PRINTL 점포의 통로는 넓기 때문에, 라미아등의 신체의 큰 몽마에서도 문제 없게 일할 수 있고,
PRINTL 일하기 힘든 모습의 몽마에게는, 형태를 인간형으로 바꾸는 마법을 건다거나, 걸지 않는다거나 합니다
PRINTL 또한, 남자지만 보기에 아름다운 용모라면 일을 시킵니다만, 손님의 꿈을 부수지 않게 배려해, 
PRINTL 중성적인 외모를 가진 사람에 한해, 여성스탭과 같은 복장으로 웨이트리스로 일하게 합니다
PRINTFORML

VARSET LOCAL
MONEY:8 = 0

;다방が稼動しているとき
IF NUM("運営店舗")
	FOR LCOUNT, 2, CHARANUM
		IF COND("플로어점원", LCOUNT)
			CALL SETFLAG, "매각価格", LCOUNT
			MONEY:8 += CALCF("다방の給금", LCOUNT)
		ENDIF
	NEXT
	LOCAL = (MONEY:8 * 14 * NUM("運営店舗") ) / CHARA_NUM("플로어점원")/1000*1000
	PRINTFORML 점원수：{CHARA_NUM("플로어점원")}명 운영가능점포수：{NUM("運営店舗")} 예상수입:%MONEY_C(LOCAL)%/주
ELSEIF NUM("所持店舗")
	PRINTFORML 점원수：{CHARA_NUM("플로어점원")}명…점원이 부족합니다
ELSE
	PRINTFORM 점원수：{CHARA_NUM("バイト가능")}명 운영가능점포수：{NUM("運営가능店舗")} 
	FLAG:400 += 1
	IF CHARA_NUM("플로어점원") > 0
		FOR LCOUNT, 2, CHARANUM
			IF COND("플로어점원", LCOUNT)
				CALL SETFLAG, "매각価格", LCOUNT
				MONEY:8 += CALCF("다방の給금", LCOUNT)
			ENDIF
		NEXT
		LOCAL = (MONEY:8 * 14 * NUM("運営店舗") ) / CHARA_NUM("플로어점원")/1000*1000
	ENDIF
	FLAG:400 -= 1
	PRINTFORML 예상수입:%MONEY_C(LOCAL)%/주
ENDIF
PRINTL 

SIF NUM("所持店舗") < NUM("運営가능店舗")
	PRINTFORML  [ 0] 새 가게를 차린다 （$10,000） 소지금:%MONEY_C(MONEY)%
IF NUM("所持店舗")
	PRINTFORML  [ 1] 가게이름을 변경   현재：%NAMES("다방")%
	PRINTFORML  [ 2] 제복을 변경   현재：%TEXTS("제복")%
	;営業方바늘はとりあえず凍結
	;PRINTFORML  [ 3] 영업 방침을 변경　 현재：%TEXTS("영업 방침")%
	PRINTFORML  [98] 이벤트 빈도 변경 현재：\@ CONFIG("다방描写") ?  시간 경과 때에 발생 # 발생하지 않는다 \@
ENDIF
SIF CHARA_NUM("バイト가능")
	PRINTL  [99] 점원 일람＆지시
PRINTL [100] 돌아온다

$INPUT_LOOP
INPUT

IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 98 && FLAG:400
	CALL SETCONFIG, "다방"
ELSEIF RESULT == 99 && CHARA_NUM("バイト가능")
	CALL CAFE_LIST, 0, 0
ELSEIF RESULT == 0 && NUM("所持店舗") < NUM("運営가능店舗")
	IF MONEY >= 10000
		IF NUM("所持店舗") == 0
			SAVESTR:1 = Dream Goblet
			FLAG:401 = 1p0
			PRINTFORML 초기설정에서는, 가게이름은 %NAMES("다방")%, 제복은 %TEXTS("제복")%으로 되어있습니다
			PRINTFORMW 원하는 내용을 변경해서 즐겨주세요
		ELSE
			PRINTFORMW 점포를 한개 추가했습니다
		ENDIF
		MONEY -= 10000
		FLAG:400 += 1
	ELSE
		PRINTFORMW 금액이 부족합니다. 
	ENDIF
ELSEIF RESULT == 1 && NUM("所持店舗")
	PRINTFORML 다방의 이름을 「%NAMES("다방")%」에서 변경합니다
	PRINTFORML 이름을 입력해주세요（아무 것도 입력하지 않으면 변경하지 않습니다）
	CALL INPUTS_SELECT, 24
	IF RESULTS != "" && SAVESTR:1 != RESULTS
		SAVESTR:1 = %RESULTS%
		PRINTFORMW 가게 이름을 %NAMES("다방")% 개명했습니다
	ENDIF
ELSEIF RESULT == 2 && NUM("所持店舗")
	CALL SETFLAG, "다방の복장"
ELSE
	GOTO INPUT_LOOP
ENDIF

RESTART

@CAFE
#DIM LCOUNT
#DIM MEMO_TARGET
#DIM MEMO_LINECOUNT, 2

;ここではTARGETを変更させるので保存
MEMO_TARGET = TARGET
TARGET = 0

;同じ이벤트は起こさない
VARSET TFLAG
;勤労플래그初期化(一일二回あるので)
FOR LCOUNT, 1, CHARANUM
	CLEARBIT CFLAG:LCOUNT:95, 1
NEXT

IF CONFIG("다방描写")
	PRINTL 
	CUSTOMDRAWLINE ☆

	;挨拶。LINECOUNTを記録して、キャラが喋ったかを判別させる
	MEMO_LINECOUNT = LINECOUNT

	;喋る캐릭터의選別
	FOR LCOUNT, 1, MAX(4, NUM("運営店舗") + 1)
		;挨拶していない人から選出
		TARGET = FIND_COND("출연가능な점원", "다방挨拶まだ")

		MEMO_LINECOUNT:1 = LINECOUNT
		;台詞有る場合に, 台詞の名前表示をする？
		SIF CONFIG("대사の名前비표시") == 0
			PRINTFORM %TEXT_LJ(CALLNAME:TARGET, 16)%

		CALL SELF_KOJO, "다방"

		;挨拶フラグ
		SETBIT CFLAG:95, 6

		;台詞が無いなら辻褄あわせ
		IF LINECOUNT == MEMO_LINECOUNT:1
			PRINTL 
			CLEARLINE 1
		ENDIF
	NEXT
	IF LINECOUNT > MEMO_LINECOUNT
		WAIT
		PRINTL 
	ENDIF
ENDIF

FOR LCOUNT, 1, NUM("運営店舗") + 1
	;FIND_CONDはTARGET以外から探すので一旦リセット
	TARGET = 0
	;挨拶済みを優遇
	TARGET = FIND_COND("출연가능な점원", "다방挨拶済み")
	SIF TARGET == 0
		TARGET = FIND_COND("출연가능な점원")

	;収入get
	CALL SETFLAG, "매각価格", TARGET
	MONEY:7 += CALCF("다방の給금", TARGET)

	;이벤트描写플래그ON
	SETBIT CFLAG:95, 1
	IF CONFIG("다방描写")
		SIF LCOUNT >= 2
			PRINTL 

		CALL EVENT_CAFE, LCOUNT
	ENDIF
NEXT

IF CONFIG("다방描写")
	CUSTOMDRAWLINE ☆
	PRINTL 
ENDIF

TARGET = MEMO_TARGET

