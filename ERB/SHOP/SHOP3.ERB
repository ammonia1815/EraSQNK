			CALL SETFLAG, "힙설정", TARGET
		CASE "폭엉덩이"
			SIF COND("큰엉덩이") != 2
				CONTINUE

			IF RAND:2
				FOR LCOUNT2, 0, 100
					SIF COND("큰엉덩이") != 2
