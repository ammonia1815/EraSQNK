		TALENT:상위몽마 = 1
	ENDIF
;뱀파이어系
CASE 25
	IF ARG:1 == 2
		CFLAG:8 = 2
		ABL:욕망 += 2
		ABL:기교 += 2
		ABL:레즈끼 += 1
		TALENT:몽마 = 0
		TALENT:상위몽마 = 1
	ENDIF
;유니콘系
CASE 30
	IF ARG:1 == 1
		CFLAG:8 = 1
		ABL:Ａ감각 = ABL:Ｖ감각
		ABL:Ｖ감각 = 0
		TALENT:Ｖ민감 = 0
		TALENT:Ａ민감 = 1
		TALENT:호색 = 1
		TALENT:치료 = 0
		TALENT:고무 = 1
		TALENT:로긴 = 0
		TALENT:우르즈군 = 1
	ENDIF
;미믹系
CASE 33
	IF ARG:1 == 4
		CFLAG:8 = 4
		ABL:욕망 += 2
		ABL:기교 += 2
		ABL:정액중독 += 1
		TALENT:몽마 = 0
		TALENT:상위몽마 = 1
	ENDIF
CASE 44
	IF ARG:1 == 4
		CFLAG:8 = 4
		MAXBASE:체력 = 2500
		BASE:체력 = MAXBASE:체력
		MAXBASE:기력 = 2200
		BASE:기력 = MAXBASE:기력
		ABL:욕망 += 1
		ABL:기교 = 8
		ABL:봉사정신 += 3
		ABL:Ｃ감각 += 2
		ABL:Ｖ감각 += 2
		EXP:Ｖ경험 += 50
		EXP:자위경험 += 5
		EXP:Ｖ성교경험 += 30
		EXP:정액경험 += 20
		EXP:펠라경험 += 30
		EXP:수음경험 += 30
		EXP:풋잡경험 += 10
		EXP:허리사용경험 += 20
		EXP:파이즈리경험 += 15
		EXP:Ａ자극경험 += 10
		EXP:커닐링구스경험 += 10
	ENDIF
ENDSELECT

;둔감を得たり
IF COND("유니크") == 0 && RAND:3 == 0 && ARGS != "애널라이즈"
	IF TALENT:Ｃ민감 == 0 && RAND:10 == 0
		TALENT:Ｃ둔감 = 1
		ABL:Ｃ감각 = MAX(ABL:Ｃ감각 - 2, 0)
	ENDIF
	IF TALENT:Ｖ민감 == 0 && RAND:10 == 0
		TALENT:Ｖ둔감 = 1
		ABL:Ｖ감각 = MAX(ABL:Ｖ감각 - 2, 0)
	ENDIF
	IF TALENT:Ａ민감 == 0 && RAND:10 == 0
		TALENT:Ａ둔감 = 1
		ABL:Ａ감각 = MAX(ABL:Ａ감각 - 2, 0)
	ENDIF
	IF TALENT:Ｂ민감 == 0 && RAND:10 == 0
		TALENT:Ｂ둔감 = 1
		ABL:Ｂ감각 = MAX(ABL:Ｂ감각 - 2, 0)
	ENDIF
	IF TALENT:Ｍ민감 == 0 && RAND:10 == 0
		TALENT:Ｍ둔감 = 1
		ABL:Ｍ감각 = MAX(ABL:Ｍ감각 - 2, 0)
	ENDIF
ENDIF

;能力のMAXを체크する
FOR LOCAL, 0, 20
	ABL:LOCAL = MIN(NUM("최대레벨", LOCAL), ABL:LOCAL)
NEXT

;原作のLVを参考に, 経験を増やす
FOR LOCAL, 0, 100
	SIF LOCAL == 2 || LOCAL == 3 || LOCAL == 50
		CONTINUE

	SIF CFLAG:8
		EXP:LOCAL *= CFLAG:8
	;所持している経験の揺らぎ
	SIF EXP:LOCAL && ARGS != "애널라이즈"
		EXP:LOCAL = MAX(MULTIPLY(EXP:LOCAL, 50 + (RAND:6 + RAND:6)*10), 1)
NEXT
EXP:Ｖ경험 = MAX(EXP:Ｖ경험, EXP:Ｖ성교경험, EXP:Ｖ확장경험)
EXP:Ａ경험 = MAX(EXP:Ａ경험, EXP:Ａ성교경험, EXP:Ａ확장경험)

;처녀？
IF TALENT:처녀
	TALENT:출산경험 = 0
	ABL:Ｖ감각 = 0
	EXP:Ｖ경험 = 0
	EXP:Ｖ성교경험 = 0
	EXP:허리사용경험 = 0
	EXP:Ｖ확장경험 = 0
	BASE:태아의부친 = 0
ELSEIF TALENT:남자 == 0
	SIF EXP:Ｖ경험 == 0
		EXP:Ｖ경험 = 1
ENDIF
;동정？
IF TALENT:동정
	EXP:Ｖ삽입경험 = 0
ELSEIF PENIS(TARGET)
	SIF EXP:Ｖ삽입경험 == 0
		EXP:Ｖ삽입경험 = 1
ENDIF

;확장적정はレア소질
SIF RAND:10 == 0
	TALENT:확장적정 = 1

;확장度の상한に応じて확장度にぶれがある（상한側になりやすい）
IF CONFIG("ＶＡ상세표시：수동ＬＶＵＰ")
	LOCAL = CALCF("Ｖ확장の상한", TARGET)
	SIF LOCAL > ABL:Ｖ확장
		ABL:Ｖ확장 = LIMIT(RAND:LOCAL + RAND:2 + RAND:2, ABL:Ｖ확장, LOCAL)

	LOCAL = CALCF("Ａ확장の상한", TARGET)
	SIF LOCAL > ABL:Ａ확장
		ABL:Ａ확장 = LIMIT(RAND:LOCAL + RAND:2 + RAND:2, ABL:Ａ확장, LOCAL)
ELSE
	ABL:Ｖ확장 = CALCF("Ｖ확장の상한", TARGET)
	ABL:Ａ확장 = CALCF("Ａ확장の상한", TARGET)
ENDIF

;わーい모유が出るぞ！（レア소질）
SIF TALENT:남자 == 0 && TALENT:빈유 == 0 && RAND:10 + (TALENT:거유 + 1)/2 >= 9
	TALENT:모유체질 = 1

;정액에약함のも良いですよね
IF TALENT:정액에약함 == 0 && ABL:정액중독 > RAND:5
	IF EXP:펠라경험 && RANDIF(1, EXP:Ｖ성교경험, EXP:Ａ성교경험)
		TALENT:정액에약함 = 1p0
	ELSEIF EXP:Ｖ성교경험 && RANDIF(1, EXP:Ａ성교경험)
		TALENT:정액에약함 = 1p1
	ELSEIF EXP:Ａ성교경험
		TALENT:정액에약함 = 1p2
	ENDIF
ENDIF

;펠라경험豊富だと구토반사내성が有るかも
SIF EXP:펠라경험 + EXP:목성교경험 >= MAX(50, RAND:300)
	TALENT:구토반사내성 = 1
;인후성감
SIF TALENT:Ｍ민감*3 + ABL:Ｍ감각 + GET_EXP_LV(EXP:펠라경험) + GET_EXP_LV(EXP:목성교경험) >= RAND:20
	TALENT:인후성감 = 1
;자궁성감
SIF TALENT:남자 == 0 && TALENT:Ｖ민감*3 + ABL:Ｖ감각 + GET_EXP_LV(EXP:Ｖ경험) + GET_EXP_LV(EXP:Ｖ성교경험) >= RAND:20
	TALENT:자궁성감 = 1
;요도성감
SIF TALENT:도착적 + RAND:10 >= 9
	TALENT:요도성감 = 1
;냄새민감
IF (TALENT:동물귀 > 0)*3 + (TALENT:악취민감 - TALENT:악취둔감)*2 + TALENT:망신 + (ABL:욕망 + 1)/2 + RAND:10 >= 9
	TALENT:냄새민감 = 1
	TALENT:악취둔감 = 0
ENDIF

