			SIF ARG != ASSI && ASSI > 0
				ARG = ASSI
			RESTART
		CASE "8"
			IF ARG == 1
				ARG = CHARANUM - 1
			ELSE
				ARG -= 1
			ENDIF
			RESTART
		CASE "9"
			IF ARG == CHARANUM - 1
				ARG = 1
			ELSE
				ARG += 1
			ENDIF
			RESTART
		ENDSELECT
	ENDIF
CASE "通常：２"
