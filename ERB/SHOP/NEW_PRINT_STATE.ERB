;[남자]なら9,레즈끼のかわりに13,BLを表示

FOR LCOUNT, 0, 29
	VARSET LOCAL
	VARSET LOCALS
	NOABL = 0

	SELECTCASE LCOUNT
	CASE 0
		LOCAL = 0
	CASE 1
		LOCAL = 1
	CASE 2
		LOCAL = 2
	CASE 3
		LOCAL = 6
	CASE 4
		LOCAL = 7
	CASE 5
