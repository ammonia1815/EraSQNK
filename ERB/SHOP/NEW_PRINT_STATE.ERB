		DRAWLINE
		PRINTBUTTON "[Enter]닫는다", "Enter"
		PRINTFORM  [ 1]page1
		;PRINTPLAINFORM  [×]page2
		PRINTFORM  [ 4]주인
		IF ARG != TARGET && TARGET > 0
			PRINTFORM  [ 5]조교대상
		ELSE
			PRINTPLAINFORM  [×]조교대상
		ENDIF
		IF ARG != ASSI && ASSI > 0
			PRINTFORM  [ 6]%TEXTS("조수の명칭")%
		ELSE
			PRINTPLAINFORM  [×]%TEXTS("조수の명칭")%
		ENDIF
		PRINTFORM  [ 8]\@ ARG == 1 ? 最後 # 前 \@
		PRINTFORML  [ 9]\@ ARG == CHARANUM - 1 ? 最初 # 次 \@

