		ELSE
			PRINTPLAINFORM  [×]%TEXTS("조수の명칭")%
		ENDIF
		PRINTFORM  [ 8]\@ ARG == 1 ? 最後 # 前 \@
		PRINTFORML  [ 9]\@ ARG == CHARANUM - 1 ? 最初 # 次 \@

		ONEINPUTS

		SELECTCASE RESULTS
		CASE "0", "1", "3", "7"
			CLEARLINE LINECOUNT - MEMO_LINECOUNT
			RESTART
		CASE "2"
			ARGS = 通常：２
			RESTART
		CASE "4"
			ARG = MASTER
			RESTART
		CASE "5"
			SIF ARG != TARGET && TARGET > 0
				ARG = TARGET
			RESTART
