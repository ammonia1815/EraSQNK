		ELSEIF TALENT:ARG:Ｍ둔감
			LOCALS = (둔)
		ELSEIF TALENT:ARG:Ｍ민감
			LOCALS = (민)
		ENDIF
	CASE 10
		LOCAL = 10
	CASE 11
		LOCAL = 11
	CASE 12
		LOCAL = 8
	CASE 13
		LOCAL = 15
	CASE 14
		IF TALENT:ARG:남자
			LOCAL = 13
		ELSE
			LOCAL = 9
		ENDIF
	CASE 15
		LOCAL = 30
	CASE 16
		LOCAL = 31
	CASE 17
		LOCAL = 32
	CASE 18
		LOCAL = 33
	CASE 19
		LOCAL = 34
