		;SIF PENIS(ARG)
		;	NOABL = 1
	ENDSELECT

	SELECTCASE ARG
	CASE MASTER
		SELECTCASE LCOUNT
		CASE 2, 12 TO 29
		CASEELSE
			NOABL = 1
		ENDSELECT
	ENDSELECT

	LOCALS = %ABLNAME:LOCAL%%LOCALS%

	SELECTCASE NOABL
	CASE 0
