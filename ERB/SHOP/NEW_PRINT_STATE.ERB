		MOTHERS = \@ MOTHER != MASTER ? [{MOTHER}] # \@%CALLNAME:MOTHER%
	SIF FATHER > 0
		FATHERS = \@ FATHER != MASTER ? [{FATHER}] # \@%CALLNAME:FATHER%

