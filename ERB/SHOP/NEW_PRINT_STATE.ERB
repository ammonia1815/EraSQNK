			NOABL = 1

	;이하は性器詳細表示がＯＮの時のみ
	CASE 20
		;詳細表示ＯＦＦなら이하は表示しない
		SIF CONFIG("성기상세표시") == 0
			BREAK
		LOCAL = 22
		SIF TALENT:ARG:남자 || EXP:ARG:Ｖ성교경험 == 0
			NOABL = 1
	CASE 21
		LOCAL = 23
		SIF TALENT:ARG:남자
